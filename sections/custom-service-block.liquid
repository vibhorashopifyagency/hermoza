                                                --block-heading-size-mb: {{ block.settings.heading_title_mb | append: 'px' }};
                                                --block-heading-color: {{ block.settings.heading_color }};
                                                --block-heading-font_weight: {{ block.settings.heading_font_weight }};
                                                --block-heading-text_transform: {{ block.settings.heading_text_transform }};"
                                                --block-heading-spacing-bottom: {{ block.settings.mg_bottom_desktop | append: 'px' }};
                                                --block-heading-spacing-bottom-tb: {{ block.settings.mg_bottom_tablet | append: 'px' }};
