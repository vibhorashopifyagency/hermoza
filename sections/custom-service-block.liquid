{%- liquid
  assign blog_bg = section.settings.blog_bg
  assign blog_bg_gradient = section.settings.blog_bg_gradient
  assign blog_bg_image = section.settings.blog_bg_image
-%}

<style>
  :root {
    --block_title_align: {{ section.settings.block_title_align }};
    --block_title_align_bm: {{ section.settings.block_title_align_mobile }};
    --block_title_mg_bottom_title: {{ section.settings.mg_bottom_title | append: 'px' }};
    --block_title_mg_bottom_tb_title: {{ section.settings.mg_bottom_title_tb | append: 'px' }};
    --block_title_mg_bottom_mb_title: {{ section.settings.mg_bottom_title_mb | append: 'px' }};

    --block_title_view_all_font_size: {{ section.settings.view_all_font_size | append: 'px' }};
    --block_title_view_all_font_weight: {{ section.settings.button_text_font_weight }};
    --block_title_view_all_text_transform: {{ section.settings.button_text_text_transform }};
    --block_title_view_all_align: {{ section.settings.view_all_align }};
    --block_title_view_all_align_mobile: {{ section.settings.view_all_align_mobile }};
  }
  
  .custom-service-block.custom-service-block-{{section.id}} {
    padding-top: {{ section.settings.mg_top_desktop | append: 'px' }};
    padding-bottom: {{ section.settings.mg_bottom_desktop | append: 'px' }};
    {% if blog_bg_image != blank %}
      background-image: url({{ blog_bg_image | img_url: '1920x' }});
      background-repeat: no-repeat;
      background-size: cover;
      background-position: center;
      background-color: {{ blog_bg }};
    {% else %}
      background: {% if blog_bg_gradient != blank %}{{ blog_bg_gradient }}{% else %}{{ blog_bg }}{% endif %};
    {% endif %}
    position: relative;
  }
  .custom-service-block.custom-service-block-{{section.id}} .container > .row {
        justify-content: center;
  }
  .custom-service-block.custom-service-block-{{section.id}} .section-header .title {
      text-align: var(--block_title_align);
      padding-bottom: var(--block_title_mg_bottom_title);
      animation-delay: {{ section.settings.animation_delay_title }};
      {% if section.settings.button_color != blank %}
          color: {{ section.settings.button_color }};
      {% else %}
          color: var(--heading-text-color);
      {% endif %}
      font-weight: {{ section.settings.block_title_font_weight }};
      font-size: {{ section.settings.title_font_size | append: 'px' }};
  }
  .custom-service-block.custom-service-block-{{section.id}} .view-all-button {
      animation-delay: {{ section.settings.animation_delay_button }};
  }
  .custom-service-block.custom-service-block-{{section.id}} .section-header .view_all {
      font-size: var(--block_title_view_all_font_size);
      font-weight: var(--block_title_view_all_font_weight);
      text-transform: var(--block_title_view_all_text_transform);
      text-align: var(--block_title_view_all_align);
  }
  .custom-service-block.custom-service-block-{{section.id}} .section-header .view_all .icon-svg {
      width: 19px;
      display: inline-block;
      margin-left: 5px;
  }
  .custom-service-block.custom-service-block-{{section.id}} .section-header .view_all .icon-svg path {
      fill: var(--link-color);
      transition: all 400ms ease;
  }
  .custom-service-block.custom-service-block-{{section.id}} .section-header .view_all:hover .icon-svg path {
      fill: var(--link-hover-color);
  }
  .custom-service-block.custom-service-block-{{section.id}} .service-item-1 .custom-service-item {
      width: 100%;
  }
  .custom-service-block.custom-service-block-{{section.id}} .service-item-2 .custom-service-item {
      width: 50%;
  }
  .custom-service-block.custom-service-block-{{section.id}} .service-item-3 .custom-service-item {
      width: 33.33%;
  }
  .custom-service-block.custom-service-block-{{section.id}} .service-item-4 .custom-service-item {
      width: 25%;
  }
  .custom-service-block.custom-service-block-{{section.id}} .service-item-5 .custom-service-item {
      width: 20%;
  }
  .custom-service-block-{{section.id}} .custom-service-item .policies-icon {
      padding-bottom: 25px;
      position: relative;
  }
  .custom-service-block-{{section.id}} .custom-service-item .policies-icon svg {
      fill: var(--block-color-icon);
      stroke: var(--block-color-icon);
      width: var(--block-width-icon);
      height: var(--block-height-icon);
      display: block;
      margin: 0 auto;
  }
  .custom-service-block-{{section.id}} .custom-service-item .policies-icon svg path {
      fill: var(--block-color-icon);
      stroke: var(--block-color-icon);
  }
  .custom-service-content a:hover.block-title {
      color: var(--link-hover-color);
  }
  .custom-service-block-{{section.id}} .custom-service-content .block-title {
      font-weight: var(--block-heading-font_weight);
      font-size: var(--block-heading-size);
      text-transform: var(--block-heading-text_transform);
      margin-bottom: var(--block-heading-spacing-bottom);
      display: inline-block;
  }
  .custom-service-block-{{section.id}} .custom-service-content .block-description {
      font-size: var(--block-description-size);
      font-weight: var(--block-description-font-weight);
      margin-left: auto;
      margin-right: auto;
  }
  .custom-service-block-{{section.id}} .custom-service-item .policies-icon img {
      display: block;
      transition: all 400ms ease;
  }
  .custom-service-block-{{section.id}} .custom-service-item .custom-service-box.text-center .policies-icon img {
      margin-left: auto;
      margin-right: auto;
  }
  .custom-service-block-{{section.id}} .custom-service-item .custom-service-box.text-right .policies-icon img {
      margin-left: auto;
  }
  .custom-service-block-{{section.id}} .custom-service-item .policies-icon a {
      overflow: hidden;
      display: block;
  }
  {% if section.settings.image_zoom != blank %}
    .custom-service-block-{{section.id}} .custom-service-item .policies-icon img:hover {
        transform: scale(1.1);
    }
  {% endif %}
  .custom-service-block-{{section.id}} .custom-service-item {
      padding-bottom: 40px;
  }
  .custom-service-block-{{section.id}} .custom-service-item .policies-icon .position-r {
      position: relative;
      overflow: hidden;
  }
  .custom-service-block-{{section.id}} .policies-icon-overlay {
      position: absolute;
      top: 0;
      left: 0;
      width: 100%;
      height: 100%;
      background: rgba(0, 0, 0, 0.2);
      opacity: 0;
      visibility: hidden;
      transition: all 400ms ease;
  }
  .custom-service-block-{{section.id}} .policies-icon-overlay ul {
      width: fit-content;
      margin-left: auto;
      padding-top: 30px;
      padding-right: 30px;
      transition: all 400ms ease;
      position: relative;
      right: -70px;
  }
  .custom-service-block-{{section.id}} .policies-icon-overlay ul li {
      padding: 10px 0px;
  }
  .custom-service-block-{{section.id}} .policies-icon-overlay ul li:first-child {
      padding-top: 0;
  }
  .custom-service-block-{{section.id}} .policies-icon-overlay ul li:last-child {
      padding-bottom: 0;
  }
  .custom-service-block-{{section.id}} .policies-icon-overlay ul li a svg {
      width: unset !important;
      height: 18px !important;
      fill: #fff !important;
      transition: all 400ms ease;
  }
  .custom-service-block-{{section.id}} .custom-service-box:hover .policies-icon-overlay {
      opacity: 1;
      visibility: visible;
  }
  .custom-service-block-{{section.id}} .custom-service-box:hover .policies-icon-overlay ul {
      right: 0;
  }
  .custom-service-block-{{section.id}} .policies-icon-overlay ul li a:hover svg {
      fill: var(--link-hover-color) !important;
  }

  .custom-service-block-{{section.id}} .icon-align-left .custom-service-box {
      display: flex;
      align-items: center;
  }
  .custom-service-block-{{section.id}} .icon-align-left .custom-service-box .policies-icon {
      padding-bottom: 0;
      padding-right: 18px;
  }
  body.layout_rtl .custom-service-block-{{section.id}} .icon-align-left .custom-service-box .policies-icon {
      padding-right: 0;
      padding-left: 18px;
  }
  body.layout_rtl .custom-service-block-{{section.id}} .icon-align-left .custom-service-box .custom-service-content {
      text-align: right;
  }
  body.layout_rtl .custom-service-block-{{section.id}} .icon-align-right .custom-service-box .policies-icon {
      margin-left: 0;
      margin-right: 18px;
  }
  body.layout_rtl .custom-service-block-{{section.id}} .icon-align-right .custom-service-box .custom-service-content {
      text-align: right;
  }
  .custom-service-block-{{section.id}} .icon-align-left .custom-service-box .custom-service-content {
      text-align: left;
  }
  .custom-service-block-{{section.id}} .icon-align-right .custom-service-box {
      display: flex;
      align-items: center;
  }
  .custom-service-block-{{section.id}} .icon-align-right .custom-service-box .policies-icon {
      padding-bottom: 0;
      order: 2;
      margin-left: 18px;
  }
  .custom-service-block-{{section.id}} .icon-align-right .custom-service-box .custom-service-content {
      text-align: left;
      order: 1;
  }
  .custom-service-block.custom-service-block-{{section.id}} .description-text{
     font-size:{{ section.settings.	description__font__size | append: 'px' }};
    {% if section.settings.description__color != blank %}
         color: {{ section.settings.description__color }};
    {% else %}
         color: var(--body-text-color);
    {% endif %}
     
     font-weight: {{ section.settings.description__font__weight }};
     max-width: {{ section.settings.content__width | append: 'px' }};
     padding-bottom: {{ section.settings.desc_space_bottom | append: 'px' }};
     margin : 0 auto;
  }

  .custom-service-block.custom-service-block-{{section.id}} .section-header .banner-sub-title {
      font-size: {{ section.settings.font_size_sub_title | append: 'px' }};
      line-height: {{ section.settings.sub_title_line_height | append: 'px' }};
      {% if section.settings.sub_title_color != blank %}
         color: {{ section.settings.sub_title_color }};
      {% else %}
          color: var(--heading-text-color);
      {% endif %}
      text-transform: {{ section.settings.product_title_transform }};
      font-weight: {{ section.settings.sub_title_font_weight }};
      padding-bottom: {{ section.settings.sub_title__space_top | append: 'px' }};
      letter-spacing: {{ section.settings.font_size_sub_letter_spacing | append: 'px' }};
      font-family: var(--tab-title-font-type-cat-slide);
      margin-bottom: 0;
    }
  .custom-service-block-{{section.id}} .vector_1 {
      position: absolute;
      {% if section.settings.vector_top != blank %}
        top: {{ section.settings.vector_top }};
      {% endif %}
      {% if section.settings.vector_bottom != blank %}
        bottom: {{ section.settings.vector_bottom }};
      {% endif %}
      {% if section.settings.vector_left != blank %}
        left: {{ section.settings.vector_left }};
      {% endif %}
      {% if section.settings.vector_right != blank %}
        right: {{ section.settings.vector_right }};
      {% endif %}
  }
  {%- if section.settings.image_max_width != blank -%}
    .custom-service-block-{{section.id}} .vector_1 img {
        max-width: {{ section.settings.image_max_width }};
    }
  {% endif %}
  .custom-service-block-{{section.id}} .vector_2 {
      position: absolute;
      {% if section.settings.vector_top_2 != blank %}
        top: {{ section.settings.vector_top_2 }};
      {% endif %}
      {% if section.settings.vector_bottom_2 != blank %}
        bottom: {{ section.settings.vector_bottom_2 }};
      {% endif %}
      {% if section.settings.vector_left_2 != blank %}
        left: {{ section.settings.vector_left_2 }};
      {% endif %}
      {% if section.settings.vector_right_2 != blank %}
        right: {{ section.settings.vector_right_2 }};
      {% endif %}
  }
  {%- if section.settings.image_max_width_2 != blank -%}
    .custom-service-block-{{section.id}} .vector_2 img {
        max-width: {{ section.settings.image_max_width_2 }};
    }
  {% endif %}
  {% if section.settings.container_position != blank %}
    .custom-service-block-{{section.id}} .container {
      position:relative;
    }
  {% endif %}


  @media (min-width: 1200px) and (max-width: 1599px) { 
    .custom-service-block.custom-service-block-{{section.id}} .service-item-5 .custom-service-item {
        width: 33.33%;
    }
  }

  @media (min-width: 1025px) and (max-width: 1199px) { 
    .custom-service-block.custom-service-block-{{section.id}} .service-item-5 .custom-service-item {
        width: 33.33%;
    }
  }

  @media (max-width: 1199px) {
    .custom-service-block.custom-service-block-{{section.id}} {
      padding-top: {{ section.settings.mg_top_tablet | append: 'px' }};
      padding-bottom: {{ section.settings.mg_bottom_tablet | append: 'px' }};
    }
  }

  @media (max-width: 1024px) {
    .custom-service-block-{{section.id}} .custom-service-content .block-title {
        font-size: var(--block-heading-size-tb);
    }
    .custom-service-block-{{section.id}} .custom-service-content .block-description {
        font-size: var(--block-description-size-tb);
    }
    .custom-service-block.custom-service-block-{{section.id}} .description-text{
        font-size:{{ section.settings.description__font__tb | append: 'px' }};
        padding-bottom: {{ section.settings.desc_space_bottom_tb | append: 'px' }};
    }
    .custom-service-block.custom-service-block-{{section.id}} .service-item-tb-1 .custom-service-item {
        width: 100%;
    }
    .custom-service-block.custom-service-block-{{section.id}} .service-item-tb-2 .custom-service-item {
        width: 50%;
    }
    .custom-service-block.custom-service-block-{{section.id}} .service-item-tb-3 .custom-service-item {
        width: 33.33%;
    }
    .custom-service-block.custom-service-block-{{section.id}} .service-item-tb-4 .custom-service-item {
        width: 25%;
    }
    .custom-service-block.custom-service-block-{{section.id}} .service-item-tb-5 .custom-service-item {
        width: 20%;
    }
    .custom-service-block-{{section.id}} .custom-service-item .policies-icon {
        padding-bottom: 15px;
    }
    .custom-service-block-{{section.id}} .custom-service-item {
        padding-bottom: 30px;
    }
    .custom-service-block.custom-service-block-{{section.id}} .section-header .title {
        padding-bottom: var(--block_title_mg_bottom_tb_title);
        font-size:{{ section.settings.title_font_size_tb | append: 'px' }};
    }
    .custom-service-block-{{section.id}} .custom-service-content .block-title {
        margin-bottom: var(--block-heading-spacing-bottom-tb);
    }
    .custom-service-block.custom-service-block-{{section.id}} .section-header .banner-sub-title {
      font-size: {{ section.settings.font_size_sub_title_tb | append: 'px' }};
        line-height: {{ section.settings.sub_title_line_height_tb | append: 'px' }};
        padding-bottom: {{ section.settings.sub_title__space_top_tb | append: 'px' }};
    }

    .custom-service-block-{{section.id}} .vector_1 {
        {% if section.settings.vector_tablet_top != blank %}
          top: {{ section.settings.vector_tablet_top }};
        {% endif %}
        {% if section.settings.vector_tablet_bottom != blank %}
          bottom: {{ section.settings.vector_tablet_bottom }};
        {% endif %}
        {% if section.settings.vector_tablet_left != blank %}
          left: {{ section.settings.vector_tablet_left }};
        {% endif %}
        {% if section.settings.vector_tablet_right != blank %}
          right: {{ section.settings.vector_tablet_right }};
        {% endif %}
    }
    .custom-service-block-{{section.id}} .vector_2 {
        {% if section.settings.vector_tablet_top_2 != blank %}
          top: {{ section.settings.vector_tablet_top_2 }};
        {% endif %}
        {% if section.settings.vector_tablet_bottom_2 != blank %}
          bottom: {{ section.settings.vector_tablet_bottom_2 }};
        {% endif %}
        {% if section.settings.vector_tablet_left_2 != blank %}
          left: {{ section.settings.vector_tablet_left_2 }};
        {% endif %}
        {% if section.settings.vector_tablet_right_2 != blank %}
          right: {{ section.settings.vector_tablet_right_2 }};
        {% endif %}
    }
    {%- if section.settings.image_max_width_tb != blank -%}
      .custom-service-block-{{section.id}} .vector_1 img {
        max-width: {{ section.settings.image_max_width_tb }};
      }
    {% endif %}
    {%- if section.settings.image_max_width_tb_2 != blank -%}
      .custom-service-block-{{section.id}} .vector_2 img {
        max-width: {{ section.settings.image_max_width_tb_2 }};
      }
    {% endif %}
  }
    
  @media (max-width: 767px) {
    .custom-service-block.custom-service-block-{{section.id}} {
      padding-top: {{ section.settings.mg_top_mobile | append: 'px' }};
      padding-bottom: {{ section.settings.mg_bottom_mobile | append: 'px' }};
    }
    .custom-service-block.custom-service-block-{{section.id}} .section-header .title {
        text-align: var(--block_title_align_bm);
        padding-bottom: var(--block_title_mg_bottom_mb_title);
        font-size:{{ section.settings.title_font_size_mb | append: 'px' }};
    }
    .custom-service-block.custom-service-block-{{section.id}} .description-text{
        font-size:{{ section.settings.description__font__mb | append: 'px' }};
        padding-bottom: {{ section.settings.desc_space_bottom_mb | append: 'px' }};
    }
    .custom-service-block.custom-service-block-{{section.id}} .section-header .view_all {
        text-align: var(--block_title_view_all_align_mobile);
    }
    .custom-service-block-{{section.id}}.custom-service-content .block-title {
        font-size: var(--block-heading-size-mb);
    }
    .custom-service-block-{{section.id}} .custom-service-content .block-description {
        font-size: var(--block-description-size-mb);
    }
    .custom-service-block.custom-service-block-{{section.id}} .service-item-mb-1 .custom-service-item {
        width: 100%;
    }
    .custom-service-block.custom-service-block-{{section.id}} .service-item-mb-2 .custom-service-item {
        width: 50%;
    }
    .custom-service-block.custom-service-block-{{section.id}} .service-item-mb-3 .custom-service-item {
        width: 33.33%;
    }
    .custom-service-block.custom-service-block-{{section.id}} .service-item-mb-4 .custom-service-item {
        width: 25%;
    }
    .custom-service-block.custom-service-block-{{section.id}} .service-item-mb-5 .custom-service-item {
        width: 20%;
    }
    .custom-service-block-{{section.id}} .custom-service-item .policies-icon {
        padding-bottom: 15px;
    }
    .custom-service-block-{{section.id}} .custom-service-content .block-title {
        margin-bottom: var(--block-heading-spacing-bottom-mb);
        font-size: var(--block-heading-size-mb);
    }
    .custom-service-block-{{section.id}} .icon-align-left .custom-service-box .policies-icon img {
        max-width: 85%;
    }
    .custom-service-block.custom-service-block-{{section.id}} .section-header .banner-sub-title {
      font-size: {{ section.settings.font_size_sub_title_mb | append: 'px' }};
        line-height: {{ section.settings.sub_title_line_height_mb | append: 'px' }};
        padding-bottom: {{ section.settings.sub_title__space_top_mb | append: 'px' }}
    }

    .custom-service-block-{{section.id}} .vector_1 {
        {% if section.settings.vector_mobile_top != blank %}
          top: {{ section.settings.vector_mobile_top }};
        {% endif %}
        {% if section.settings.vector_mobile_bottom != blank %}
          bottom: {{ section.settings.vector_mobile_bottom }};
        {% endif %}
        {% if section.settings.vector_mobile_left != blank %}
          left: {{ section.settings.vector_mobile_left }};
        {% endif %}
        {% if section.settings.vector_mobile_right != blank %}
          right: {{ section.settings.vector_mobile_right }};
        {% endif %}
    }
    .custom-service-block-{{section.id}} .vector_2 {
        {% if section.settings.vector_mobile_top_2 != blank %}
          top: {{ section.settings.vector_mobile_top_2 }};
        {% endif %}
        {% if section.settings.vector_mobile_bottom_2 != blank %}
          bottom: {{ section.settings.vector_mobile_bottom_2 }};
        {% endif %}
        {% if section.settings.vector_mobile_left_2 != blank %}
          left: {{ section.settings.vector_mobile_left_2 }};
        {% endif %}
        {% if section.settings.vector_mobile_right_2 != blank %}
          right: {{ section.settings.vector_mobile_right_2 }};
        {% endif %}
    }
    {%- if section.settings.image_max_width_mb != blank -%}
      .custom-service-block-{{section.id}} .vector_1 img {
        max-width: {{ section.settings.image_max_width_mb }};
      }
    {% endif %}
    {%- if section.settings.image_max_width_mb_2 != blank -%}
      .custom-service-block-{{section.id}} .vector_2 img {
        max-width: {{ section.settings.image_max_width_mb_2 }};
      }
    {% endif %}
  }
</style>

<div class="custom-service-block custom-service-block-{{section.id}}">
  <div class="{% if section.settings.container == 'fullwidth' %}container-full{% else %}container{% endif %}">
    {%- if section.settings.vector_image_1 != blank -%}
      <div class="vector_1 vector_1_{{section.id}}">
        {%- liquid
          assign vector_img_1 = section.settings.vector_image_1
        -%}
        <div class="vector_img_1-an {% if section.settings.vectore_1_animations_type != 'none'%}wow{% endif %} {{ section.settings.vectore_1_animations_type }} delay-{{ section.settings.vectore_1_animation_delay | replace: ".", "_" }}">
          <img {% render 'lazyload', preview_img: vector_img_1 %} loading="lazy"/>
          {% if settings.enable_lazyload %}<span class="data-lazy-loading"></span>{% endif %}
        </div>
        <style>
          .image-content-block-{{section.id}} .vector_img_1-an.delay-{{ section.settings.vectore_1_animation_delay | replace: ".", "_" }} {
              animation-delay: {{ section.settings.vectore_1_animation_delay }}
          }
        </style>
      </div>
    {%- endif -%}
    {%- if section.settings.vector_image_2 != blank -%}
      <div class="vector_2 vector_2_{{section.id}}">
        {%- liquid
          assign vector_img_2 = section.settings.vector_image_2
        -%}
        <div class="vector_img_2-an {% if section.settings.vectore_2_animations_type != 'none'%}wow{% endif %} {{ section.settings.vectore_2_animations_type }} delay-{{ section.settings.vectore_2_animation_delay | replace: ".", "_" }}">
          <img {% render 'lazyload', preview_img: vector_img_2 %} loading="lazy"/>
          {% if settings.enable_lazyload %}<span class="data-lazy-loading"></span>{% endif %}  
        </div>
        <style>
          .image-content-block-{{section.id}} .vector_img_2-an.delay-{{ section.settings.vectore_2_animation_delay | replace: ".", "_" }} {
              animation-delay: {{ section.settings.vectore_2_animation_delay }}
          }
        </style>
      </div>
    {%- endif -%}
    
    {%- if section.settings.block_title_title != blank -%}
        <div class="section-header">
          <div class="row align-item-center">
            <div class="{% if section.settings.view_all != blank %}col-xl-9 col-lg-9 col-md-9{% else %}col-xl-12 col-lg-12 col-md-12{% endif %}">
