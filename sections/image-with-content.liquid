{%- liquid
  assign container = section.settings.container
  assign background_color = section.settings.background_color
  assign background_gradient = section.settings.background_gradient
-%}

<style>
  {% if settings.type_font_1 == 'shopify_fonts' and settings.type_font_2 == 'shopify_fonts' %}
      {% assign base_font = settings.type_font_1_font | font_modify: 'weight', '400' %}
      {% assign base_font_medium = settings.type_font_1_font | font_modify: 'weight', '500' %}
      {% assign base_font_sem_bold = settings.type_font_1_font | font_modify: 'weight', '600' %}
      {% assign base_font_bold = settings.type_font_1_font | font_modify: 'weight', 'bold' %}

      {{ base_font | font_face | replace: '@font-face {', '@font-face { font-display: swap;' }}
      {{ base_font_medium | font_face | replace: '@font-face {', '@font-face { font-display: swap;' }}
      {{ base_font_sem_bold | font_face | replace: '@font-face {', '@font-face { font-display: swap;' }}
      {{ base_font_bold | font_face | replace: '@font-face {', '@font-face { font-display: swap;' }}

      {% assign fonts_name = base_font.family | append: ',' | append: base_font.fallback_families %}

      {% if settings.type_font_2_font.family != settings.type_font_1_font.family %}
          {% assign header_font = settings.type_font_2_font | font_modify: 'weight', '400' %}
          {% assign header_font_bold = settings.type_font_2_font | font_modify: 'weight', 'bold' %}
          {% assign header_font_sem_bold = settings.type_font_2_font | font_modify: 'weight', '600' %}
          {% assign header_font_medium = settings.type_font_2_font | font_modify: 'weight', '500' %}
          
          {{ header_font | font_face | replace: '@font-face {', '@font-face { font-display: swap;' }}
          {{ header_font_medium | font_face | replace: '@font-face {', '@font-face { font-display: swap;' }}
          {{ header_font_sem_bold | font_face | replace: '@font-face {', '@font-face { font-display: swap;' }}
          {{ header_font_bold | font_face | replace: '@font-face {', '@font-face { font-display: swap;' }}

                {% assign fonts_name_2 = header_font.family | append: ',' | append: header_font.fallback_families %}
      {% else %}
                {% assign fonts_name_2 = base_font.family | append: ',' | append: base_font.fallback_families %}
      {% endif %}
  {% elsif settings.type_font_1 != 'shopify_fonts' and settings.type_font_2 != 'shopify_fonts' %}
        {%- assign font_1 = settings.type_font_1_name -%}
        {%- capture font_name_1 -%}
            {{font_1 | strip | replace: ' ', '+'}}:300,300i,400,400i,500,500i,600,600i,700,700i,800,800i
        {%- endcapture -%}
        @import url('https://fonts.googleapis.com/css?family={{font_name_1}}&display=swap');
              {% assign fonts_name = font_1 %}

        {% if settings.type_font_1_name != settings.type_font_2_name %}
            {%- assign font_2 = settings.type_font_2_name -%}
            {%- capture fonts_name_2 -%}
                {{font_2 | strip | replace: ' ', '+'}}:300,300i,400,400i,500,500i,600,600i,700,700i,800,800i
            {%- endcapture -%}
            @import url('https://fonts.googleapis.com/css?family={{fonts_name_2}}&display=swap');
                  {% assign fonts_name_2 = font_2 %}
        {% else %}
                  {% assign fonts_name_2 = font_1 %}
        {% endif %}
  {% elsif settings.type_font_1 == 'shopify_fonts' %}
        {% assign base_font = settings.type_font_1_font | font_modify: 'weight', '400' %}
        {% assign base_font_medium = settings.type_font_1_font | font_modify: 'weight', '500' %}
        {% assign base_font_sem_bold = settings.type_font_1_font | font_modify: 'weight', '600' %}
        {% assign base_font_bold = settings.type_font_1_font | font_modify: 'weight', 'bold' %}

        {{ base_font | font_face | replace: '@font-face {', '@font-face { font-display: swap;' }}
        {{ base_font_medium | font_face | replace: '@font-face {', '@font-face { font-display: swap;' }}
        {{ base_font_sem_bold | font_face | replace: '@font-face {', '@font-face { font-display: swap;' }}
        {{ base_font_bold | font_face | replace: '@font-face {', '@font-face { font-display: swap;' }}

        {% assign fonts_name = base_font.family | append: ',' | append: base_font.fallback_families %}
      
        {% if base_font.family != settings.type_font_2_name %}
            {%- assign font_2 = settings.type_font_2_name -%}
            {%- capture fonts_name_2 -%}
                {{font_2 | strip | replace: ' ', '+'}}:300,300i,400,400i,500,500i,600,600i,700,700i,800,800i
            {%- endcapture -%}
            @import url('https://fonts.googleapis.com/css?family={{fonts_name_2}}&display=swap');
                    {% assign fonts_name_2 = font_2 %}
        {% else %}
                  {% assign fonts_name_2 = base_font.family | append: ',' | append: base_font.fallback_families %}
        {% endif %}
  {% else %}
        {% assign header_font = settings.type_font_2_font | font_modify: 'weight', '400' %}
        {% assign header_font_bold = settings.type_font_2_font | font_modify: 'weight', 'bold' %}
        {% assign header_font_sem_bold = settings.type_font_2_font | font_modify: 'weight', '600' %}
        {% assign header_font_medium = settings.type_font_2_font | font_modify: 'weight', '500' %}

        {% if header_font.family != settings.type_font_1_name %}
            {%- assign font_1 = settings.type_font_1_name -%}
            {%- capture font_name_1 -%}
                {{font_1 | strip | replace: ' ', '+'}}:300,300i,400,400i,500,500i,600,600i,700,700i,800,800i
            {%- endcapture -%}
            @import url('https://fonts.googleapis.com/css?family={{font_name_1}}&display=swap');
                  {% assign fonts_name = font_1 %}
        {% else %}
                  {% assign fonts_name = header_font.family | append: ',' | append: header_font.fallback_families %}
        {% endif %}

        {{ header_font | font_face | replace: '@font-face {', '@font-face { font-display: swap;' }}
        {{ header_font_medium | font_face | replace: '@font-face {', '@font-face { font-display: swap;' }}
        {{ header_font_sem_bold | font_face | replace: '@font-face {', '@font-face { font-display: swap;' }}
        {{ header_font_bold | font_face | replace: '@font-face {', '@font-face { font-display: swap;' }}
             
        {% assign fonts_name_2 = header_font.family | append: ',' | append: header_font.fallback_families %}
  {% endif %}
  
  .image-with-content.image-content-block-{{section.id}} {
    background: {% if background_gradient != blank %}{{ background_gradient }}{% else %}{{ background_color }}{% endif %};
  }
  .image-content-block-animation-{{section.id}} {
    background: {% if section.settings.clip_background_gradient != blank %}{{ section.settings.clip_background_gradient }}{% else %}{{ section.settings.clip_background_color }}{% endif %};
    padding-top: {{ section.settings.mg_top_desktop | append: 'px' }};
    padding-bottom: {{ section.settings.mg_bottom_desktop | append: 'px' }};
    position:relative;
  }
  {% if section.settings.container_position != blank %}
    .image-with-content.image-content-block-{{section.id}} .container {
      position:relative;
    }
  {% endif %}
  .image-with-content.image-content-block-{{section.id}} .image {
      display: block;
      height: 100%;
      width: 100%;
  }
  .image-with-content.image-content-block-{{section.id}} .image img {
      display: block;
      width: 100%;
      height: 100%;
      object-fit: cover;
  }
  .image-with-content.image-content-block-{{section.id}} .row > * {
      display: flex;
      flex-wrap: wrap;
  }
  body.video-show .scoder-video-popup {
      opacity: 1;
      visibility: visible;
      pointer-events: auto;
      transform: translate3d(-50%,-50%,0);
  }
  .image-with-content.image-content-block-{{section.id}} .animate-ambient-image {
    width: 100%;
  }
  .image-with-content.image-content-block-{{section.id}} .img-image img {
      object-fit: cover;
      height: 100%;
      width: 100%;
      display: block;
  }
  .image-with-content.image-content-block-{{section.id}} img.js-cloud-1{
       width:151px;
  } 
  .image-with-content.image-content-block-{{section.id}} .image-animation{
      position:relative;
      z-index:5;
  }
  .image-content-block-{{section.id}} .vector_1 {
      position: absolute;
      {% if section.settings.vector_top != blank %}
        top: {{ section.settings.vector_top }};
      {% endif %}
      {% if section.settings.vector_bottom != blank %}
        bottom: {{ section.settings.vector_bottom }};
      {% endif %}
      {% if section.settings.vector_left != blank %}
        left: {{ section.settings.vector_left }};
      {% endif %}
      {% if section.settings.vector_right != blank %}
        right: {{ section.settings.vector_right }};
      {% endif %}
  }
  {%- if section.settings.image_max_width != blank -%}
    .image-content-block-{{section.id}} .vector_1 img {
        max-width: {{ section.settings.image_max_width }};
    }
  {% endif %}
  .image-content-block-{{section.id}} .vector_2 {
      position: absolute;
      {% if section.settings.vector_top_2 != blank %}
        top: {{ section.settings.vector_top_2 }};
      {% endif %}
      {% if section.settings.vector_bottom_2 != blank %}
        bottom: {{ section.settings.vector_bottom_2 }};
      {% endif %}
      {% if section.settings.vector_left_2 != blank %}
        left: {{ section.settings.vector_left_2 }};
      {% endif %}
      {% if section.settings.vector_right_2 != blank %}
        right: {{ section.settings.vector_right_2 }};
      {% endif %}
  }
  {%- if section.settings.image_max_width_2 != blank -%}
    .image-content-block-{{section.id}} .vector_2 img {
        max-width: {{ section.settings.image_max_width_2 }};
    }
  {% endif %}
  

  @media (max-width: 1199px) {
    .image-content-block-animation-{{section.id}} {
      padding-top: {{ section.settings.mg_top_tablet | append: 'px' }};
      padding-bottom: {{ section.settings.mg_bottom_tablet | append: 'px' }};
    }  
  }

  @media (max-width: 1024px) {
    .image-content-block-{{section.id}} .vector_1 {
        {% if section.settings.vector_tablet_top != blank %}
          top: {{ section.settings.vector_tablet_top }};
        {% endif %}
        {% if section.settings.vector_tablet_bottom != blank %}
          bottom: {{ section.settings.vector_tablet_bottom }};
        {% endif %}
        {% if section.settings.vector_tablet_left != blank %}
          left: {{ section.settings.vector_tablet_left }};
        {% endif %}
        {% if section.settings.vector_tablet_right != blank %}
          right: {{ section.settings.vector_tablet_right }};
        {% endif %}
    }
    .image-content-block-{{section.id}} .vector_2 {
        {% if section.settings.vector_tablet_top_2 != blank %}
          top: {{ section.settings.vector_tablet_top_2 }};
        {% endif %}
        {% if section.settings.vector_tablet_bottom_2 != blank %}
          bottom: {{ section.settings.vector_tablet_bottom_2 }};
        {% endif %}
        {% if section.settings.vector_tablet_left_2 != blank %}
          left: {{ section.settings.vector_tablet_left_2 }};
        {% endif %}
        {% if section.settings.vector_tablet_right_2 != blank %}
          right: {{ section.settings.vector_tablet_right_2 }};
        {% endif %}
    }
    {%- if section.settings.image_max_width_tb != blank -%}
      .image-content-block-{{section.id}} .vector_1 img {
        max-width: {{ section.settings.image_max_width_tb }};
      }
    {% endif %}
    {%- if section.settings.image_max_width_tb_2 != blank -%}
      .image-content-block-{{section.id}} .vector_2 img {
        max-width: {{ section.settings.image_max_width_tb_2 }};
      }
    {% endif %}
  }

  @media (max-width: 767px) {
    .image-content-block-animation-{{section.id}} {
      padding-top: {{ section.settings.mg_top_mobile | append: 'px' }};
      padding-bottom: {{ section.settings.mg_bottom_mobile | append: 'px' }};
    }
    .image-content-block-{{section.id}} .vector_1 {
        {% if section.settings.vector_mobile_top != blank %}
          top: {{ section.settings.vector_mobile_top }};
        {% endif %}
        {% if section.settings.vector_mobile_bottom != blank %}
          bottom: {{ section.settings.vector_mobile_bottom }};
        {% endif %}
        {% if section.settings.vector_mobile_left != blank %}
          left: {{ section.settings.vector_mobile_left }};
        {% endif %}
        {% if section.settings.vector_mobile_right != blank %}
          right: {{ section.settings.vector_mobile_right }};
        {% endif %}
    }
    .image-content-block-{{section.id}} .vector_2 {
        {% if section.settings.vector_mobile_top_2 != blank %}
          top: {{ section.settings.vector_mobile_top_2 }};
        {% endif %}
        {% if section.settings.vector_mobile_bottom_2 != blank %}
          bottom: {{ section.settings.vector_mobile_bottom_2 }};
        {% endif %}
        {% if section.settings.vector_mobile_left_2 != blank %}
          left: {{ section.settings.vector_mobile_left_2 }};
        {% endif %}
        {% if section.settings.vector_mobile_right_2 != blank %}
          right: {{ section.settings.vector_mobile_right_2 }};
        {% endif %}
    }
    {%- if section.settings.image_max_width_mb != blank -%}
      .image-content-block-{{section.id}} .vector_1 img {
        max-width: {{ section.settings.image_max_width_mb }};
      }
    {% endif %}
    {%- if section.settings.image_max_width_mb_2 != blank -%}
      .image-content-block-{{section.id}} .vector_2 img {
        max-width: {{ section.settings.image_max_width_mb_2 }};
      }
    {% endif %}
  }
</style>

<div class="image-with-content image-content-block-{{section.id}}">
  <div class="image-content-block-animation-{{section.id}}">
    <div class="{% if container == 'fullwidth'%}container-full{% else %}container{% endif %}">
      {%- if section.settings.vector_image_1 != blank -%}
      <div class="vector_1 vector_1_{{section.id}}">
        {%- liquid
          assign vector_img_1 = section.settings.vector_image_1
        -%}
        <div class="vector_img_1-an {% if section.settings.vectore_1_animations_type != 'none'%}wow{% endif %} {{ section.settings.vectore_1_animations_type }} delay-{{ section.settings.vectore_1_animation_delay | replace: ".", "_" }}">
          <img {% render 'lazyload', preview_img: vector_img_1 %} loading="lazy"/>
          {% if settings.enable_lazyload %}<span class="data-lazy-loading"></span>{% endif %}
        </div>
        <style>
          .image-content-block-{{section.id}} .vector_img_1-an.delay-{{ section.settings.vectore_1_animation_delay | replace: ".", "_" }} {
              animation-delay: {{ section.settings.vectore_1_animation_delay }}
          }
        </style>
      </div>
    {%- endif -%}
  
    {%- if section.settings.vector_image_2 != blank -%}
      <div class="vector_2 vector_2_{{section.id}}">
        {%- liquid
          assign vector_img_2 = section.settings.vector_image_2
        -%}
        <div class="vector_img_2-an {% if section.settings.vectore_2_animations_type != 'none'%}wow{% endif %} {{ section.settings.vectore_2_animations_type }} delay-{{ section.settings.vectore_2_animation_delay | replace: ".", "_" }}">
          <img {% render 'lazyload', preview_img: vector_img_2 %} loading="lazy"/>
          {% if settings.enable_lazyload %}<span class="data-lazy-loading"></span>{% endif %}  
        </div>
        <style>
          .image-content-block-{{section.id}} .vector_img_2-an.delay-{{ section.settings.vectore_2_animation_delay | replace: ".", "_" }} {
              animation-delay: {{ section.settings.vectore_2_animation_delay }}
          }
        </style>
      </div>
    {%- endif -%}
      {%- for block in section.blocks -%}
        {%- liquid
          assign preview_img = block.settings.image
          assign preview_img_1 = block.settings.image_left
          assign preview_img_2 = block.settings.image_center
          assign preview_img_3 = block.settings.image_right
           
        -%}
        <style>
          :root {
            {% if block.settings.heading_fontfamily == 'font_1' %}
                --image_with-content-sub-heading-font-family: {{fonts_name}};
            {% else %}
                --image_with-content-sub-heading-font-family: {{fonts_name_2}};
            {% endif %}
          }
          .image-with-content.image-content-block-{{section.id}} .img-image img {
            max-width: {{ block.settings.image_max_width }};
            margin-left: auto;
            margin-right: auto;
          }
          .image-with-content.image-content-block-{{section.id}} .img-image .image-align-left img{
            margin-left: 0;
            margin-right: auto;
          }
          .image-with-content.image-content-block-{{section.id}} .img-image .image-align-right img{
            margin-left: auto;
            margin-right: 0;
          }
          .image-with-content.image-content-block-{{section.id}} .banner-sub-heading {
              font-size: {{ block.settings.font_size_sub_title | append: 'px' }};
              padding-bottom: {{ block.settings.sub_title__space_top | append: 'px' }};
              font-weight: {{ block.settings.sub_title_font_weight }};
              text-transform: {{ block.settings.sub_title_text_transform }};
              letter-spacing: {{ block.settings.font_size_sub_letter_spacing }};
              {% if block.settings.sub_title_color != blank %}
                    color:{{ block.settings.sub_title_color }};
              {% else %}
                    color: var(--heading-text-color);
              {% endif %}
              font-family: var(--image_with-content-sub-heading-font-family);
          }
          .image-with-content.image-content-block-{{section.id}} .banner-heading {
              font-size: {{ block.settings.heading_title | append: 'px' }};
              padding-bottom: {{ block.settings.heading__space_bottom | append: 'px' }};
              font-weight: {{ block.settings.block_title_font_weight }};
          }
          .image-with-content.image-content-block-{{section.id}} .row.align-center > * {
              align-items: center;
          }
          .image-with-content.image-content-block-{{section.id}} .row.align-bottom > * {
              align-items: flex-end;
          }
          .image-with-content.image-content-block-{{section.id}} .banner-content {
              margin: 0 auto;
              width: 100%;
          }
          .image-with-content.image-content-block-{{section.id}} .banner-content.text-center .description {
            margin: 0 auto;
          }
          .image-with-content.image-content-block-{{section.id}} .banner-content.text-right .description {
            margin-left: auto;
          }
          .image-with-content.image-content-block-{{section.id}} .banner-content .description {
              max-width: {{ block.settings.content_width | append: 'px' }};
          }
          .image-with-content.image-content-block-{{section.id}} .row_reverse > *:first-child {
              order: 2;
          }
          .image-with-content.image-content-block-{{section.id}} .banner-content .description {
              max-width: {{ block.settings.content_width | append: 'px' }};
          }
          .image-with-content.image-content-block-{{section.id}} .row > *:first-child {
              order: {{ block.settings.desktop_order }};
          }
          .image-with-content.image-content-block-{{section.id}} .row > *:last-child {
              order: {{ block.settings.desktop_order_2 }};
          }
          .image-with-content.image-content-block-{{section.id}} .description {
              padding-bottom: {{ block.settings.description__space_bottom | append: 'px' }};
              font-size: {{ block.settings.desc_title | append: 'px' }};
              {% if block.settings.desc_color != blank %}
                   color: {{ block.settings.desc_color }};
              {% else %}
                   color: var(--body-text-color);
              {% endif %}
              font-weight: {{ block.settings.desc_font_weight }};
          }
          .image-content-block-{{section.id}} .button {
              letter-spacing: {{ block.settings.button_letter_spacing | append: 'px' }};
              {% if block.settings.button_border_color != blank %}
                  border: {{ block.settings.button_border | append: 'px' }} solid {{ block.settings.button_border_color }};
              {% else %}
                  border: {{ block.settings.button_border | append: 'px' }} solid var(--button-border-color);
              {% endif %}
              {% if block.settings.button_bg_color != blank %}
                  background: {{ block.settings.button_bg_color }};
              {% else %}
                  background: var(--button-bg-color);
              {% endif %}
              {% if block.settings.button_color != blank %}
                  color: {{ block.settings.button_color }};
              {% else %}
                  color: var(--button-color);
              {% endif %}
          }
          .image-content-block-{{section.id}} .button svg {
              {% if block.settings.button_color != blank %}
                  fill: {{ block.settings.button_color }};
              {% else %}
                  color: var(--button-color);
              {% endif %}
              transition: all 400ms ease;
          }
        
          {% if settings.button_hover_style == '2' %}
            .image-content-block-{{section.id}} .button {
              position: relative;
              overflow: hidden;
              z-index: 0;
            }
            .image-content-block-{{section.id}} .button:before {
              content: "";
              position: absolute;
              top: 0;
              right: 0;
              bottom: 0;
              left: 0;
              transform: unset;
              display: unset;
              border: unset;
              border-radius: unset;
              width: unset;
              height: unset;
              animation: unset;
              opacity: unset;
              visibility: unset;
              background: {{ block.settings.button_bghover_color }};
              right: 100%;
              z-index: -1;
              transition: all 400ms ease;
            }
            .image-content-block-{{section.id}} .button:hover {
              {% if block.settings.button_hover_color != blank %}
                  color: {{ block.settings.button_hover_color }};
              {% else %}
                  color: var(--button-hover-color);
              {% endif %}
              {% if block.settings.button_borderhover_color != blank %}
                  border: {{ block.settings.button_border | append: 'px' }} solid {{ block.settings.button_borderhover_color }};
              {% else %}
                  border: {{ block.settings.button_border | append: 'px' }} solid var(--button-border-hover-color);
              {% endif %}
              
            }
            .image-content-block-{{section.id}} .button:hover svg {
              {% if block.settings.button_hover_color != blank %}
                  fill: {{ block.settings.button_hover_color }};
              {% else %}
                  fill: var(--button-hover-color);
              {% endif %}
            }
            .image-content-block-{{section.id}} .button:hover:before {
              right: 0%;
            }
          {% elsif settings.button_hover_style == '3' %}
            .image-content-block-{{section.id}} .button {
              position: relative;
              overflow: hidden;
              z-index: 0;
            }
            .image-content-block-{{section.id}} .button:before {
              content: "";
              position: absolute;
              top: 0;
              left: -70%;
              display: unset;
              border: unset;
              border-radius: unset;
              animation: unset;
              opacity: unset;
              visibility: unset;
              background: {{ block.settings.button_bghover_color }};
              z-index: -1;
              transition: all 400ms ease;
              clip-path: polygon(25px 0%, 100% 0%, 100% 100%, 0% 100%);
              width: 70%;
              height: 100%;
              transform: rotateY(180deg);
            }
            .image-content-block-{{section.id}} .button:after {
              content: "";
              position: absolute;
              top: 0;
              right: -70%;
              transform: unset;
              display: unset;
              border: unset;
              border-radius: unset;
              animation: unset;
              opacity: unset;
              visibility: unset;
              background: {{ block.settings.button_bghover_color }};
              z-index: -1;
              transition: all 400ms ease;
              clip-path: polygon(25px 0%, 100% 0%, 100% 100%, 0% 100%);
              width: 70%;
              height: 100%;
            }
            .image-content-block-{{section.id}} .button:hover {
              color: {{ block.settings.button_hover_color }};
               border: {{ block.settings.button_border | append: 'px' }} solid {{ block.settings.button_borderhover_color }};
            }
            .image-content-block-{{section.id}} .button:hover svg {
              fill: {{ block.settings.button_hover_color }};
            }
            .image-content-block-{{section.id}} .button:hover:before {
              left: calc(0% - 2px);
            }
