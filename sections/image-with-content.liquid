{%- if section.settings.clip_path_enabel != blank -%}
  <script>
    $(window).on("load resize scroll", function() {
      $('.image-content-block-{{section.id}}').each(function(){
          var height_sec = $(".image-content-block-{{section.id}}").height();
          var main_height = (height_sec - height_sec);
          if ($(".image-content-block-{{section.id}}").offset().top < ($(window).scrollTop() + main_height)) {
              var difference = ($(window).scrollTop() + main_height) - $(this).offset().top;
              var half = (difference / 5) + 'px',
              left = ' + half + ';
              $(this).find('.image-content-block-animation-{{section.id}}').css('clip-path', 'inset(0 '+ half +' 0 '+ half +')');
            
          } else {
              $(this).find('.image-content-block-animation-{{section.id}}').css('clip-path', 'inset(-1px -1px -1px -1px)');
          }
      });
    });
