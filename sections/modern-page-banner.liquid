{%- liquid
  assign container = section.settings.container
  assign background_color = section.settings.background_color
  assign background_gradient = section.settings.background_gradient
  assign blog_bg_image = section.settings.background_image
-%}

<style>
  {% if settings.type_font_1 == 'shopify_fonts' and settings.type_font_2 == 'shopify_fonts' %}
      {% assign base_font = settings.type_font_1_font | font_modify: 'weight', '400' %}
      {% assign base_font_medium = settings.type_font_1_font | font_modify: 'weight', '500' %}
      {% assign base_font_sem_bold = settings.type_font_1_font | font_modify: 'weight', '600' %}
      {% assign base_font_bold = settings.type_font_1_font | font_modify: 'weight', 'bold' %}

      {{ base_font | font_face | replace: '@font-face {', '@font-face { font-display: swap;' }}
      {{ base_font_medium | font_face | replace: '@font-face {', '@font-face { font-display: swap;' }}
      {{ base_font_sem_bold | font_face | replace: '@font-face {', '@font-face { font-display: swap;' }}
      {{ base_font_bold | font_face | replace: '@font-face {', '@font-face { font-display: swap;' }}

      {% assign fonts_name = base_font.family | append: ',' | append: base_font.fallback_families %}

      {% if settings.type_font_2_font.family != settings.type_font_1_font.family %}
          {% assign header_font = settings.type_font_2_font | font_modify: 'weight', '400' %}
          {% assign header_font_bold = settings.type_font_2_font | font_modify: 'weight', 'bold' %}
          {% assign header_font_sem_bold = settings.type_font_2_font | font_modify: 'weight', '600' %}
          {% assign header_font_medium = settings.type_font_2_font | font_modify: 'weight', '500' %}
          
          {{ header_font | font_face | replace: '@font-face {', '@font-face { font-display: swap;' }}
          {{ header_font_medium | font_face | replace: '@font-face {', '@font-face { font-display: swap;' }}
          {{ header_font_sem_bold | font_face | replace: '@font-face {', '@font-face { font-display: swap;' }}
          {{ header_font_bold | font_face | replace: '@font-face {', '@font-face { font-display: swap;' }}

                {% assign fonts_name_2 = header_font.family | append: ',' | append: header_font.fallback_families %}
      {% else %}
                {% assign fonts_name_2 = base_font.family | append: ',' | append: base_font.fallback_families %}
      {% endif %}
  {% elsif settings.type_font_1 != 'shopify_fonts' and settings.type_font_2 != 'shopify_fonts' %}
        {%- assign font_1 = settings.type_font_1_name -%}
        {%- capture font_name_1 -%}
            {{font_1 | strip | replace: ' ', '+'}}:300,300i,400,400i,500,500i,600,600i,700,700i,800,800i
        {%- endcapture -%}
        @import url('https://fonts.googleapis.com/css?family={{font_name_1}}&display=swap');
              {% assign fonts_name = font_1 %}

        {% if settings.type_font_1_name != settings.type_font_2_name %}
            {%- assign font_2 = settings.type_font_2_name -%}
            {%- capture fonts_name_2 -%}
                {{font_2 | strip | replace: ' ', '+'}}:300,300i,400,400i,500,500i,600,600i,700,700i,800,800i
            {%- endcapture -%}
            @import url('https://fonts.googleapis.com/css?family={{fonts_name_2}}&display=swap');
                  {% assign fonts_name_2 = font_2 %}
        {% else %}
                  {% assign fonts_name_2 = font_1 %}
        {% endif %}
  {% elsif settings.type_font_1 == 'shopify_fonts' %}
        {% assign base_font = settings.type_font_1_font | font_modify: 'weight', '400' %}
        {% assign base_font_medium = settings.type_font_1_font | font_modify: 'weight', '500' %}
        {% assign base_font_sem_bold = settings.type_font_1_font | font_modify: 'weight', '600' %}
        {% assign base_font_bold = settings.type_font_1_font | font_modify: 'weight', 'bold' %}

        {{ base_font | font_face | replace: '@font-face {', '@font-face { font-display: swap;' }}
        {{ base_font_medium | font_face | replace: '@font-face {', '@font-face { font-display: swap;' }}
        {{ base_font_sem_bold | font_face | replace: '@font-face {', '@font-face { font-display: swap;' }}
        {{ base_font_bold | font_face | replace: '@font-face {', '@font-face { font-display: swap;' }}

        {% assign fonts_name = base_font.family | append: ',' | append: base_font.fallback_families %}
      
        {% if base_font.family != settings.type_font_2_name %}
            {%- assign font_2 = settings.type_font_2_name -%}
            {%- capture fonts_name_2 -%}
                {{font_2 | strip | replace: ' ', '+'}}:300,300i,400,400i,500,500i,600,600i,700,700i,800,800i
            {%- endcapture -%}
            @import url('https://fonts.googleapis.com/css?family={{fonts_name_2}}&display=swap');
                    {% assign fonts_name_2 = font_2 %}
        {% else %}
                  {% assign fonts_name_2 = base_font.family | append: ',' | append: base_font.fallback_families %}
        {% endif %}
  {% else %}
        {% assign header_font = settings.type_font_2_font | font_modify: 'weight', '400' %}
        {% assign header_font_bold = settings.type_font_2_font | font_modify: 'weight', 'bold' %}
        {% assign header_font_sem_bold = settings.type_font_2_font | font_modify: 'weight', '600' %}
        {% assign header_font_medium = settings.type_font_2_font | font_modify: 'weight', '500' %}

        {% if header_font.family != settings.type_font_1_name %}
            {%- assign font_1 = settings.type_font_1_name -%}
            {%- capture font_name_1 -%}
                {{font_1 | strip | replace: ' ', '+'}}:300,300i,400,400i,500,500i,600,600i,700,700i,800,800i
            {%- endcapture -%}
            @import url('https://fonts.googleapis.com/css?family={{font_name_1}}&display=swap');
                  {% assign fonts_name = font_1 %}
        {% else %}
                  {% assign fonts_name = header_font.family | append: ',' | append: header_font.fallback_families %}
        {% endif %}

        {{ header_font | font_face | replace: '@font-face {', '@font-face { font-display: swap;' }}
        {{ header_font_medium | font_face | replace: '@font-face {', '@font-face { font-display: swap;' }}
        {{ header_font_sem_bold | font_face | replace: '@font-face {', '@font-face { font-display: swap;' }}
        {{ header_font_bold | font_face | replace: '@font-face {', '@font-face { font-display: swap;' }}
             
        {% assign fonts_name_2 = header_font.family | append: ',' | append: header_font.fallback_families %}
  {% endif %}
  
  .modern-page-banner-{{section.id}} {
      background: {% if section.settings.background_gradient != blank %}{{ section.settings.background_gradient }}{% else %}{{ section.settings.background_color }}{% endif %};
      padding-top: {{ section.settings.mg_top_desktop | append: 'px' }};
      padding-bottom: {{ section.settings.mg_bottom_desktop | append: 'px' }};
      position: relative;
  }
  .modern-page-banner-{{section.id}} .modern-page-banner-bg, .modern-page-banner-{{section.id}} .modern-page-banner-bg .animate-ambient-image {
    position: absolute;
    top: 0;
    left: 0;
    width: 100%;
    height: 100%;
  }
  .modern-page-banner-{{section.id}} .modern-page-banner-bg .animate-ambient-image img {
    position: absolute;
    top: 0;
    left: 0;
    width: 100%;
    height: 100%;
    object-fit: cover;
  }
  .modern-page-banner-{{section.id}} .row.align-iteam-center {
      align-items: center;
  }
  .modern-page-banner-{{section.id}} .row.align-iteam-bottom {
      align-items: flex-end;
  }

  @media (max-width: 1199px) {
    .modern-page-banner-{{section.id}} {
      padding-top: {{ section.settings.mg_top_tablet | append: 'px' }};
      padding-bottom: {{ section.settings.mg_bottom_tablet | append: 'px' }};
    }  
  }

  @media (max-width: 767px) {
    .modern-page-banner-{{section.id}} {
      padding-top: {{ section.settings.mg_top_mobile | append: 'px' }};
      padding-bottom: {{ section.settings.mg_bottom_mobile | append: 'px' }};
    }
  }
</style>

<div class="modern-page-banner modern-page-banner-{{section.id}}">
  <div class="modern-page-banner-bg {% if section.settings.image_behavior != 'none' %} animate--{{ section.settings.image_behavior }} animate-hidden{% endif %}">
    <div class="animate-ambient-image">
      {%- if blog_bg_image != blank -%}
        <img {% render 'lazyload', preview_img: blog_bg_image %} loading="lazy"/>
        {% if settings.enable_lazyload %}<span class="data-lazy-loading"></span>{% endif %}
      {%- endif -%}
    </div>
  </div>
  <div class="{% if container == 'fullwidth'%}container-full{% else %}container{% endif %}">
    <div class="row align-iteam-{{ section.settings.align }}">
      {%- for block in section.blocks -%}
        {%- case block.type -%}
          {%- when 'text' -%}
            <div class="modern-page-banner-content modern-page-banner-content-{{ block.id }} col-lg-{{ block.settings.content_grid }} col-md-{{ block.settings.content_grid_tablate }} col-{{ block.settings.content_grid_mobile }} text-{{ block.settings.align }} text-tb-{{ block.settings.align_tb }}">
              {%- if block.settings.sub_title != blank -%}
                  <h4 class="modern-banner-sub-heading wow {{ block.settings.animations_type_sub_title }} delay-{{ block.settings.animation_delay_sub_title | replace: ".", "_" }}">
                    <style>
                      .modern-page-banner-content-{{ block.id }} .modern-banner-sub-heading.delay-{{ block.settings.animation_delay_sub_title | replace: ".", "_" }} {
                          animation-delay: {{ block.settings.animation_delay_sub_title }}
                      }
                    </style>
                    {{ block.settings.sub_title }}
                  </h4>
              {%- endif -%}
              {%- if block.settings.heading != blank -%}
                  <h2 class="modern-banner-heading wow {{ block.settings.animations_type_title }} delay-{{ block.settings.animation_delay_title | replace: ".", "_" }}">
                    <style>
                      .modern-page-banner-content-{{ block.id }} .modern-banner-heading.delay-{{ block.settings.animation_delay_title | replace: ".", "_" }} {
                          animation-delay: {{ block.settings.animation_delay_title }}
                      }
                    </style>
                      {{ block.settings.heading }}
                  </h2>
              {%- endif -%}
              {%- if block.settings.description_text != blank -%}
                  <div class="modern-banner-content-text wow {{ block.settings.animations_type_desc }} delay-{{ block.settings.animation_delay_desc | replace: ".", "_" }}">
                    <style>
                      .modern-page-banner-content-{{ block.id }} .modern-banner-content-text.delay-{{ block.settings.animation_delay_desc | replace: ".", "_" }} {
                          animation-delay: {{ block.settings.animation_delay_desc }}
                      }
                    </style>
                      <p>{{ block.settings.description_text }}</p>
                  </div>
              {%- endif -%}

              {%- if block.settings.image_banner_button != blank or block.settings.image_banner_button_2 != blank -%}
                <div class="button-group">
                    {%- if block.settings.image_banner_button != blank -%}
                      {% if block.settings.button_style == '1'%}
                        <div class="image-banner-button wow {{ block.settings.animations_type_button_1 }} delay-{{ block.settings.animation_delay_button_1 | replace: ".", "_" }}">
                          <style>
                            .modern-page-banner-content-{{ block.id }} .image-banner-button.delay-{{ block.settings.animation_delay_button_1 | replace: ".", "_" }} {
                                animation-delay: {{ block.settings.animation_delay_button_1 }}
                            }
                          </style>
                          <a {% if block.settings.image_banner_link == blank %} role="link" aria-disabled="true"{% else %} href="{{ block.settings.image_banner_link }}"{% endif %} class="button">
                            {{ block.settings.image_banner_button }}
                          </a>
                        </div>
                      {% else %}
                        <div class="image-banner-button wow {{ block.settings.animations_type_button_1 }} delay-{{ block.settings.animation_delay_button_1 | replace: ".", "_" }}">
                          <style>
                            .modern-page-banner-content-{{ block.id }} .image-banner-button.delay-{{ block.settings.animation_delay_button_1 | replace: ".", "_" }} {
                                animation-delay: {{ block.settings.animation_delay_button_1 }}
                            }
                          </style>
                          <a {% if block.settings.image_banner_link == blank %} role="link" aria-disabled="true"{% else %} href="{{ block.settings.image_banner_link }}"{% endif %} class="button">
                            {{ block.settings.image_banner_button }} {% render 'arrow-right-2' %}
                          </a>
                        </div>
                      {% endif %}
                    {%- endif -%}
  
                    {%- if block.settings.image_banner_button_2 != blank -%}
                      {% if block.settings.button_style_2 == '1'%}
                        <div class="image-banner-button wow {{ block.settings.animations_type_button_2 }} delay-{{ block.settings.animation_delay_button_2 | replace: ".", "_" }}">
                          <style>
                            .modern-page-banner-content-{{ block.id }} .image-banner-button.delay-{{ block.settings.animation_delay_button_2 | replace: ".", "_" }} {
                                animation-delay: {{ block.settings.animation_delay_button_2 }}
                            }
                          </style>
                          <a {% if block.settings.image_banner_link_2 == blank %} role="link" aria-disabled="true"{% else %} href="{{ block.settings.image_banner_link_2 }}"{% endif %} class="button button_2">
                            {{ block.settings.image_banner_button_2 }}
                          </a>
                        </div>
                      {% else %}
                        <div class="image-banner-button wow {{ block.settings.animations_type_button_2 }} delay-{{ block.settings.animation_delay_button_2 | replace: ".", "_" }}">
                          <style>
                            .modern-page-banner-content-{{ block.id }} .image-banner-button.delay-{{ block.settings.animation_delay_button_2 | replace: ".", "_" }} {
                                animation-delay: {{ block.settings.animation_delay_button_2 }}
                            }
                          </style>
                          <a {% if block.settings.image_banner_link_2 == blank %} role="link" aria-disabled="true"{% else %} href="{{ block.settings.image_banner_link_2 }}"{% endif %} class="button button_2">
                            {{ block.settings.image_banner_button_2 }} {% render 'arrow-right-2' %}
                          </a>
                        </div>
                      {% endif %}
                    {%- endif -%}
                </div>
              {%- endif -%}
              <style>
                :root {
                  {% if block.settings.sub_heading_fontfamily == 'font_1' %}
                      --modern-banner-heading-font-family: {{fonts_name}};
                  {% else %}
                      --modern-banner-heading-font-family: {{fonts_name_2}};
                  {% endif %}
                }
                .modern-page-banner-content-{{ block.id }} {
                    padding-top: {{ block.settings.mg_top_desktop | append: 'px' }};
                    padding-bottom: {{ block.settings.mg_bottom_desktop | append: 'px' }};
                }
                .modern-page-banner-{{section.id}} .container {
                      position: relative;
                }
                .modern-page-banner-content-{{ block.id }} .modern-banner-sub-heading{
                    font-size: {{ block.settings.sub_title_font_size | append: 'px' }}; 
                    padding-bottom: {{ block.settings.sub_title__space_bottom | append: 'px' }};
                    font-weight: {{ block.settings.sub_title_font_weight }};
                    color: {{ block.settings.sub_title_color }};
                    text-transform: {{ block.settings.sub_title_text_transform }};
                    font-family: var(--modern-banner-heading-font-family);
                    letter-spacing: {{ block.settings.sub_title_letter_spacing | append: 'px' }};
                }
                .modern-page-banner-content-{{ block.id }} .modern-banner-heading{
                    font-size: {{ block.settings.title_font_size | append: 'px' }}; 
                    line-height: {{ block.settings.title_line_height | append: 'px' }};
                    padding-bottom: {{ block.settings.title__space_bottom | append: 'px' }};
                    font-weight: {{ block.settings.title_font_weight }};
                    color: {{ block.settings.title_color }};
                    text-transform: {{ block.settings.title_text_transform }};
                    font-family: var(--modern-heading-font-family);
                    letter-spacing: {{ block.settings.title_letter_spacing | append: 'px' }};
                }
                .modern-page-banner-content-{{ block.id }} .modern-banner-content-text {
                    font-size: {{ block.settings.desc_title | append: 'px' }};
                    max-width: {{ block.settings.content__width | append: 'px' }};
                    color: {{ block.settings.desc_color }};
                    font-weight: {{ block.settings.desc_font_weight }};
                    padding-bottom: {{ block.settings.description__space_bottom | append: 'px' }};
                    text-transform: {{ block.settings.desc_text_transform }};
                    letter-spacing: {{ block.settings.desc_letter_spacing | append: 'px' }};
                }
                .modern-page-banner-content-{{ block.id }}.text-center .modern-banner-content-text {
                  margin-left: auto;
                  margin-right: auto;
                }
                .modern-page-banner-content-{{ block.id }}.text-right .modern-banner-content-text {
                  margin-left: auto;
                }

                .modern-page-banner-content-{{ block.id }} .image-banner-button .button {
                  color: {{ block.settings.button_color }};
                  background-color: {{ block.settings.button_bg_color }};
                  border: {{ block.settings.button_border | append: 'px' }} solid {{ block.settings.button_border_color }};
                  letter-spacing: {{ block.settings.button_letter_spacing | append: 'px' }};
                  padding:{{ block.settings.btn_padding_top_bottom | append: 'px' }} {{ block.settings.btn_padding_left_right | append: 'px' }};
                  font-size:{{ block.settings.button_font_size | append: 'px' }};
                }
                .modern-page-banner-content-{{ block.id }} .image-banner-button .button svg {
                  fill: {{ block.settings.button_color }};
                  transition: all 400ms ease;
                }
                .modern-page-banner-content-{{ block.id }} .image-banner-button .button:hover svg {
                  fill: {{ block.settings.button_hover_color }};
                }
                .modern-page-banner-content-{{ block.id }} .image-banner-button .button:hover {
                  color: {{ block.settings.button_hover_color }};
                  background-color: {{ block.settings.button_bghover_color }};
                  border: {{ block.settings.button_border | append: 'px' }} solid {{ block.settings.button_borderhover_color }};
                }


                .modern-page-banner-content-{{ block.id }} .image-banner-button .button_2 {
                  color: {{ block.settings.button_color_2 }};
                  background-color: {{ block.settings.button_bg_color_2 }};
                  border: {{ block.settings.button_border_2 | append: 'px' }} solid {{ block.settings.button_border_color_2 }};
                  letter-spacing: {{ block.settings.button_letter_spacing_2 | append: 'px' }};
                  padding:{{ block.settings.btn_padding_top_bottom_2 | append: 'px' }} {{ block.settings.btn_padding_left_right_2 | append: 'px' }};
                  font-size:{{ block.settings.button_font_size_2 | append: 'px' }};
                }
                .modern-page-banner-content-{{ block.id }} .image-banner-button .button_2 svg {
                  fill: {{ block.settings.button_color_2 }};
                  transition: all 400ms ease;
                }
                .modern-page-banner-content-{{ block.id }} .image-banner-button .button_2:hover svg {
                  fill: {{ block.settings.button_hover_color_2 }};
                }
                .modern-page-banner-content-{{ block.id }} .image-banner-button .button_2:hover {
                  color: {{ block.settings.button_hover_color_2 }};
                  background-color: {{ block.settings.button_bghover_color_2 }};
                  border: {{ block.settings.button_border_2 | append: 'px' }} solid {{ block.settings.button_borderhover_color_2 }};
                }
                .button-group {
                    display: flex;
                    flex-wrap: wrap;
                    margin: 0px -10px;
                }
                .image-banner-button {
                    padding: 0px 10px;
                }

                @media (max-width: 1199px) {
                  .modern-page-banner-content-{{ block.id }} {
                      padding-top: {{ block.settings.mg_top_tablet | append: 'px' }};
                      padding-bottom: {{ block.settings.mg_bottom_tablet | append: 'px' }};
                  }
                }

                @media (max-width: 1024px) {
                  .modern-page-banner-content-{{ block.id }} .modern-banner-sub-heading{
                    font-size: {{ block.settings.sub_title_tb_font_size | append: 'px' }}; 
                    padding-bottom: {{ block.settings.sub_title__space_bottom_tb | append: 'px' }};
                  }
                  .modern-page-banner-content-{{ block.id }} .modern-banner-heading{
                    font-size: {{ block.settings.title_tb_font_size | append: 'px' }}; 
                    line-height: {{ block.settings.title_line_height_tb | append: 'px' }};
                    padding-bottom: {{ block.settings.title__space_bottom_tb | append: 'px' }};
                  }
                  .modern-page-banner-content-{{ block.id }} .modern-banner-content-text {
                      font-size: {{ block.settings.desc_tb | append: 'px' }};
                      padding-bottom: {{ block.settings.description__space_bottom_tb | append: 'px' }};
                  }
                  .modern-page-banner-content-{{ block.id }}.text-tb-center .modern-banner-content-text {
                      margin-left: auto;
                      margin-right: auto;
                  }
                  .modern-page-banner-content-{{ block.id }}.text-tb-center .button-group {
                      justify-content: center;
                  }
                  .modern-page-banner-content-{{ block.id }} .image-banner-button .button {
                    font-size:{{ block.settings.button_font_size_tb_font_size | append: 'px' }};
                  }
                  .modern-page-banner-content-{{ block.id }} .image-banner-button .button_2 {
                    font-size:{{ block.settings.button_font_size_tb_font_size_2 | append: 'px' }};
                  }
                }

                @media (max-width: 767px) {
                  .modern-page-banner-content-{{ block.id }} {
                      padding-top: {{ block.settings.mg_top_mobile | append: 'px' }};
                      padding-bottom: {{ block.settings.mg_bottom_mobile | append: 'px' }};
                  }
                  .modern-page-banner-content-{{ block.id }} .modern-banner-sub-heading{
                    font-size: {{ block.settings.sub_title_mb_font_size | append: 'px' }}; 
                    padding-bottom: {{ block.settings.sub_title__space_bottom_mb | append: 'px' }};
                    text-align: center;
                  }
                  .modern-page-banner-content-{{ block.id }} .modern-banner-heading{
                    font-size: {{ block.settings.title_mb_font_size | append: 'px' }}; 
                    line-height: {{ block.settings.title_line_height_mb | append: 'px' }};
                    padding-bottom: {{ block.settings.title__space_bottom_mb | append: 'px' }};
                    text-align: center;
                  }
                  .modern-page-banner-content-{{ block.id }} .button-group {
                      justify-content: center;
                  }
                  .modern-page-banner-content-{{ block.id }} .image-banner-button {
                      padding: 5px 10px;
                  }
                  .modern-page-banner-content-{{ block.id }} .modern-banner-content-text {
                      font-size: {{ block.settings.desc_mb | append: 'px' }};
                      padding-bottom: {{ block.settings.description__space_bottom_mb | append: 'px' }};
                      text-align: center;
                      max-width: 100%;
                  }
                  .modern-page-banner-content-{{ block.id }} .image-banner-button .button {
                    padding:{{ block.settings.btn_padding_top_bottom_mb | append: 'px' }} {{ block.settings.btn_padding_left_right_mb | append: 'px' }};
                    font-size:{{ block.settings.button_font_size_mb_font_size | append: 'px' }};
                  }
                  .modern-page-banner-content-{{ block.id }} .image-banner-button .button_2 {
                    padding:{{ block.settings.btn_padding_top_bottom_mb_2 | append: 'px' }} {{ block.settings.btn_padding_left_right_mb_2 | append: 'px' }};
                    font-size:{{ block.settings.button_font_size_mb_font_size_2 | append: 'px' }};
                  }
                }
              </style>
            </div>
          {%- when 'image' -%}
            <div class="modern-page-banner-image modern-page-banner-image-{{ block.id }} col-lg-{{ block.settings.content_grid }} col-md-{{ block.settings.content_grid_tablate }} col-{{ block.settings.content_grid_mobile }}">
              <div class="modern-page-banner-image-inner">
                <div class="image-group">
                  {%- liquid
                    assign preview_img = block.settings.image
                    assign preview_img_2 = block.settings.image_2
                  -%}
                  <div class="image-group-box wow {{ block.settings.animations_type_image_1 }} delay-{{ block.settings.animation_delay_image_1 | replace: ".", "_" }}">
