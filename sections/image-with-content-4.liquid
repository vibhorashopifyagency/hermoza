{%- liquid
  assign container = section.settings.container
  assign background_color = section.settings.background_color
  assign background_gradient = section.settings.background_gradient
-%}

<style>
  .image-content-block-{{section.id}} {
      background: {% if background_gradient != blank %}{{ background_gradient }}{% else %}{{ background_color }}{% endif %};
  }
  .image-content-block-{{section.id}} .image-content-animation-{{section.id}} {
    background: {% if section.settings.clip_background_gradient != blank %}{{ section.settings.clip_background_gradient }}{% else %}{{ section.settings.clip_background_color }}{% endif %};
    padding-top: {{ section.settings.mg_top_desktop | append: 'px' }};
    padding-bottom: {{ section.settings.mg_bottom_desktop | append: 'px' }};
  }
  .image-with-content-2 .align-center{
    align-items: center;
  }
  .image-with-content-2 .images-box-main > div img {
    display: block;
    box-shadow: 0px 0px 10px 0px rgb(0 0 0 / 10%);
  }
  .image-with-content-2 .images-box-main .images-box-1 img,
  .image-with-content-2 .images-box-main .images-box-2 img,
  .image-with-content-2 .images-box-main .images-box-3 img {
    width: 100%;
  }
  .image-with-content-2 .images-box-main .images-box-1{
    margin-bottom: 30px;
  }
  .image-with-content-2 .images-box-main .images-box-2{
    top: 26%;
    position: sticky;
  }
  .image-with-content-2 .images-box-main .images-box-1.images-box-3-blank{
    margin-bottom: 0px !important;
  }
  .image-with-content-2 .images-box-main .images-box-1.images-box-2-blank{
    width: 100%;
  }
  .image-with-content-2 .images-box-main .images-box-1.images-box-2-blank img{
    margin: auto;
  }
  .image-content-block-{{section.id}} .images-box-in {
    overflow: hidden;
  }
  /* .image-content-block-{{section.id}} .images-box-in img {
        transform: translate3d(0px, 0px, 0px) scale(1.2);
  } */
  .passion-main-box-1 {
    transform: translate(0px, var(--up-amount));
  }
  @media (max-width: 1199px) {
    .image-content-block-{{section.id}} .image-content-animation-{{section.id}} {
      padding-top: {{ section.settings.mg_top_tablet | append: 'px' }};
      padding-bottom: {{ section.settings.mg_bottom_tablet | append: 'px' }};
    }  
    .image-with-content-2 .images-box-main .images-box-1 {
      margin-bottom: 7%;
    }
  }
  @media (max-width: 767px) {
    .image-content-block-{{section.id}} .image-content-animation-{{section.id}} {
      padding-top: {{ section.settings.mg_top_mobile | append: 'px' }};
      padding-bottom: {{ section.settings.mg_bottom_mobile | append: 'px' }};
    }
    .image-with-content-2.image-content-block-{{section.id}} .img-image{
      order: 2;
    }
    .image-with-content-2.image-content-block-{{section.id}} .content-im{
      order: 1;
    }
  }
</style>
<div class="image-content-2-block-{{section.id}}">
  <div class="image-with-content-2 image-content-block-{{section.id}}">
    <div class="image-content-animation-{{section.id}}">
      <div class="{% if container == 'fullwidth'%}container-full{% else %}container{% endif %}">
