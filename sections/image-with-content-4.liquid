                overflow: hidden;
                z-index: 0;
              }
              .image-content-block-{{section.id}} .button:before {
                content: "";
                position: absolute;
                top: 0;
                right: 0;
                bottom: 0;
                left: 0;
                transform: unset;
                display: unset;
                border: unset;
                border-radius: unset;
                width: unset;
                height: unset;
                animation: unset;
                opacity: unset;
                visibility: unset;
