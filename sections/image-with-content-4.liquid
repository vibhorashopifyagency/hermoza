                font-weight: {{ block.settings.block_title_font_weight }};
                text-transform: {{ block.settings.block_title_transform }};
                {% if block.settings.title_color != blank %}
                     color: {{ block.settings.title_color }};
                {% else %}
                     color: var(--heading-text-color);
                {% endif %}
                
