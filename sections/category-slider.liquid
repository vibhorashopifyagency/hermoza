      cat_img_height();
    });
    if ($(".section-id-{{section.id}} .brand-blocks").length > 0) {
        $(".section-id-{{section.id}} .brand-blocks").slick({
          slidesToShow: {{ brand_show }},
          slidesToScroll: 1,
          speed: 400,
          dots: {{ brand_dots }},
