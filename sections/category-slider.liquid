              settings: {
                dots: {{ brand_dots_mb }},
                arrows: {{ brand_arrows_mb }},
                slidesToShow: {{ brand_show_tb }}
              }
            },
            {
              breakpoint: 768,
              settings: {
                dots: {{ brand_dots_mb }},
                arrows: {{ brand_arrows_mb }},
                slidesToShow: {{ brand_show_mb }}
              }
            },
          ]
        });
    }
