{% if section.blocks.size > 0 %}
  {%-liquid
    assign container = section.settings.container
    assign dots = section.settings.product_block_dots
    assign dots_mb = section.settings.product_block_dots_mb
    assign arrows = section.settings.product_block_arrows
    assign arrows_mb = section.settings.product_block_arrows_mb
    assign autoplay = section.settings.autoplay
    assign slideshow_speed = section.settings.slideshow_speed
    assign slideshow_transition = section.settings.slideshow_transition
   
  
    if settings.enable_layout_rtl != blank
        assign rtl = true
    else
        assign rtl = false
    endif
  -%}

  {% render 'slide-show-style' %}
  <div class="slide-show {% if section.settings.slick_dots_style == '1'%}dot-style-1{% else %}dot-style-2{% endif %}" id="slideshow-wrapper-{{section.id}}"">
    <div class="{% if container == 'fullwidth'%}container-full{% else %}container{% endif %}">
      <div class="slideshow-slider" data-auto-video="{{ section.settings.auto_playvideo }}">
        {% for block in section.blocks %}
          {% if block.type == 'image' or block.type == 'video' %}
            <div class="slide-show-slde block-{{block.id}}" >
            
              {% if block.type == 'image' %}
                <div class="images-contain {% if section.settings.image_behavior != 'none' %} animate--{{ section.settings.image_behavior }} animate-hidden{% endif %}">
                  {% if block.settings.image_slide != blank %}
                    <div class="animate-ambient-image {% if block.settings.image_slide_mb != blank %} slide-pc{% endif %}">
                      <a {% if block.settings.image_banner_link == blank %} role="link" aria-disabled="true"{% else %} href="{{ block.settings.image_banner_link }}"{% endif %}>
                        {%- assign img_url = block.settings.image_slide -%}
                        <img {% if settings.enable_lazyload and forloop.first == false %}data-{% endif %}srcset="{%- if img_url.width < 320 -%}{{ img_url | image_url: width: img_url.width }} 320w,{%- endif -%}
                            {%- if img_url.width >= 320 -%}{{ img_url | image_url: width: 375 }} 375w,{%- endif -%}
                            {%- if img_url.width >= 740 -%}{{ img_url | image_url: width: 740 }} 740w,{%- endif -%} 
                            {%- if img_url.width >= 750 -%}{{ img_url | image_url: width: 750 }} 750w,{%- endif -%}
                            {%- if img_url.width >= 1100 -%}{{ img_url | image_url: width: 1100 }} 1100w,{%- endif -%}
                            {%- if img_url.width >= 1370 -%}{{ img_url | image_url: width: 1370 }} 1370w,{%- endif -%}
                            {%- if img_url.width >= 1500 -%}{{ img_url | image_url: width: 1500 }} 1500w,{%- endif -%}
                            {%- if img_url.width >= 1770 -%}{{ img_url | image_url: width: 1770 }} 1770w,{%- endif -%}
                            {%- if img_url.width >= 1780 -%}{{ img_url | image_url: width: 1780 }} 1780w,{%- endif -%}
                            {%- if img_url.width >= 1920 -%}{{ img_url | image_url: width: 1920 }} 1920w,{%- endif -%}
                            {%- if img_url.width >= 2000 -%}{{ img_url | image_url: width: 2000 }} 2000w,{%- endif -%}
                            {%- if img_url.width >= 2800 -%}{{ img_url | image_url: width: 2800 }} 2800w{%- endif -%}"
                            {% unless settings.enable_lazyload and forloop.first == false %}src="{{ img_url | image_url: width: 750 }}"{% endunless %}
                            alt="{{ img_url.alt  | escape }}"
                            width="{{ img_url.width }}"
                            height="{{ img_url.width | divided_by: img_url.aspect_ratio | round }}"
                            {% if settings.enable_lazyload and forloop.first == false %}class="lazyload" loading="lazy"{% endif %}
                        >
                        {% if settings.enable_lazyload and forloop.first == false %}<span class="data-lazy-loading"></span>{% endif %}
                      </a>
                    </div>
                  {% else %}
                      <div class="not_img" style="--height_not_img: 790px">
                          1920 x 790px
                      </div>
                  {% endif %}
  
                  {% if block.settings.image_slide_mb != blank %}
                      {%- assign img_url_mb = block.settings.image_slide_mb -%}
                      <div class="slide-mobile">
                          <a {% if block.settings.image_banner_link == blank %}role="link" aria-disabled="true"{% else %}href="{{ block.settings.image_banner_link }}"{% endif %} title="{{ img_url_mb.alt | default: shop.name | escape }}" class="slide-image">
                              <img {% if settings.enable_lazyload and forloop.first == false %}data-{% endif %}srcset="{%- if img_url_mb.width < 320 -%}{{ img_url_mb | image_url: width: img_url_mb.width }} 320w,{%- endif -%}
                                  {%- if img_url_mb.width >= 320 -%}{{ img_url_mb | image_url: width: 375 }} 375w,{%- endif -%}
                                  {%- if img_url_mb.width >= 750 -%}{{ img_url_mb | image_url: width: 750 }} 750w,{%- endif -%}
                                  {%- if img_url_mb.width >= 1100 -%}{{ img_url_mb | image_url: width: 1100 }} 1100w,{%- endif -%}
                                  {%- if img_url_mb.width >= 1370 -%}{{ img_url_mb | image_url: width: 1370 }} 1370w,{%- endif -%}
                                  {%- if img_url_mb.width >= 1500 -%}{{ img_url_mb | image_url: width: 1500 }} 1500w,{%- endif -%}
                                  {%- if img_url_mb.width >= 1780 -%}{{ img_url_mb | image_url: width: 1780 }} 1780w,{%- endif -%}
                                  {%- if img_url_mb.width >= 2000 -%}{{ img_url_mb | image_url: width: 2000 }} 2000w,{%- endif -%}
                                  {%- if img_url_mb.width >= 2800 -%}{{ img_url_mb | image_url: width: 2800 }} 2800w{%- endif -%}"
                                  sizes="{% if section.settings.image != blank and section.settings.stack_images_on_mobile %}(min-width: 750px) 50vw, 100vw{% elsif section.settings.image != blank %}50vw{% else %}100vw{% endif %}"
                                  {% unless settings.enable_lazyload %}src="{{ img_url_mb | image_url: width: 750 }}"{% endunless %}
                                  alt="{{ img_url_mb.alt | escape }}"
                                  width="{{ img_url_mb.width }}"
                                  height="{{ img_url_mb.width | divided_by: img_url_mb.aspect_ratio | round}}"
                                  {% if settings.enable_lazyload and forloop.first == false %}class="lazyload" loading="lazy"{% endif %}
                              >
                              {% if settings.enable_lazyload and forloop.first == false %}<span class="data-lazy-loading"></span>{% endif %}
                          </a>
                      </div>
                  {% endif %}
                </div>
              {% elsif block.type == 'video' %}
                <div class="video-contain">
                    {% if block.settings.style_video == 'youtube' %}
                        <div data-video-youtube class="item slide-youtube embed-responsive item-video" id="slide-{{ block.id }}">
                            <div class="fluid-width-video-wrapper">
                                <iframe class="youtube-player" id="player_{{ block.id }}" width="100%" height="100%" data-video-id="{{ block.settings.video_link }}" src="https://www.youtube.com/embed/{{ block.settings.video_link }}?enablejsapi=1&loop=1&rel=0&autoplay={% if section.settings.auto_playvideo != blank %}1{% else %}0{% endif %}&mute=1" frameborder="0" allowfullscreen=""></iframe>
                            </div>
                        </div>
                        {% else %}
                        <div data-video-mp4 class="item slide-video item-video" id="slide-{{ block.id }}">
                            <div class="fluid-width-video-wrapper">
                                <video class="video" muted playsinline preload="none" autoplay loop>
                                    <source type="video/mp4" src="{{ block.settings.video_mp4_link }}">
                                </video>
                            </div>
                        </div>
                    {% endif %}
                </div>
              {% endif %}

              <div class="slide-show-content">
                <a {% if block.settings.image_banner_link == blank %} role="link" aria-disabled="true"{% else %} href="{{ block.settings.image_banner_link }}"{% endif %} class="banner-text-link"></a>
                <div class="container">
                  {%- assign img_url = block.settings.image_slide -%}
                  {%- assign position = block.settings.position -%}
                  {%- assign align = block.settings.align -%}
                  <div class="slideshow-content-outer content-box-{{ position }}">
                    <div class="slideshow-content-inner text-{{ align }}">
                      {% if block.settings.sub_title != blank %}
                        <h3 class="slideshow-sub-title wow animation-type-{{ block.settings.animations_type_subtitle }}"
                                  style="animation-delay: {{ block.settings.animation_delay_subtitle }};"
                          >
                          {{ block.settings.sub_title }}
                        </h3>
                      {% endif %}
                      {% if block.settings.heading != blank %}
                        <h2 class="slideshow-title wow animation-type-{{ block.settings.animations_type_title }}"
                                  style="animation-delay: {{ block.settings.animation_delay_title }};"
                          >
                          {{ block.settings.heading }}
                        </h2>
                      {% endif %}
                      {% if block.settings.description_text != blank %}
                        <div class="slideshow-description wow animation-type-{{ block.settings.animations_type_description }}"
                                    style="animation-delay: {{ block.settings.animation_delay_description }};"
                          >
                          {{ block.settings.description_text }}
                        </div>
                      {% endif %}
                    <div class="slide-show-banner-group">
                      {% if block.settings.image_banner_button != blank %}
                        {% if block.settings.button_style == '1'%}
                          <div class="slideshow-banner-button wow animation-type-{{ block.settings.animations_type_button }}"
                                        style="animation-delay: {{ block.settings.animation_delay_button }};"  
                          >
                            <a {% if block.settings.image_banner_link == blank %} role="link" aria-disabled="true"{% else %} href="{{ block.settings.image_banner_link }}"{% endif %} class="button">
                              {{ block.settings.image_banner_button }}
                            </a>
                          </div>
                        {% else %}
                          <div class="slideshow-banner-button wow animation-type-{{ block.settings.animations_type_button }}"
                                        style="animation-delay: {{ block.settings.animation_delay_button }};"
                          >
                            <a {% if block.settings.image_banner_link == blank %} role="link" aria-disabled="true"{% else %} href="{{ block.settings.image_banner_link }}"{% endif %} class="button">
                              {{ block.settings.image_banner_button }} {% render 'arrow-right-2' %}
                            </a>
                          </div>
                        {% endif %}
                      {% endif %}
                 {%-liquid assign link = block.settings.video_link_button
                    assign adaptive_height_video_button = block.settings.adaptive_height_video_button
                 -%}
                  {% if block.settings.video_heading != blank %}
                    <div class="video-button wow animation-type-{{ block.settings.animations_type_video_button }}" data-video-link="{{ link }}" 
                          style="animation-delay: {{ block.settings.animation_delay_video_button }};" 
                        >
                      {%- if block.settings.icon_view_all != blank -%}
                            <button type="button" class="icon-svg"  data-type="{{ link.type }}" data-src="{{ link.id }}" aspect_ratio="{{ adaptive_height_video_button| default: 56 | append: '%' }}">
                                {{ block.settings.icon_view_all }}
                                  
                            </button>
                       {% endif %}
                      {% if block.settings.video_heading != blank %}
                           <button type="button" class="video-heading"  data-type="{{ link.type }}" data-src="{{ link.id }}" aspect_ratio="{{ adaptive_height_video_button| default: 56 | append: '%' }}">
                               {{ block.settings.video_heading }}
                           </button>
                       {%- endif -%}
                    </div> 
                  {%- endif -%}
                </div>
            
                     </div>
            
                  </div>
                </div>
              </div>
              <style>
                {% if settings.type_font_1 == 'shopify_fonts' and settings.type_font_2 == 'shopify_fonts' %}
                {% assign base_font = settings.type_font_1_font | font_modify: 'weight', '400' %}
                {% assign base_font_medium = settings.type_font_1_font | font_modify: 'weight', '500' %}
                {% assign base_font_sem_bold = settings.type_font_1_font | font_modify: 'weight', '600' %}
                {% assign base_font_bold = settings.type_font_1_font | font_modify: 'weight', 'bold' %}
            
                {{ base_font | font_face | replace: '@font-face {', '@font-face { font-display: swap;' }}
                {{ base_font_medium | font_face | replace: '@font-face {', '@font-face { font-display: swap;' }}
                {{ base_font_sem_bold | font_face | replace: '@font-face {', '@font-face { font-display: swap;' }}
                {{ base_font_bold | font_face | replace: '@font-face {', '@font-face { font-display: swap;' }}
            
                {% assign fonts_name = base_font.family | append: ',' | append: base_font.fallback_families %}
            
                {% if settings.type_font_2_font.family != settings.type_font_1_font.family %}
                    {% assign header_font = settings.type_font_2_font | font_modify: 'weight', '400' %}
                    {% assign header_font_bold = settings.type_font_2_font | font_modify: 'weight', 'bold' %}
                    {% assign header_font_sem_bold = settings.type_font_2_font | font_modify: 'weight', '600' %}
                    {% assign header_font_medium = settings.type_font_2_font | font_modify: 'weight', '500' %}
                    
                    {{ header_font | font_face | replace: '@font-face {', '@font-face { font-display: swap;' }}
                    {{ header_font_medium | font_face | replace: '@font-face {', '@font-face { font-display: swap;' }}
                    {{ header_font_sem_bold | font_face | replace: '@font-face {', '@font-face { font-display: swap;' }}
                    {{ header_font_bold | font_face | replace: '@font-face {', '@font-face { font-display: swap;' }}
            
                          {% assign fonts_name_2 = header_font.family | append: ',' | append: header_font.fallback_families %}
                {% else %}
                          {% assign fonts_name_2 = base_font.family | append: ',' | append: base_font.fallback_families %}
                {% endif %}
            {% elsif settings.type_font_1 != 'shopify_fonts' and settings.type_font_2 != 'shopify_fonts' %}
                  {%- assign font_1 = settings.type_font_1_name -%}
                  {%- capture font_name_1 -%}
                      {{font_1 | strip | replace: ' ', '+'}}:300,300i,400,400i,500,500i,600,600i,700,700i,800,800i
                  {%- endcapture -%}
                  @import url('https://fonts.googleapis.com/css?family={{font_name_1}}&display=swap');
                        {% assign fonts_name = font_1 %}
            
                  {% if settings.type_font_1_name != settings.type_font_2_name %}
                      {%- assign font_2 = settings.type_font_2_name -%}
                      {%- capture fonts_name_2 -%}
                          {{font_2 | strip | replace: ' ', '+'}}:300,300i,400,400i,500,500i,600,600i,700,700i,800,800i
                      {%- endcapture -%}
                      @import url('https://fonts.googleapis.com/css?family={{fonts_name_2}}&display=swap');
                            {% assign fonts_name_2 = font_2 %}
                  {% else %}
                            {% assign fonts_name_2 = font_1 %}
                  {% endif %}
            {% elsif settings.type_font_1 == 'shopify_fonts' %}
                  {% assign base_font = settings.type_font_1_font | font_modify: 'weight', '400' %}
                  {% assign base_font_medium = settings.type_font_1_font | font_modify: 'weight', '500' %}
                  {% assign base_font_sem_bold = settings.type_font_1_font | font_modify: 'weight', '600' %}
                  {% assign base_font_bold = settings.type_font_1_font | font_modify: 'weight', 'bold' %}
            
                  {{ base_font | font_face | replace: '@font-face {', '@font-face { font-display: swap;' }}
                  {{ base_font_medium | font_face | replace: '@font-face {', '@font-face { font-display: swap;' }}
                  {{ base_font_sem_bold | font_face | replace: '@font-face {', '@font-face { font-display: swap;' }}
                  {{ base_font_bold | font_face | replace: '@font-face {', '@font-face { font-display: swap;' }}
            
                  {% assign fonts_name = base_font.family | append: ',' | append: base_font.fallback_families %}
                
                  {% if base_font.family != settings.type_font_2_name %}
                      {%- assign font_2 = settings.type_font_2_name -%}
                      {%- capture fonts_name_2 -%}
                          {{font_2 | strip | replace: ' ', '+'}}:300,300i,400,400i,500,500i,600,600i,700,700i,800,800i
                      {%- endcapture -%}
                      @import url('https://fonts.googleapis.com/css?family={{fonts_name_2}}&display=swap');
                              {% assign fonts_name_2 = font_2 %}
                  {% else %}
                            {% assign fonts_name_2 = base_font.family | append: ',' | append: base_font.fallback_families %}
                  {% endif %}
            {% else %}
                  {% assign header_font = settings.type_font_2_font | font_modify: 'weight', '400' %}
                  {% assign header_font_bold = settings.type_font_2_font | font_modify: 'weight', 'bold' %}
                  {% assign header_font_sem_bold = settings.type_font_2_font | font_modify: 'weight', '600' %}
                  {% assign header_font_medium = settings.type_font_2_font | font_modify: 'weight', '500' %}
            
                  {% if header_font.family != settings.type_font_1_name %}
                      {%- assign font_1 = settings.type_font_1_name -%}
                      {%- capture font_name_1 -%}
                          {{font_1 | strip | replace: ' ', '+'}}:300,300i,400,400i,500,500i,600,600i,700,700i,800,800i
                      {%- endcapture -%}
                      @import url('https://fonts.googleapis.com/css?family={{font_name_1}}&display=swap');
                            {% assign fonts_name = font_1 %}
                  {% else %}
                            {% assign fonts_name = header_font.family | append: ',' | append: header_font.fallback_families %}
                  {% endif %}
            
                  {{ header_font | font_face | replace: '@font-face {', '@font-face { font-display: swap;' }}
                  {{ header_font_medium | font_face | replace: '@font-face {', '@font-face { font-display: swap;' }}
                  {{ header_font_sem_bold | font_face | replace: '@font-face {', '@font-face { font-display: swap;' }}
                  {{ header_font_bold | font_face | replace: '@font-face {', '@font-face { font-display: swap;' }}
                       
                  {% assign fonts_name_2 = header_font.family | append: ',' | append: header_font.fallback_families %}
            {% endif %}
            :root {
                {% if block.settings.sub_heading_font == 'font_1' %}
                    --sub-title-font-family: {{fonts_name}};
                {% else %}
                    --sub-title-font-family: {{fonts_name_2}};
                {% endif %}
                
            }
                .block-{{block.id}} .slideshow-content-inner {
                    max-width: {{ block.settings.content_width | append: 'px' }};
                    width: 100%;
                }
                .block-{{block.id}} .slideshow-sub-title {
                    font-size: {{ block.settings.font_size_sub_title | append: 'px' }};
                    line-height: calc({{ block.settings.font_size_sub_title | append: 'px' }} + 4px);
                    color: {{ block.settings.sub_title_color }};
                    {% if block.settings.sub_heading_font == 'font_1' %}
                        font-family: var(--font-family-mul-1);
                    {% else %}
                        font-family: var(--font-family-mul-2);
                    {% endif %}
                    font-weight: {{ block.settings.sub_title_font_weight }};
                    {% if block.settings.sub_title_color != blank %}
                         color: {{ block.settings.sub_title_color }};
                    {% else %}
                         color: var(--heading-text-color);
                    {% endif %}
                    padding-bottom: {{ block.settings.sub_title__space_top | append: 'px' }};
                    letter-spacing: {{ block.settings.font_size_sub_letter_spacing | append: 'px' }};
                    text-transform: {{ block.settings.sub_title_text_transform }};
                }
                .block-{{block.id}} .slideshow-title {
                    font-size: {{ block.settings.heading_title | append: 'px' }};
                    line-height: {{ block.settings.heading_title_line_height | append: 'px' }};
                    font-weight: {{ block.settings.heading_font_weight }};
                    {% if block.settings.heading_color != blank %}
                         color: {{ block.settings.heading_color }};
                    {% else %}
                         color: var(--heading-text-color);
                    {% endif %}
                    
                    padding-bottom: {{ block.settings.heading__space_top | append: 'px' }};
                    letter-spacing: {{ block.settings.heading_letter_spacing | append: 'px' }};
                    text-transform: {{ block.settings.title_text_transform }};
                }
                .block-{{block.id}} .slideshow-content-outer{
                    padding-top: {{ block.settings.heading__space_top_title | append: 'px' }};
                    padding-bottom: {{ block.settings.heading__space_bottom_title | append: 'px' }};
                } 
                
                .block-{{block.id}}.slick-active .slideshow-sub-title.animation-type-fadeInUp {
                    -webkit-animation-duration: 1.5s; animation-duration: 1.5s; -webkit-animation-fill-mode: both; animation-fill-mode: both; animation-name: fadeInUp;
                }
                .block-{{block.id}}.slick-active .slideshow-sub-title.animation-type-fadeInDown {
                    -webkit-animation-duration: 1.5s; animation-duration: 1.5s; -webkit-animation-fill-mode: both; animation-fill-mode: both; animation-name: fadeInDown;
                }
                .block-{{block.id}}.slick-active .slideshow-sub-title.animation-type-fadeInLeft {
                    -webkit-animation-duration: 1.5s; animation-duration: 1.5s; -webkit-animation-fill-mode: both; animation-fill-mode: both; animation-name: fadeInLeft;
                }
                .block-{{block.id}}.slick-active .slideshow-sub-title.animation-type-fadeInRight {
                    -webkit-animation-duration: 1.5s; animation-duration: 1.5s; -webkit-animation-fill-mode: both; animation-fill-mode: both; animation-name: fadeInRight;
                }
            
                .block-{{block.id}}.slick-active .slideshow-title.animation-type-fadeInUp {
                    -webkit-animation-duration: 1.5s; animation-duration: 1.5s; -webkit-animation-fill-mode: both; animation-fill-mode: both; animation-name: fadeInUp;
                }
                .block-{{block.id}}.slick-active .slideshow-title.animation-type-fadeInDown {
                    -webkit-animation-duration: 1.5s; animation-duration: 1.5s; -webkit-animation-fill-mode: both; animation-fill-mode: both; animation-name: fadeInDown;
                }
                .block-{{block.id}}.slick-active .slideshow-title.animation-type-fadeInLeft {
                    -webkit-animation-duration: 1.5s; animation-duration: 1.5s; -webkit-animation-fill-mode: both; animation-fill-mode: both; animation-name: fadeInLeft;
                }
                .block-{{block.id}}.slick-active .slideshow-title.animation-type-fadeInRight {
                    -webkit-animation-duration: 1.5s; animation-duration: 1.5s; -webkit-animation-fill-mode: both; animation-fill-mode: both; animation-name: fadeInRight;
                }

                .block-{{block.id}}.slick-active .slideshow-description.animation-type-fadeInUp {
                    -webkit-animation-duration: 1.5s; animation-duration: 1.5s; -webkit-animation-fill-mode: both; animation-fill-mode: both; animation-name: fadeInUp;
                }
                .block-{{block.id}}.slick-active .slideshow-description.animation-type-fadeInDown {
                    -webkit-animation-duration: 1.5s; animation-duration: 1.5s; -webkit-animation-fill-mode: both; animation-fill-mode: both; animation-name: fadeInDown;
                }
                .block-{{block.id}}.slick-active .slideshow-description.animation-type-fadeInLeft {
                    -webkit-animation-duration: 1.5s; animation-duration: 1.5s; -webkit-animation-fill-mode: both; animation-fill-mode: both; animation-name: fadeInLeft;
                }
                .block-{{block.id}}.slick-active .slideshow-description.animation-type-fadeInRight {
                    -webkit-animation-duration: 1.5s; animation-duration: 1.5s; -webkit-animation-fill-mode: both; animation-fill-mode: both; animation-name: fadeInRight;
                }

                .block-{{block.id}}.slick-active .slideshow-banner-button.animation-type-fadeInUp {
                    -webkit-animation-duration: 1.5s; animation-duration: 1.5s; -webkit-animation-fill-mode: both; animation-fill-mode: both; animation-name: fadeInUp;
                }
                .block-{{block.id}}.slick-active .slideshow-banner-button.animation-type-fadeInDown {
                    -webkit-animation-duration: 1.5s; animation-duration: 1.5s; -webkit-animation-fill-mode: both; animation-fill-mode: both; animation-name: fadeInDown;
                }
                .block-{{block.id}}.slick-active .slideshow-banner-button.animation-type-fadeInLeft {
                    -webkit-animation-duration: 1.5s; animation-duration: 1.5s; -webkit-animation-fill-mode: both; animation-fill-mode: both; animation-name: fadeInLeft;
                }
                .block-{{block.id}}.slick-active .slideshow-banner-button.animation-type-fadeInRight {
                    -webkit-animation-duration: 1.5s; animation-duration: 1.5s; -webkit-animation-fill-mode: both; animation-fill-mode: both; animation-name: fadeInRight;
                }

                .block-{{block.id}}.slick-active .video-button.animation-type-fadeInUp {
                    -webkit-animation-duration: 1.5s; animation-duration: 1.5s; -webkit-animation-fill-mode: both; animation-fill-mode: both; animation-name: fadeInUp;
                }
                .block-{{block.id}}.slick-active .video-button.animation-type-fadeInDown {
