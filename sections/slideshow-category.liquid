<script>
  $(document).ready(function(){
    $('.slide-show .slick-dots li').wrapAll('<div class="container"></div>');
    $(".slideshow-prev-icon").appendTo(".slideshow-slider .slick-arrow.slick-prev");
    $(".slideshow-next-icon").appendTo(".slideshow-slider .slick-arrow.slick-next");
  });
  if ($(".slideshow-slider").length > 0) {
      $(".slideshow-slider").slick({
        slidesToShow: 1,
        slidesToScroll: 1,
        speed: 800,
        dots: {{ dots }},
        arrows: {{ arrows }},
        rtl: {{ rtl }},
        fade: {{ slideshow_transition }},
        autoplay: {{ autoplay }},
        pauseOnHover: false,
        autoplaySpeed: {{ slideshow_speed }},
        responsive: [
          {
            breakpoint: 1024,
            settings: {
              dots: {{dots_mb}},
              arrows: {{ arrows_mb }}
            }
          }
        ]
      });
  }
</script>

