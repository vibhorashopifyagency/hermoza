{%- liquid
    assign container = section.settings.container
    assign background_color = section.settings.background_color
    assign background_gradient = section.settings.background_gradient
-%}

<style>
  .image-with-content.image-content-block-{{section.id}} {
    background: {% if background_gradient != blank %}{{ background_gradient }}{% else %}{{ background_color }}{% endif %};
    padding-top: {{ section.settings.mg_top_desktop | append: 'px' }};
    padding-bottom: {{ section.settings.mg_bottom_desktop | append: 'px' }};
    position: relative;
  }
  .image-with-content.image-content-block-{{section.id}} .image {
      display: block;
      height: 100%;
      width: 100%;
  }
  .image-with-content.image-content-block-{{section.id}} .image img {
      display: block;
      width: 100%;
      height: 100%;
      object-fit: cover;
  }
  .image-with-content.image-content-block-{{section.id}} .row > * {
      display: flex;
      flex-wrap: wrap;
  }
  body.video-show .scoder-video-popup {
      opacity: 1;
      visibility: visible;
      pointer-events: auto;
      transform: translate3d(-50%,-50%,0);
  }
  .image-with-content.image-content-block-{{section.id}} .img-image img {
      height: 100%;
      object-fit: cover;
      width:100%;
  }
  

  .image-content-block-{{section.id}} .vector_1 {
      position: absolute;
      {% if section.settings.vector_top != blank %}
        top: {{ section.settings.vector_top }};
      {% endif %}
      {% if section.settings.vector_bottom != blank %}
        bottom: {{ section.settings.vector_bottom }};
      {% endif %}
      {% if section.settings.vector_left != blank %}
        left: {{ section.settings.vector_left }};
      {% endif %}
      {% if section.settings.vector_right != blank %}
        right: {{ section.settings.vector_right }};
      {% endif %}
  }
  .image-content-block-{{section.id}} .vector_2 {
      position: absolute;
      {% if section.settings.vector_top_2 != blank %}
        top: {{ section.settings.vector_top_2 }};
      {% endif %}
      {% if section.settings.vector_bottom_2 != blank %}
        bottom: {{ section.settings.vector_bottom_2 }};
      {% endif %}
      {% if section.settings.vector_left_2 != blank %}
        left: {{ section.settings.vector_left_2 }};
      {% endif %}
      {% if section.settings.vector_right_2 != blank %}
        right: {{ section.settings.vector_right_2 }};
      {% endif %}
  }

  @media (max-width: 1199px) {
    .image-with-content.image-content-block-{{section.id}} {
      padding-top: {{ section.settings.mg_top_tablet | append: 'px' }};
      padding-bottom: {{ section.settings.mg_bottom_tablet | append: 'px' }};
    }  
    .image-with-content.image-content-block-{{section.id}} .image-bottom{
        margin-bottom: 30px;
    }
    .image-with-content.image-content-block-{{section.id}} .center-align{
        justify-content: center;
    }
  }
  @media (max-width: 1024px) {
    .image-with-content.image-content-block-{{section.id}} .flex-align{
      justify-content: center;
    }

    .image-content-block-{{section.id}} .vector_1 {
        {% if section.settings.vector_tablet_top != blank %}
          top: {{ section.settings.vector_tablet_top }};
        {% endif %}
        {% if section.settings.vector_tablet_bottom != blank %}
          bottom: {{ section.settings.vector_tablet_bottom }};
        {% endif %}
        {% if section.settings.vector_tablet_left != blank %}
          left: {{ section.settings.vector_tablet_left }};
        {% endif %}
        {% if section.settings.vector_tablet_right != blank %}
          right: {{ section.settings.vector_tablet_right }};
        {% endif %}
    }
    .image-content-block-{{section.id}} .vector_2 {
        {% if section.settings.vector_tablet_top_2 != blank %}
          top: {{ section.settings.vector_tablet_top_2 }};
        {% endif %}
        {% if section.settings.vector_tablet_bottom_2 != blank %}
          bottom: {{ section.settings.vector_tablet_bottom_2 }};
        {% endif %}
        {% if section.settings.vector_tablet_left_2 != blank %}
          left: {{ section.settings.vector_tablet_left_2 }};
        {% endif %}
        {% if section.settings.vector_tablet_right_2 != blank %}
          right: {{ section.settings.vector_tablet_right_2 }};
        {% endif %}
    }

  }
  @media (max-width: 767px) {
    .image-with-content.image-content-block-{{section.id}} {
      padding-top: {{ section.settings.mg_top_mobile | append: 'px' }};
      padding-bottom: {{ section.settings.mg_bottom_mobile | append: 'px' }};
    }
    .image-content-block-{{section.id}} .vector_1 {
        {% if section.settings.vector_mobile_top != blank %}
          top: {{ section.settings.vector_mobile_top }};
        {% endif %}
        {% if section.settings.vector_mobile_bottom != blank %}
          bottom: {{ section.settings.vector_mobile_bottom }};
        {% endif %}
        {% if section.settings.vector_mobile_left != blank %}
          left: {{ section.settings.vector_mobile_left }};
        {% endif %}
        {% if section.settings.vector_mobile_right != blank %}
          right: {{ section.settings.vector_mobile_right }};
        {% endif %}
    }
    .image-content-block-{{section.id}} .vector_2 {
        {% if section.settings.vector_mobile_top_2 != blank %}
          top: {{ section.settings.vector_mobile_top_2 }};
        {% endif %}
        {% if section.settings.vector_mobile_bottom_2 != blank %}
          bottom: {{ section.settings.vector_mobile_bottom_2 }};
        {% endif %}
        {% if section.settings.vector_mobile_left_2 != blank %}
          left: {{ section.settings.vector_mobile_left_2 }};
        {% endif %}
        {% if section.settings.vector_mobile_right_2 != blank %}
          right: {{ section.settings.vector_mobile_right_2 }};
        {% endif %}
    }
  }
</style>

<div class="image-with-content image-content-block-{{section.id}}">
  <div class="{% if container == 'fullwidth'%}container-full{% else %}container{% endif %}">
    {%- if section.settings.vector_image_1 != blank -%}
    <div class="vector_1 vector_1_{{section.id}}">
      {%- liquid
        assign vector_img_1 = section.settings.vector_image_1
      -%}
      <div class="vector_img_1-an {% if section.settings.vectore_1_animations_type != 'none'%}wow{% endif %} {{ section.settings.vectore_1_animations_type }} delay-{{ section.settings.vectore_1_animation_delay | replace: ".", "_" }}">
        <img {% render 'lazyload', preview_img: vector_img_1 %} loading="lazy"/>
        {% if settings.enable_lazyload %}<span class="data-lazy-loading"></span>{% endif %}
      </div>
      <style>
        .image-content-block-{{section.id}} .vector_img_1-an.delay-{{ section.settings.vectore_1_animation_delay | replace: ".", "_" }} {
            animation-delay: {{ section.settings.vectore_1_animation_delay }}
        }
      </style>
    </div>
  {%- endif -%}

  {%- if section.settings.vector_image_2 != blank -%}
    <div class="vector_2 vector_2_{{section.id}}">
      {%- liquid
        assign vector_img_2 = section.settings.vector_image_2
      -%}
      <div class="vector_img_2-an {% if section.settings.vectore_2_animations_type != 'none'%}wow{% endif %} {{ section.settings.vectore_2_animations_type }} delay-{{ section.settings.vectore_2_animation_delay | replace: ".", "_" }}">
        <img {% render 'lazyload', preview_img: vector_img_2 %} loading="lazy"/>
        {% if settings.enable_lazyload %}<span class="data-lazy-loading"></span>{% endif %}  
      </div>
      <style>
        .image-content-block-{{section.id}} .vector_img_2-an.delay-{{ section.settings.vectore_2_animation_delay | replace: ".", "_" }} {
            animation-delay: {{ section.settings.vectore_2_animation_delay }}
        }
      </style>
    </div>
  {%- endif -%}
  
  <div class="{% if container == 'fullwidth'%}container-full{% else %}container{% endif %}">
    {%- for block in section.blocks -%}
      {%- liquid
        assign preview_img = block.settings.image
        assign preview_img_1 = block.settings.image_center
        assign preview_img_2 = block.settings.image_right
         
      -%}
      <style>
        .image-with-content.image-content-block-{{section.id}} .banner-heading {
            font-size: {{ block.settings.heading_title | append: 'px' }};
            padding-bottom: {{ block.settings.heading__space_bottom | append: 'px' }};
            font-weight: {{ block.settings.block_title_font_weight }};
            {% if block.settings.heading_text_color != blank %}
                color: {{ block.settings.heading_text_color }};
            {% else %}
                color: var(--heading-text-color);
            {% endif %}
        }
        .image-with-content.image-content-block-{{section.id}} .row.align-center > * {
            align-items: center;
        }
        .image-with-content.image-content-block-{{section.id}} .row.align-bottom > * {
            align-items: flex-end;
        }
        .image-with-content.image-content-block-{{section.id}} .banner-content {
            max-width: {{ block.settings.content_width | append: 'px' }};
            width: 100%;
        }
        .image-with-content.image-content-block-{{section.id}} .img-image .image-overlay a{
            border-radius: {{ block.settings.image_border_radius | append: '%' }}; 
            display: block;
            overflow: hidden;
        }
        .image-with-content.image-content-block-{{section.id}} .img-image .image-overlay a img {
            display: block;
        }
        .image-with-content.image-content-block-{{section.id}} .image-overlay{
             position: relative; 
             width:100%;
        }
        {%- if block.settings.enable_overlay != blank -%}
          .image-with-content.image-content-block-{{section.id}} .image-overlay a:before {
            content: "";
            position: absolute;
            top: 0;
            left: 0;
            width: 100%;
            height: 100%;
            background: {{ block.settings.overlay_color }};
            opacity: 0.{{ block.settings.overlay__opacity }};
            z-index: 1;
          }
        {%- endif -%}
        .image-with-content.image-content-block-{{section.id}} .row_reverse > *:first-child {
            order: 2;
        }
        .image-with-content.image-content-block-{{section.id}} .row_reverse > *:last-child {
            order: 1;
        }
        .image-with-content.image-content-block-{{section.id}} .description {
            padding-bottom: {{ block.settings.description__space_bottom | append: 'px' }};
            font-size: {{ block.settings.desc_title | append: 'px' }};
            {% if block.settings.desc_color != blank %}
                color: {{ block.settings.desc_color }};
            {% else %}
               color: var(--body-text-color);
            {% endif %}
            font-weight: {{ block.settings.desc_font_weight }};
        }
        .image-content-block-{{section.id}} .button {
            letter-spacing:{{ block.settings.button_letter_spacing | append: 'px' }};
            border: {{ block.settings.button_border | append: 'px' }} solid {{ block.settings.button_border_color }};
            background: {{ block.settings.button_bg_color }};
            color: {{ block.settings.button_color }};
        }
        .image-content-block-{{section.id}} .button svg {
            fill: {{ block.settings.button_color }};
            transition: all 400ms ease;
        }
        .image-content-block-{{section.id}} .button:hover {
            border: {{ block.settings.button_border | append: 'px' }} solid {{ block.settings.button_borderhover_color }};
            background: {{ block.settings.button_bghover_color }};
            color: {{ block.settings.button_hover_color }};
        }
        .image-content-block-{{section.id}} .button:hover svg {
            fill: {{ block.settings.button_hover_color }};
        }
        .image-with-content.image-content-block-{{section.id}} .row > * {
            background: {{ block.settings.background_color }};
            position: relative;
        }
        {%- if block.settings.background_color != blank -%}
          .image-with-content.image-content-block-{{section.id}} .img-image{
              padding-left: 50px;
              border-top-left-radius: {{ section.settings.image_border_radius | append: 'px'  }};
              border-bottom-left-radius:{{ section.settings.image_border_radius | append: 'px'  }}; 
          }
          .image-with-content.image-content-block-{{section.id}} .content-im{
            border-top-right-radius: {{ section.settings.image_border_radius | append: 'px'  }};
            border-bottom-right-radius: {{ section.settings.image_border_radius | append: 'px'  }};
          }
        {%- endif -%}
        {%- if block.settings.background_color != blank -%}
          .image-with-content.image-content-block-{{section.id}} .row {
            margin-left: -15px;
            margin-right: -15px;
          }
        {%- endif -%}
        .image-with-content.image-content-block-{{section.id}}  .animation-text {
            font-size: {{ block.settings.animation_font_size | append: 'vw' }};
            font-weight: {{ block.settings.animation_text_font_weight }};
            display: block;
            line-height: 0.7;
            color: {{ block.settings.animation_text_color }};
            -webkit-text-stroke: {{ block.settings.animation_text_border_width | append: 'px' }} {{ block.settings.animation_text_border_color }};
            opacity: 0.{{ block.settings.overlay_opacity }};
            position: absolute;
            bottom: {{ block.settings.button_position_bottom }};
            left: calc(50% - 6px);
            transform: translate(-50%);
            white-space: nowrap;
            letter-spacing: {{ block.settings.animation_text_letter_spacing | append: 'px' }};
        }
        .image-with-content.image-content-block-{{section.id}} .content-on-image{
              position: absolute;
              z-index: 1;
              transition: all 400ms ease;
              width: max-content;
              top: 80%;
        }
        .image-with-content.image-content-block-{{section.id}} .image-overlay:hover .content-on-image{
            top: 50% !important;
            left: 50% !important;
            bottom: unset !important;
            transform: translate(-50%, -50%) !important;
        }
        .image-with-content.image-content-block-{{section.id}} .content-on-image.justify-left.align-top {
            left: {{ block.settings.button_position_left_right }};
            top: {{ block.settings.button_position_top }};
        }
        .image-with-content.image-content-block-{{section.id}} .content-on-image.justify-left.align-center {
            left: {{ block.settings.button_position_left_right }};
            top: 50%;
            transform: translateY(-50%);
        }
        .image-with-content.image-content-block-{{section.id}} .content-on-image.justify-left.align-bottom {
            left: {{ block.settings.button_position_left_right }};
            bottom: {{ block.settings.button_position_top }};
        }
        .image-with-content.image-content-block-{{section.id}} .content-on-image.justify-center.align-top {
            left: 50%;
            top: {{ block.settings.button_position_top }};
            transform: translate(-50%);
        }
        .image-with-content.image-content-block-{{section.id}} .content-on-image.justify-center.align-center {
            left: 50%;
            top: 50%;
            transform: translate(-50%, -50%);
        }
        .image-with-content.image-content-block-{{section.id}} .content-on-image.justify-center.align-bottom {
            left: 50%;
            bottom: {{ block.settings.button_position_top }};
            transform: translate(-50%);
        }
        .image-with-content.image-content-block-{{section.id}} .content-on-image.justify-right.align-top {
            right: {{ block.settings.button_position_left_right }};
            top: {{ block.settings.button_position_top }};
        }
        .image-with-content.image-content-block-{{section.id}} .content-on-image.justify-right.align-center {
            right: {{ block.settings.button_position_left_right }};
            top: 50%;
            transform: translateY(-50%);
        }
        .image-with-content.image-content-block-{{section.id}} .content-on-image.justify-right.align-bottom {
            right: {{ block.settings.button_position_left_right }};
            bottom: {{ block.settings.button_position_top }};
        }
        .image-with-content.image-content-block-{{section.id}} .content-title {
            font-size: {{ block.settings.image_heading_title | append: 'px' }};
            {% if block.settings.image_heading_color != blank %}
                color: {{ block.settings.image_heading_color }};
            {% else %}
                color: var(--heading-text-color);
            {% endif %}
            font-weight: {{ block.settings.image_heading_title_font_weight }};
            text-transform: {{ block.settings.image_heading_title_transform }};
            letter-spacing: {{ block.settings.image_heading_title_letter_spacing | append: 'px' }};
            display: inline-block;
            font-family: 'Poppins';
        }
        .image-with-content.image-content-block-{{section.id}} .content-title{
             max-width: {{ block.settings.image_content_width | append: 'px' }};
        }
        .image-with-content.image-content-block-{{section.id}} .image-overlay img{
             transition: all 400ms ease;
         }
         .image-with-content.image-content-block-{{section.id}} .image-overlay:hover img{
             transform: scale(1.2);
         }
        @media (max-width:1024px) {
            .image-with-content.image-content-block-{{section.id}} .banner-heading {
                font-size: {{ block.settings.heading_title_tb | append: 'px' }};
                padding-bottom: {{ block.settings.heading__space_bottom_tb | append: 'px' }};
            }
            .image-with-content.image-content-block-{{section.id}} .description {
                padding-bottom: {{ block.settings.description__space_bottom_tb | append: 'px' }};
                font-size: {{ block.settings.desc_tb | append: 'px' }};
            }
            .image-with-content.image-content-block-{{section.id}}  .animation-text {
                font-size: {{ block.settings.animation_font_size_tb | append: 'vw' }};
            }
            .image-with-content.image-content-block-{{section.id}} .content-title {
                font-size: {{ block.settings.image_heading_title_tb | append: 'px' }};
            }
            .image-with-content.image-content-block-{{section.id}} .banner-content{
                 text-align: {{ block.settings.text_align_tb }};   
            }
            {%- if block.settings.background_color != blank -%}
              .image-with-content.image-content-block-{{section.id}} .img-image{
                  padding-left: 24px;
                  padding-top: 30px;
              }
            {%- endif -%}
          }

        @media (max-width:767px) {
          .image-with-content.image-content-block-{{section.id}} .banner-heading {
              font-size: {{ block.settings.heading_title_mb | append: 'px' }};
              padding-bottom: {{ block.settings.heading__space_bottom_mb | append: 'px' }};
          }
          .image-with-content.image-content-block-{{section.id}} .banner-content {
              max-width: 100%;
              padding: 50px 0px;
              padding-top: 20px;
          }
          .image-with-content.image-content-block-{{section.id}} .row_reverse > *:first-child {
              order: 1;
          }
          {%- if block.settings.background_color != blank -%}
            .image-with-content.image-content-block-{{section.id}} .img-image{
              padding-right: 20px;
              border-top-left-radius: {{ section.settings.image_border_radius | append: 'px'  }};
              border-top-right-radius: {{ section.settings.image_border_radius | append: 'px'  }};
              border-bottom-left-radius: 0px;
            }
            .image-with-content.image-content-block-{{section.id}} .content-im{
              border-top-right-radius: 0px;
              border-bottom-left-radius: {{ section.settings.image_border_radius | append: 'px'  }};
              border-bottom-right-radius: {{ section.settings.image_border_radius | append: 'px'  }};
              
            }
          {%- endif -%}
          
          .image-with-content.image-content-block-{{section.id}} .row_reverse > *:last-child {
              order: 2;
          }
          .image-with-content.image-content-block-{{section.id}} .description {
              padding-bottom: {{ block.settings.description__space_bottom_mb | append: 'px' }};
              font-size: {{ block.settings.desc_mb | append: 'px' }};
          }
          .image-with-content.image-content-block-{{section.id}}  .animation-text {
              font-size: {{ block.settings.animation_font_size_mb | append: 'vw' }};
              left: 50%;
          }
          .image-with-content.image-content-block-{{section.id}} .content-title {
              font-size: {{ block.settings.image_heading_title_mb | append: 'px' }};
          }
          .image-with-content.image-content-block-{{section.id}} .image-bottom{
              margin-bottom: 30px;
          }
        }
      </style>
      <div class="row align-{{ block.settings.align }} {%- if block.settings.row_reverse != blank -%}row_reverse{%- endif -%}">
        <div class="col-xl-{{ block.settings.image_grid }} col-lg-{{ block.settings.image_grid_tablate }} col-md-{{ block.settings.image_grid_mobile }} img-image {% if section.settings.animations_type_image != 'none'%}wow{% endif %} {{ block.settings.animations_type_image }} delay-{{ block.settings.animation_delay_image | replace: ".", "_" }} flex-align">
          <style>
            .image-with-content.image-content-block-{{section.id}} .img-image.delay-{{ block.settings.animation_delay_image | replace: ".", "_" }} {
                animation-delay: {{ block.settings.animation_delay_image }}
            }
          </style>
          <div class="row align-item-center center-align">
            <div class="col-xl-4 col-lg-6 col-md-4 image-bottom {% if block.settings.img_1_animations_type != 'none'%}wow{% endif %} {{ block.settings.img_1_animations_type }} delay-{{ block.settings.img_1_animation_delay | replace: ".", "_" }}">
              <style>
                .image-with-content.image-content-block-{{section.id}} .image-bottom.delay-{{ block.settings.img_1_animation_delay | replace: ".", "_" }} {
                    animation-delay: {{ block.settings.img_1_animation_delay }}
                }
              </style>
              <div class="image-overlay">
                <a {% if block.settings.title_name_link == blank %} role="link" aria-disabled="true"{% else %} href="{{ block.settings.title_name_link }}"{% endif %}>
                  {%- if block.settings.image != blank -%}
                      <img {% render 'lazyload', preview_img: preview_img %} loading="lazy"/>
                      {% if settings.enable_lazyload %}<span class="data-lazy-loading"></span>{% endif %}
                  {%- else -%}
                      <div class="not_img" style="--height_not_img: 250px">
                        300 x 300px
                      </div>  
                  {%- endif -%}
                  <div class="content-on-image justify-{{ block.settings.button_align }} align-{{ block.settings.align_horizontal }} text-{{ block.settings.button_align }}">
                    {% if block.settings.title_name != blank %}
                        <h6 class="content-title">
                          {{ block.settings.title_name }}
                        </h6>
                    {% endif %}
                  </div>
                </a>
              </div>
            </div>
            <div class="col-xl-4 col-lg-6 col-md-4 image-bottom {% if block.settings.img_2_animations_type != 'none'%}wow{% endif %} {{ block.settings.img_2_animations_type }} delay-{{ block.settings.img_2_animation_delay | replace: ".", "_" }}">
              <style>
                .image-with-content.image-content-block-{{section.id}} .image-bottom.delay-{{ block.settings.img_2_animation_delay | replace: ".", "_" }} {
                    animation-delay: {{ block.settings.img_2_animation_delay }}
                }
              </style>
              <div class="image-overlay">
                <a {% if block.settings.title__name_link == blank %} role="link" aria-disabled="true"{% else %} href="{{ block.settings.title__name_link }}"{% endif %}>
                  {%- if block.settings.image_center != blank -%}
                      <img {% render 'lazyload', preview_img: preview_img_1 %} loading="lazy"/>
                      {% if settings.enable_lazyload %}<span class="data-lazy-loading"></span>{% endif %}
                  {%- else -%}
                      <div class="not_img" style="--height_not_img: 250px">
                        300 x 300px
                      </div>  
                  {%- endif -%}
                  <div class="content-on-image justify-{{ block.settings.button_align }} align-{{ block.settings.align_horizontal }} text-{{ block.settings.button_align }}">
                    {% if block.settings.title__name != blank %}
                        <h6 class="content-title">
                          {{ block.settings.title__name }}
                        </h6>
                    {% endif %}
                  </div>
                </a>
              </div>
            </div>
            <div class="col-xl-4 col-lg-6 col-md-4 image-bottom {% if block.settings.img_3_animations_type != 'none'%}wow{% endif %} {{ block.settings.img_3_animations_type }} delay-{{ block.settings.img_3_animation_delay | replace: ".", "_" }}">
              <style>
                .image-with-content.image-content-block-{{section.id}} .image-bottom.delay-{{ block.settings.img_3_animation_delay | replace: ".", "_" }} {
