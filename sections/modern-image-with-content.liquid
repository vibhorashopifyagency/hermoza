{%- liquid
  assign container = section.settings.container
  assign background_color = section.settings.background_color
  assign background_gradient = section.settings.background_gradient
  assign blog_bg_image = section.settings.background_image
-%}
<style>
  {% if settings.type_font_1 == 'shopify_fonts' and settings.type_font_2 == 'shopify_fonts' %}
      {% assign base_font = settings.type_font_1_font | font_modify: 'weight', '400' %}
      {% assign base_font_medium = settings.type_font_1_font | font_modify: 'weight', '500' %}
      {% assign base_font_sem_bold = settings.type_font_1_font | font_modify: 'weight', '600' %}
      {% assign base_font_bold = settings.type_font_1_font | font_modify: 'weight', 'bold' %}

      {{ base_font | font_face | replace: '@font-face {', '@font-face { font-display: swap;' }}
      {{ base_font_medium | font_face | replace: '@font-face {', '@font-face { font-display: swap;' }}
      {{ base_font_sem_bold | font_face | replace: '@font-face {', '@font-face { font-display: swap;' }}
      {{ base_font_bold | font_face | replace: '@font-face {', '@font-face { font-display: swap;' }}

      {% assign fonts_name = base_font.family | append: ',' | append: base_font.fallback_families %}

      {% if settings.type_font_2_font.family != settings.type_font_1_font.family %}
          {% assign header_font = settings.type_font_2_font | font_modify: 'weight', '400' %}
          {% assign header_font_bold = settings.type_font_2_font | font_modify: 'weight', 'bold' %}
          {% assign header_font_sem_bold = settings.type_font_2_font | font_modify: 'weight', '600' %}
          {% assign header_font_medium = settings.type_font_2_font | font_modify: 'weight', '500' %}
          
          {{ header_font | font_face | replace: '@font-face {', '@font-face { font-display: swap;' }}
          {{ header_font_medium | font_face | replace: '@font-face {', '@font-face { font-display: swap;' }}
          {{ header_font_sem_bold | font_face | replace: '@font-face {', '@font-face { font-display: swap;' }}
          {{ header_font_bold | font_face | replace: '@font-face {', '@font-face { font-display: swap;' }}

                {% assign fonts_name_2 = header_font.family | append: ',' | append: header_font.fallback_families %}
      {% else %}
                {% assign fonts_name_2 = base_font.family | append: ',' | append: base_font.fallback_families %}
      {% endif %}
  {% elsif settings.type_font_1 != 'shopify_fonts' and settings.type_font_2 != 'shopify_fonts' %}
        {%- assign font_1 = settings.type_font_1_name -%}
        {%- capture font_name_1 -%}
            {{font_1 | strip | replace: ' ', '+'}}:300,300i,400,400i,500,500i,600,600i,700,700i,800,800i
        {%- endcapture -%}
        @import url('https://fonts.googleapis.com/css?family={{font_name_1}}&display=swap');
              {% assign fonts_name = font_1 %}

        {% if settings.type_font_1_name != settings.type_font_2_name %}
            {%- assign font_2 = settings.type_font_2_name -%}
            {%- capture fonts_name_2 -%}
                {{font_2 | strip | replace: ' ', '+'}}:300,300i,400,400i,500,500i,600,600i,700,700i,800,800i
            {%- endcapture -%}
            @import url('https://fonts.googleapis.com/css?family={{fonts_name_2}}&display=swap');
                  {% assign fonts_name_2 = font_2 %}
        {% else %}
                  {% assign fonts_name_2 = font_1 %}
        {% endif %}
  {% elsif settings.type_font_1 == 'shopify_fonts' %}
        {% assign base_font = settings.type_font_1_font | font_modify: 'weight', '400' %}
        {% assign base_font_medium = settings.type_font_1_font | font_modify: 'weight', '500' %}
        {% assign base_font_sem_bold = settings.type_font_1_font | font_modify: 'weight', '600' %}
        {% assign base_font_bold = settings.type_font_1_font | font_modify: 'weight', 'bold' %}

        {{ base_font | font_face | replace: '@font-face {', '@font-face { font-display: swap;' }}
        {{ base_font_medium | font_face | replace: '@font-face {', '@font-face { font-display: swap;' }}
        {{ base_font_sem_bold | font_face | replace: '@font-face {', '@font-face { font-display: swap;' }}
        {{ base_font_bold | font_face | replace: '@font-face {', '@font-face { font-display: swap;' }}

        {% assign fonts_name = base_font.family | append: ',' | append: base_font.fallback_families %}
      
        {% if base_font.family != settings.type_font_2_name %}
            {%- assign font_2 = settings.type_font_2_name -%}
            {%- capture fonts_name_2 -%}
                {{font_2 | strip | replace: ' ', '+'}}:300,300i,400,400i,500,500i,600,600i,700,700i,800,800i
            {%- endcapture -%}
            @import url('https://fonts.googleapis.com/css?family={{fonts_name_2}}&display=swap');
                    {% assign fonts_name_2 = font_2 %}
        {% else %}
                  {% assign fonts_name_2 = base_font.family | append: ',' | append: base_font.fallback_families %}
        {% endif %}
  {% else %}
        {% assign header_font = settings.type_font_2_font | font_modify: 'weight', '400' %}
        {% assign header_font_bold = settings.type_font_2_font | font_modify: 'weight', 'bold' %}
        {% assign header_font_sem_bold = settings.type_font_2_font | font_modify: 'weight', '600' %}
        {% assign header_font_medium = settings.type_font_2_font | font_modify: 'weight', '500' %}

        {% if header_font.family != settings.type_font_1_name %}
            {%- assign font_1 = settings.type_font_1_name -%}
            {%- capture font_name_1 -%}
                {{font_1 | strip | replace: ' ', '+'}}:300,300i,400,400i,500,500i,600,600i,700,700i,800,800i
            {%- endcapture -%}
            @import url('https://fonts.googleapis.com/css?family={{font_name_1}}&display=swap');
                  {% assign fonts_name = font_1 %}
        {% else %}
                  {% assign fonts_name = header_font.family | append: ',' | append: header_font.fallback_families %}
        {% endif %}

        {{ header_font | font_face | replace: '@font-face {', '@font-face { font-display: swap;' }}
        {{ header_font_medium | font_face | replace: '@font-face {', '@font-face { font-display: swap;' }}
        {{ header_font_sem_bold | font_face | replace: '@font-face {', '@font-face { font-display: swap;' }}
        {{ header_font_bold | font_face | replace: '@font-face {', '@font-face { font-display: swap;' }}
             
        {% assign fonts_name_2 = header_font.family | append: ',' | append: header_font.fallback_families %}
  {% endif %}
  
  .modern-image-content-{{section.id}} {
      padding-top: {{ section.settings.mg_top_desktop | append: 'px' }};
      padding-bottom: {{ section.settings.mg_bottom_desktop | append: 'px' }};
      background: {% if background_gradient != blank %}{{ background_gradient }}{% else %}{{ background_color }}{% endif %};
      position: relative;
  }
  .modern-image-content-{{section.id}} .row.align-iteam-center {
      align-items: center;
  }
  .modern-image-content-{{section.id}} .row.align-iteam-bottom {
      align-items: flex-end;
  }
  .modern-image-content-{{section.id}} .modern-image-content-bg {
      position: absolute;
      top: 0;
      left: 0;
      width: 100%;
      height: 100%;
  }
  .modern-image-content-{{section.id}} .modern-image-content-bg .animate-ambient-image,
  .modern-image-content-{{section.id}} .modern-image-content-bg .animate-ambient-image .icon-img{
      height: 100%;
  }
  .modern-image-content-{{section.id}} .modern-image-content-bg .animate-ambient-image img {
      height: 100%;
      width: 100%;
      object-fit: cover;
  }
  
  @media (max-width: 1199px) {
    .modern-image-content-{{section.id}} {
      padding-top: {{ section.settings.mg_top_tablet | append: 'px' }};
      padding-bottom: {{ section.settings.mg_bottom_tablet | append: 'px' }};
    }  
  }

  @media (max-width: 767px) {
    .modern-image-content-{{section.id}} {
      padding-top: {{ section.settings.mg_top_mobile | append: 'px' }};
      padding-bottom: {{ section.settings.mg_bottom_mobile | append: 'px' }};
    }
  }
</style>
<div class="modern-image-content modern-image-content-{{section.id}}">
  {%- if section.settings.background_image != blank -%}
    <div class="modern-image-content-bg {% if section.settings.image_behavior != 'none' %} animate--{{ section.settings.image_behavior }} animate-hidden{% endif %}">
      <div class="animate-ambient-image">
          {%- liquid
            assign preview_bg = section.settings.background_image
          -%}
          <div class="icon-img">
            <img {% render 'lazyload', preview_img: preview_bg %} loading="lazy"/>
              {% if settings.enable_lazyload %}<span class="data-lazy-loading"></span>{% endif %}
            </div>
      </div>
    </div>
  {%- endif -%}
  <div class="{% if container == 'fullwidth'%}container-full{% else %}container{% endif %}">
    <div class="row align-iteam-{{ section.settings.align }}">
      {%- for block in section.blocks -%}
        {%- case block.type -%}
          {%- when 'text' -%}
            <div class="modern-content modern-content-{{ block.id }} text-{{ block.settings.align }} col-lg-{{ block.settings.content_grid }} col-md-{{ block.settings.content_grid_tablate }} col-{{ block.settings.content_grid_mobile }} wow {{ block.settings.animations_type_content_block }} delay-{{ block.settings.animation_delay_content_block | replace: ".", "_" }}">
                <style>
                  .modern-content-{{ block.id }}.delay-{{ block.settings.animation_delay_content_block | replace: ".", "_" }} {
                      animation-delay: {{ block.settings.animation_delay_content_block }}
                  }
                </style>
                {%- if block.settings.sub_title != blank -%}
                  <h6 class="content-sub-title wow {{ block.settings.animations_type_sub_title }} delay-{{ block.settings.animation_delay_sub_title | replace: ".", "_" }}">{{ block.settings.sub_title }}</h6>
                  <style>
                    .modern-content-{{ block.id }} .content-sub-title.delay-{{ block.settings.animation_delay_sub_title | replace: ".", "_" }} {
                        animation-delay: {{ block.settings.animation_delay_sub_title }}
                    }
                  </style>
                {%- endif -%}
                {%- if block.settings.grid_title != blank -%}
                  <h2 class="content-grid-title wow {{ block.settings.animations_type_title }} delay-{{ block.settings.animation_delay_title | replace: ".", "_" }}">{{ block.settings.grid_title }}</h2>
                  <style>
                    .modern-content-{{ block.id }} .content-grid-title.delay-{{ block.settings.animation_delay_title | replace: ".", "_" }} {
                        animation-delay: {{ block.settings.animation_delay_title }}
                    }
                  </style>
                {%- endif -%}
                {%- if block.settings.description_text != blank -%}
                  <div class="modern-content-desc wow {{ block.settings.animations_type_scription }} delay-{{ block.settings.animation_delay_description | replace: ".", "_" }}">{{ block.settings.description_text }}</div>
                  <style>
                    .modern-content-{{ block.id }} .modern-content-desc.delay-{{ block.settings.animation_delay_description | replace: ".", "_" }} {
                        animation-delay: {{ block.settings.animation_delay_description }}
                    }
                  </style>
                {%- endif -%}
                <div class="icon-width-content">
                  {%- if block.settings.icon_1 != blank or block.settings.icon_title_1 != blank -%}
                    <div class="icon-box wow {{ block.settings.animations_type_icon_content }} delay-{{ block.settings.animation_delay_icon_content | replace: ".", "_" }}">
                      <style>
                        .modern-content-{{ block.id }} .icon-box.delay-{{ block.settings.animation_delay_icon_content | replace: ".", "_" }} {
                            animation-delay: {{ block.settings.animation_delay_icon_content }}
                        }
                      </style>
                      {%- if block.settings.icon_1 != blank -%}
                        {%- liquid
                          assign preview_icon_1 = block.settings.icon_1
                        -%}
                        <div class="icon-img">
                          <img {% render 'lazyload', preview_img: preview_icon_1 %} loading="lazy"/>
                            {% if settings.enable_lazyload %}<span class="data-lazy-loading"></span>{% endif %}
                          </div>
                      {%- endif -%}
                      {%- if block.settings.icon_title_1 != blank -%}
                        <div class="icon-title">{{ block.settings.icon_title_1 }}</div>
                      {%- endif -%}
                    </div>
                  {%- endif -%}

                  {%- if block.settings.icon_2 != blank or block.settings.icon_title_2 != blank -%}
                    <div class="icon-box wow {{ block.settings.animations_type_icon_content }} delay-{{ block.settings.animation_delay_icon_content | replace: ".", "_" }}">
                      <style>
                        .modern-content-{{ block.id }} .icon-box.delay-{{ block.settings.animation_delay_icon_content | replace: ".", "_" }} {
                            animation-delay: {{ block.settings.animation_delay_icon_content }}
                        }
                      </style>
                      {%- if block.settings.icon_2 != blank -%}
                        {%- liquid
                          assign preview_icon_2 = block.settings.icon_2
                        -%}
                        <div class="icon-img">
                          <img {% render 'lazyload', preview_img: preview_icon_2 %} loading="lazy"/>
                            {% if settings.enable_lazyload %}<span class="data-lazy-loading"></span>{% endif %}
                          </div>
                      {%- endif -%}
                      {%- if block.settings.icon_title_2 != blank -%}
                        <div class="icon-title">{{ block.settings.icon_title_2 }}</div>
                      {%- endif -%}
                    </div>
                  {%- endif -%}
                </div>
                {%- if block.settings.image_banner_button != blank -%}
                    {% if block.settings.button_style == '1'%}
                      <div class="headding-btn wow {{ block.settings.animations_type_button }} delay-{{ block.settings.animation_delay_button | replace: ".", "_" }}">
                        <style>
                            .modern-content-{{ block.id }} .headding-btn.delay-{{ block.settings.animation_delay_button | replace: ".", "_" }} {
                                animation-delay: {{ block.settings.animation_delay_button }}
                            }
                        </style>
                        <a {% if block.settings.image_banner_link == blank %}  role="link" aria-disabled="true"{% else %} href="{{ block.settings.image_banner_link }}"{% endif %} class="button" target="_blank">
                          {{ block.settings.image_banner_button }}
                        </a>
                      </div>
                    {% else %}
                      <div class="headding-btn wow {{ block.settings.animations_type_button }} delay-{{ block.settings.animation_delay_button | replace: ".", "_" }}">
                        <style>
                            .modern-content-{{ block.id }} .headding-btn.delay-{{ block.settings.animation_delay_button | replace: ".", "_" }} {
                                animation-delay: {{ block.settings.animation_delay_button }}
                            }
                        </style>
                        <a {% if block.settings.image_banner_link == blank %} role="link" aria-disabled="true"{% else %} href="{{ block.settings.image_banner_link }}"{% endif %} class="button" target="_blank">
                          {{ block.settings.image_banner_button }} {% render 'arrow-right-2' %}
                        </a>
                      </div>
                    {% endif %}
                  {%- endif -%}
                
              <style>
                :root {
                  {% if block.settings.sub_heading_fontfamily == 'font_1' %}
                      --modern-sub-heading-font-family-2: {{fonts_name}};
                  {% else %}
                      --modern-sub-heading-font-family-2: {{fonts_name_2}};
                  {% endif %}
                }
                .modern-content-{{ block.id }} {
                      position: relative;
                      padding-bottom: {{ block.settings.block__space_bottom | append: 'px' }};
                }
                .modern-content-{{ block.id }} .content-sub-title {
                  font-size: {{ block.settings.sub_title_font_size | append: 'px' }}; 
                  padding-bottom: {{ block.settings.sub_title__space_bottom | append: 'px' }};
                  font-weight: {{ block.settings.sub_title_font_weight }};
                  {% if block.settings.sub_title_color != blank %}
                      color: {{ block.settings.sub_title_color }};
                  {% else %}
                      color: var(--heading-text-color);
                  {% endif %}
                  text-transform: {{ block.settings.sub_title_text_transform }};
                  font-family: var(--modern-sub-heading-font-family-2);
                  letter-spacing: {{ block.settings.sub_title_letter_spacing | append: 'px' }};
                  position: relative;
                  z-index: 1;
                }
                .modern-content-{{ block.id }} .content-grid-title {
                    padding-bottom: {{ block.settings.mg_bottom_title | append: 'px' }};
                    font-weight: {{ block.settings.block_title_font_weight }};
                    font-size: {{ block.settings.title_font_size | append: 'px' }};
                    {% if block.settings.grid_title_color != blank %}
                         color: {{ block.settings.grid_title_color }};
                    {% else %}
                         color: var(--heading-text-color);
                    {% endif %}
                    text-transform: {{ block.settings.title_text_transform }};
                    {%- if block.settings.postion_absolut != blank -%}
                      position: absolute;
                      top: {{ block.settings.title_top_position }};
                      left: {{ block.settings.title_left_position }};
                    {% endif %}
                }
                .modern-content-{{ block.id }} .modern-content-desc {
                    padding-bottom: {{ block.settings.description__space_bottom | append: 'px' }};
                    font-weight: {{ block.settings.desc_font_weight }};
                    font-size: {{ block.settings.desc_title | append: 'px' }};
                    line-height: {{ block.settings.sub_title_line_height | append: 'px' }};
                    {% if block.settings.desc_color != blank %}
                        color: {{ block.settings.desc_color }};
                    {% else %}
                        color: var(--body-text-color);
                    {% endif %}
                    
                    max-width: {{ block.settings.content__width | append: 'px' }};
                }
                .icon-width-content {
                    display: flex;
                    align-items: center;
                    margin: 0 -15px;
                }
                .icon-width-content .icon-box {
                    display: flex;
                    align-items: center;
                    padding: 0px 15px;
                    padding-bottom: {{ block.settings.icon_title__space_bottom | append: 'px' }};
                }
                .modern-content-{{ block.id }} .icon-width-content .icon-box .icon-title {
                    font-size: {{ block.settings.icon_title_size | append: 'px' }};
                    {% if block.settings.icon_title_color != blank %}
                        color: {{ block.settings.icon_title_color }};
                    {% else %}
                        color: var(--heading-text-color);
                    {% endif %}
                    font-weight: {{ block.settings.icon_title_font_weight }};
                    padding-left: 15px;
                }
                .modern-content-{{ block.id }} .button {
                  {% if block.settings.button_color != blank %}
                        color: {{ block.settings.button_color }};
                  {% else %}
                        color: var(--button-color);
                  {% endif %}
                  {% if block.settings.button_bg_color != blank %}
                       background-color: {{ block.settings.button_bg_color }};
                  {% else %}
                       background-color: var(--button-bg-color);
                  {% endif %}
                  {% if block.settings.button_border_color != blank %}
                       border: {{ block.settings.button_border | append: 'px' }} solid {{ block.settings.button_border_color }};
                  {% else %}
                       border: {{ block.settings.button_border | append: 'px' }} solid var(--button-border-color);
                  {% endif %}
                  letter-spacing: {{ block.settings.button_letter_spacing | append: 'px' }};
                  padding:{{ block.settings.btn_padding_top_bottom | append: 'px' }} {{ block.settings.btn_padding_left_right | append: 'px' }};
                  font-size:{{ block.settings.font_size_button | append: 'px' }};
                  border-bottom: {{ block.settings.button_border_bottom | append: 'px' }} solid {{ block.settings.button_border_color_bottom }};
                  font-weight: {{ block.settings.button_font_weight }};
                }
                .modern-content-{{ block.id }} .button svg {
                  {% if block.settings.button_color != blank %}
                      fill: {{ block.settings.button_color }};
                  {% else %}
                      fill: var(--button-color);
                  {% endif %}
                  transition: all 400ms ease;
                }

                {% if settings.button_hover_style == '2' %}
                  .modern-content-{{ block.id }} .button {
                    position: relative;
                    overflow: hidden;
                    z-index: 0;
                  }
                  .modern-content-{{ block.id }} .button:before {
                    content: "";
                    position: absolute;
                    top: 0;
                    right: 0;
                    bottom: 0;
                    left: 0;
                    transform: unset;
                    display: unset;
                    border: unset;
                    border-radius: unset;
                    width: unset;
                    height: unset;
                    animation: unset;
                    opacity: unset;
                    visibility: unset;
                    background: {{ block.settings.button_bghover_color }};
                    right: 100%;
                    z-index: -1;
                    transition: all 400ms ease;
                  }
                  .modern-content-{{ block.id }} .button:hover {
                    {% if block.settings.button_hover_color != blank %}
                        color: {{ block.settings.button_hover_color }};
                    {% else %}
                        color: var(--button-hover-color);
                    {% endif %}
                    {% if block.settings.button_borderhover_color != blank %}
                        border: {{ block.settings.button_border | append: 'px' }} solid {{ block.settings.button_borderhover_color }};
                    {% else %}
                         border: {{ block.settings.button_border | append: 'px' }} solid var(--button-border-hover-color);
                    {% endif %}
                  }
                  .modern-content-{{ block.id }} .button:hover svg {
                    {% if block.settings.button_hover_color != blank %}
                        fill: {{ block.settings.button_hover_color }};
                    {% else %}
                        fill: var(--button-hover-color);
                    {% endif %}
                  }
                  .modern-content-{{ block.id }} .button:hover:before {
                    right: 0%;
                  }
                {% elsif settings.button_hover_style == '3' %}
                  .modern-content-{{ block.id }} .button {
                    position: relative;
                    overflow: hidden;
                    z-index: 0;
                  }
                  .modern-content-{{ block.id }} .button:before {
                    content: "";
                    position: absolute;
                    top: 0;
                    left: -70%;
                    display: unset;
                    border: unset;
                    border-radius: unset;
                    animation: unset;
                    opacity: unset;
                    visibility: unset;
                    background: {{ block.settings.button_bghover_color }};
                    z-index: -1;
                    transition: all 400ms ease;
                    clip-path: polygon(25px 0%, 100% 0%, 100% 100%, 0% 100%);
                    width: 70%;
                    height: 100%;
                    transform: rotateY(180deg);
                  }
                  .modern-content-{{ block.id }} .button:after {
                    content: "";
                    position: absolute;
                    top: 0;
                    right: -70%;
                    transform: unset;
                    display: unset;
                    border: unset;
                    border-radius: unset;
                    animation: unset;
                    opacity: unset;
                    visibility: unset;
                    background: {{ block.settings.button_bghover_color }};
                    z-index: -1;
                    transition: all 400ms ease;
                    clip-path: polygon(25px 0%, 100% 0%, 100% 100%, 0% 100%);
                    width: 70%;
                    height: 100%;
                  }
                  .modern-content-{{ block.id }} .button:hover {
                    {% if block.settings.button_hover_color != blank %}
                        color: {{ block.settings.button_hover_color }};
                    {% else %}
                        color: var(--button-hover-color);
                    {% endif %}
                    {% if block.settings.button_borderhover_color != blank %}
                        border: {{ block.settings.button_border | append: 'px' }} solid {{ block.settings.button_borderhover_color }};
                    {% else %}
                         border: {{ block.settings.button_border | append: 'px' }} solid var(--button-border-hover-color);
                    {% endif %}
                  }
                  .modern-content-{{ block.id }} .button:hover svg {
                    {% if block.settings.button_hover_color != blank %}
                        fill: {{ block.settings.button_hover_color }};
                    {% else %}
                        fill: var(--button-hover-color);
                    {% endif %}
                  }
                  .modern-content-{{ block.id }} .button:hover:before {
                    left: calc(0% - 2px);
                  }
                  .modern-content-{{ block.id }} .button:hover:after {
                    right: calc(0% - 2px);
                  }
                {% elsif settings.button_hover_style == '4' %}
                  .modern-content-{{ block.id }} .button {
                    position: relative;
                    overflow: hidden;
                    z-index: 0;
                  }
                  .modern-content-{{ block.id }} .button:before {
                    content: "";
                    position: absolute;
                    top: 0;
                    left: -70%;
                    display: unset;
                    border: unset;
                    border-radius: unset;
                    animation: unset;
                    opacity: unset;
                    visibility: unset;
                    background: {{ block.settings.button_bghover_color }};
                    z-index: -1;
                    transition: all 400ms ease;
                    clip-path: polygon(25px 0%, 100% 0%, 100% 100%, 0% 100%);
                    width: 70%;
                    height: 100%;
                    transform: rotateY(180deg);
                  }
                  .modern-content-{{ block.id }} .button:after {
                    content: "";
                    position: absolute;
                    top: 0;
                    right: -70%;
                    transform: unset;
                    display: unset;
