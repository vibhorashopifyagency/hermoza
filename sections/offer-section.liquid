{% if section.blocks.size > 0 %}
  {%-liquid
    assign container = section.settings.container
    assign blog_bg_image = section.settings.background_image
  -%}

  <style>
    .offer-gird-main.offer-gird-{{section.id}} {
      {% if section.settings.background_image != blank %}
        background: url({{ blog_bg_image | img_url: '1920x' }});
        background-repeat: no-repeat;
        background-size: cover;
        background-position: center;
      {% else %}
        background: {% if section.settings.background_gradient != blank %}{{ section.settings.background_gradient }}{% else %}{{ section.settings.background_color }}{% endif %};
      {% endif %}
      padding-top: {{ section.settings.mg_top_desktop | append: 'px' }};
      padding-bottom: {{ section.settings.mg_bottom_desktop | append: 'px' }};
    }
    .offer-gird-{{section.id}} .offer-gird-row {
        display: flex;
        flex-wrap: wrap;
        margin: 0px -{{ section.settings.grid_spacing | append: 'px' }};
    }
    .offer-gird-{{section.id}} .offer-gird {
        padding-left: {{ section.settings.grid_spacing | append: 'px' }};
        padding-right: {{ section.settings.grid_spacing | append: 'px' }};
    }
    .offer-gird-inner {
        overflow: hidden;
        position: relative;
        margin-bottom: 30px;
    }
    .offer-gird.column-1 {
        width: 100%;
    }
    .offer-gird.column-2 {
        width: 50%;
    }
    .offer-gird.column-3 {
        width: 33.33%;
    }
    .offer-gird.column-4 {
        width: 25%;
    }
    .offer-gird.column-5 {
        width: 20%;
    }
    .offer-gird-inner .grid-image img {
        width: 100%;
        height: 100%;
        display: block;
        object-fit: cover;
        transition: all 400ms ease;
    }
    .offer-gird-inner:hover .grid-image img {
        transform: scale(1.1);
    }
    .offer-gird-inner .grid-image {
        position: absolute;
        width: 100%;
        height: 100%;
        z-index: -1;
    }
    .offer-gird-inner .grid-image .animate-ambient-image {
        height: 100%;
    }
  
    @media (max-width:1199px) {
       .offer-gird-main.offer-gird-{{section.id}} {
        padding-top: {{ section.settings.mg_top_tablet | append: 'px' }};
        padding-bottom: {{ section.settings.mg_bottom_tablet | append: 'px' }};
      } 
    }

    @media(max-width:1024px){

      .offer-gird.column-tb-1 {
          width: 100%;
      }
      .offer-gird.column-tb-2 {
          width: 50%;
      }
      .offer-gird.column-tb-3 {
          width: 33.33%;
      }
      .offer-gird.column-tb-4 {
          width: 25%;
      }
      .offer-gird.column-tb-5 {
          width: 20%;
      }
    
    }

    @media (max-width:767px) {
       .offer-gird-main.offer-gird-{{section.id}} {
        padding-top: {{ section.settings.mg_top_mobile | append: 'px' }};
        padding-bottom: {{ section.settings.mg_bottom_mobile | append: 'px' }};
      } 
       .offer-gird.column-mb-1 {
          width: 100%;
      }
      .offer-gird.column-mb-2 {
          width: 50%;
      }
      .offer-gird.column-mb-3 {
          width: 33.33%;
      }
      .offer-gird.column-mb-4 {
          width: 25%;
      }
      .offer-gird.column-mb-5 {
          width: 20%;
      }
    }
  </style>
  
  <div class="offer-gird-main offer-gird-{{section.id}}">
    <div class="{% if container == 'fullwidth'%}container-full{% else %}container{% endif %}">
      <div class="offer-gird-row">
          {% for block in section.blocks %}
            <div class="offer-gird block-{{block.id}} column-{{ section.settings.layout }} column-tb-{{ section.settings.layout_tablet }} column-mb-{{ section.settings.layout_mobile }} wow {{ block.settings.animations_type_offer_card }} delay-{{ block.settings.animation_delay_offer_card | replace: ".", "_" }}">
              <div class="offer-gird-inner">
                <div class="grid-image {% if block.settings.image_behavior != 'none' %} animate--{{ block.settings.image_behavior }} animate-hidden{% endif %}">
                  {% if block.settings.image != blank %}
                    {%- assign preview_img = block.settings.image -%}
                    <div class="animate-ambient-image">
                      <img {% render 'lazyload',preview_img: preview_img %} />
                    </div>
                    {% if settings.enable_lazyload %}<span class="data-lazy-loading"></span>{% endif %}
                  {% else %}
                    <div class="not_img" style="--height_not_img: 262px">506 x 262</div>
                  {% endif %}
                </div>
                <div class="grid-content">
                  <h6 class="grid-subtitle">{{ block.settings.sub_title }}</h6>
                  <h2 class="grid-title">{{ block.settings.title }}</h2>
                  <div class="offer-button">
                    <a {% if block.settings.image_banner_link == blank %} role="link" aria-disabled="true"{% else %} href="{{ block.settings.image_banner_link }}"{% endif %}  class="button">
                    {{ block.settings.offer_button }} {% render 'arrow-right-2' %}</a>
                  </div>
                </div>
              </div>
            </div>
