                  border-radius: {{ block.settings.border_radius | append: 'px' }};
              }
              .block-{{block.id}} .offer-gird-inner .grid-content {
                  padding: {{ block.settings.offer_top_bottom_padding | append: 'px' }} {{ block.settings.offer_left_right_padding | append: 'px' }};
                  padding-bottom: calc({{ block.settings.offer_top_bottom_padding | append: 'px' }} + 5px);
              }
              .block-{{block.id}} .offer-gird-inner .grid-subtitle {
                  font-size: {{ block.settings.font_size_sub_title | append: 'px' }};
                  font-family: var(--offer-sub-title-font-family);
                  {% if block.settings.sub_title_color != blank %}
                    color: {{ block.settings.sub_title_color }};
                  {% else %}
                    color: var(--heading-text-color);
                  {% endif %}
                  font-weight: {{ block.settings.sub_title_font_weight }};
                  text-transform: {{ block.settings.sub_title_text_transform }};
                  letter-spacing: {{ block.settings.font_size_sub_letter_spacing | append: 'px' }};
                  margin-bottom: {{ block.settings.sub_title__space_top | append: 'px' }};
                  {% if block.settings.border_color_2 != blank %}
                    border-bottom: {{ block.settings.border_width_2 | append: 'px' }} solid {{ block.settings.border_color_2}}; 
                  {% else %}
                    border-bottom: {{ block.settings.border_width_2 | append: 'px' }} solid var(--heading-text-color);
                  {% endif %}
                  display:inline-block;
              }
              .block-{{block.id}} .offer-gird-inner .grid-title{
                  font-size: {{ block.settings.font_size_title | append: 'px' }};
                  font-family: var(--offer-title-font-family);
                  {% if block.settings.title_color != blank %}
                    color: {{ block.settings.title_color }};
                  {% else %}
                    color: var(--heading-text-color);
                  {% endif %}
                  font-weight: {{ block.settings.title_font_weight }};
                  text-transform: {{ block.settings.title_text_transform }};
                  letter-spacing: {{ block.settings.font_size_letter_spacing | append: 'px' }};
                  line-height: {{ block.settings.title_line_height | append: 'px' }};
                  max-width: {{ block.settings.title_max_width | append: 'px' }};
                  margin-bottom: {{ block.settings.title__space_top | append: 'px' }};
              }
              .block-{{block.id}} .offer-button .button {
                  {% if block.settings.button_color != blank %}
                    color: {{ block.settings.button_color }};
                  {% else %}
                    color: var(--button-color);
                  {% endif %}
                  {% if block.settings.button_bg_color != blank %}
                    background-color: {{ block.settings.button_bg_color }};
                  {% else %}
                    background-color: var(--button-bg-color);
                  {% endif %}
                  {% if block.settings.button_border_color != blank %}
                    border: {{ block.settings.button_border | append: 'px' }} solid {{ block.settings.button_border_color }};
                  {% else %}
                    border: {{ block.settings.button_border | append: 'px' }} solid var(--button-border-color);
                  {% endif %}
                  letter-spacing: {{ block.settings.button_letter_spacing | append: 'px' }};
                  font-size: {{ block.settings.font_size_button | append: 'px' }};
                  padding: {{ block.settings.btn_padding_top_bottom | append: 'px' }} {{ block.settings.btn_padding_left_right | append: 'px' }};
                }
                .block-{{block.id}} .offer-button .button svg {
                   fill: {{ block.settings.button_color }};
                   width: var(--font-button-size);
                   transition: all 400ms ease;
                }
                {% if settings.button_hover_style == '2' %}
                  .block-{{block.id}} .offer-button .button {
                    position: relative;
                    overflow: hidden;
                    z-index: 0;
                  }
                  .block-{{block.id}} .offer-button .button:before {
                    content: "";
                    position: absolute;
                    top: 0;
                    right: 0;
                    bottom: 0;
                    left: 0;
                    transform: unset;
                    display: unset;
                    border: unset;
                    border-radius: unset;
                    width: unset;
                    height: unset;
                    animation: unset;
                    opacity: unset;
                    visibility: unset;
                    {% if block.settings.button_bghover_color != blank %}
                      background: {{ block.settings.button_bghover_color }};
                    {% else %}
                      background: var(--button-bg-hover-color);
                    {% endif %}
                    right: 100%;
                    z-index: -1;
                    transition: all 400ms ease;
                  }
                  .block-{{block.id}} .offer-button .button:hover {
                    {% if block.settings.button_hover_color != blank %}
                      color: {{ block.settings.button_hover_color }};
                    {% else %}
                      color: var(--button-hover-color);
                    {% endif %}
