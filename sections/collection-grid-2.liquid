{% if section.blocks.size > 0 %}
  {%-liquid
    assign container = section.settings.container
  -%}
  {% render 'collection-grid-style-2' %}
  <div class="collection-grids collection-grids-block-{{section.id}}">
    <div class="{% if container == 'fullwidth'%}container-full{% else %}container{% endif %}">
      <div class="collection-grids-inner">
        {%- if section.settings.product_block_title != blank or section.settings.sub_title != blank -%}
          <div class="section-header">
            <div class="row align-item-center">
              <div class="{% if section.settings.view_all != blank %}col-xl-9 col-lg-9 col-md-9{% else %}col-xl-12 col-lg-12 col-md-12{% endif %}">
                <div class="section-title text-{{ section.settings.product_block_align }} text-mb-{{ section.settings.product_block_align_mobile }}">
                  {%- if section.settings.sub_title != blank -%}
                      <p class="banner-sub-title wow {{ section.settings.animations_type_subtitle }} delay-{{ settings.settings.animation_delay_subtitle | replace: ".", "_" }}">
                          <style>
                            .collection-grids-block-{{section.id}} .banner-sub.delay-{{ block.settings.animation_delay_subtitle | replace: ".", "_" }} {
                                animation-delay: {{ block.settings.animation_delay_subtitle }}
                            }
                          </style>
                          <span>{{ section.settings.sub_title }}</span>
                      </p>
                  {%- endif -%}
                  <h3 class="title wow {{ section.settings.animations_type_title }} delay-{{ settings.settings.animation_delay_title | replace: ".", "_" }}">
                      <style>
                        .collection-grids-block-{{section.id}} .section-header .title.delay-{{ block.settings.animation_delay_title | replace: ".", "_" }} {
                            animation-delay: {{ block.settings.animation_delay_title }}
                        }
                      </style>
                      <span class="text">{{ section.settings.product_block_title | escape }}</span>
                  </h3>
                </div>
              </div>
              {% if section.settings.view_all != blank %}
                <div class="col-xl-3 col-lg-3 col-md-3">
                  
                <div class="view-all-button text-{{ section.settings.view_all_align }} text-mb-{{ section.settings.view_all_align_mobile }} wow {{ section.settings.animations_type_button }} delay-{{ settings.settings.animation_delay_button | replace: ".", "_" }}">
                    <style>
                      .collection-grids-block-{{section.id}} .section-header .view-all-button.delay-{{ block.settings.animation_delay_button | replace: ".", "_" }} {
                          animation-delay: {{ block.settings.animation_delay_button }}
                      }
                    </style>
                    <a class="view_all"{% if section.settings.link_view_all == blank %} href="#"
                      role="link" aria-disabled="true"{% else %} href="{{ section.settings.link_view_all }}"{% endif %} title="{{ section.settings.view_all }}">
                        {{ section.settings.view_all }}
                        {%- if section.settings.icon_view_all -%}
                          <span class="icon-svg">
                              {{ section.settings.icon_view_all }}
                          </span>
                        {%- endif -%}
                    </a>
                  </div>
                </div>
              {% endif %}
            </div>
          </div>
        {%- endif -%}
        <div class="collectionGrid">
          {% for block in section.blocks %}
            <div class="collectionGrid-box collectionGrid-{{ block.id }} column-{{ section.settings.layout }} column-tb-{{ section.settings.layout_tablet }} column-mb-{{ section.settings.layout_mobile }} collectio-block-{{block.id}} wow {{ block.settings.animations_type }} delay-{{ block.settings.animation_delay | replace: ".", "_" }}">
              <style>
                .collection-grids-block-{{section.id}} .collectionGrid-box.delay-{{ block.settings.animation_delay | replace: ".", "_" }} {
                    animation-delay: {{ block.settings.animation_delay }}
                }
              </style>
              <div class="collection-grid-box">
              {% if block.settings.image != blank %}
                <div class="collection-grid-img {% if section.settings.image_behavior != 'none' %} animate--{{ section.settings.image_behavior }} animate-hidden{% endif %}">
                  <div class="animate-ambient-image">
                    <a {% if block.settings.link == blank %} role="link" aria-disabled="true"{% else %} href="{{ block.settings.link }}"{% endif %} class="image-zoom">
                        {%- assign preview_img = block.settings.image -%}
                        <img {% render 'lazyload',preview_img: preview_img %} />
                        {% if settings.enable_lazyload %}<span class="data-lazy-loading"></span>{% endif %}
                    </a>
                  </div>
                  </div>
                {% else %}
                  <div class="collection-grid-img">
                    <div class="not_img" style="--height_not_img: 525px">935 x 525</div>
                  </div>
                {% endif %}
                <div class="collection-grid-content justify-{{ section.settings.button_align }} align-{{ section.settings.align_horizontal }} text-{{ section.settings.button_align }}">
                 {% if block.settings.sub__title != blank %}
                    <h3 class="collection-grid-subtitle">
                      {{ block.settings.sub__title }}
                    </h3>
                  {% endif %}
                  {% if block.settings.title_name != blank %}
                    <h6 class="collection-grid-title">
                      <a {% if block.settings.title_link == blank %} href="#" role="link" aria-disabled="true"{% else %} href="{{ block.settings.title_link }}"{% endif %} title="{{ block.settings.title_name }}">
                          {{ block.settings.title_name }}
                      </a>
                    </h6>
                  {% endif %}
                {% if block.settings.description_text != blank %}
                    <p class="collection-grid-dec text-{{ section.settings.title_align }} align-{{ block.settings.button_align }}">{{ block.settings.description_text }}</p>
                  {% endif %}
                  {% if block.settings.button != blank %}
                  
                  {% if section.settings.button_style == '1'%}
                    <div class="collection-grid-link align-{{ block.settings.button_align }} align-h-{{ block.settings.button_align_horizontal }}">
                      <a {% if block.settings.title_link == blank %} role="link" aria-disabled="true"{% else %} href="{{ block.settings.title_link }}"{% endif %} class="button-link">
                        {{ block.settings.button }}
                      </a>
                    </div>
                    {% elsif section.settings.button_style == '3'%}
                         <div class="collection-grid-link align-{{ block.settings.button_align }} align-h-{{ block.settings.button_align_horizontal }}">
                          <a {% if block.settings.title_link == blank %} role="link" aria-disabled="true"{% else %} href="{{ block.settings.title_link }}"{% endif %} class="button button-link">
                            {{ block.settings.button }} {% render 'arrow-right-2' %}
                          </a>
                        </div>
                     {% else %}
                      <div class="collection-grid-link align-{{ block.settings.button_align }} align-h-{{ block.settings.button_align_horizontal }}">
                        <a {% if block.settings.title_link == blank %} role="link" aria-disabled="true"{% else %} href="{{ block.settings.title_link }}"{% endif %} class="button button-link">
                          {{ block.settings.button }}
                        </a>
                      </div>
                    {% endif %}
                  {% endif %}
                </div>
              </div>
            </div>
            <style>
                 .collectionGrid-{{ block.id }} .collection-grid-img{
                     border-radius:{{ block.settings.image_border_radius | append: 'px' }};
                 }
                 {% if section.settings.title_style == '2'%}
                   .collection-grids-block-{{section.id}} .collectionGrid-{{ block.id }} .collection-grid-content {
                      position: absolute;
                   }
                   .collection-grids-block-{{section.id}} .collectionGrid-{{ block.id }} .collection-grid-content.justify-left.align-top {
                      left: {{ section.settings.button_position_left_right }};
                      top: {{ section.settings.button_position_top }};
                  }
                   body.layout_rtl .collection-grids-block-{{section.id}} .collectionGrid-{{ block.id }} .collection-grid-content.justify-left.align-top {
                     left: unset;
                     right: {{ section.settings.button_position_left_right }};
                   }
                  .collection-grids-block-{{section.id}} .collectionGrid-{{ block.id }} .collection-grid-content.justify-left.align-center {
                      left: {{ section.settings.button_position_left_right }};
                      top: 50%;
                      transform: translateY(-50%);
                  }
                  body.layout_rtl .collection-grids-block-{{section.id}} .collectionGrid-{{ block.id }} .collection-grid-content.justify-left.align-center {
                    left: unset;
                    right: {{ section.settings.button_position_left_right }};
                  }
                  .collection-grids-block-{{section.id}} .collectionGrid-{{ block.id }} .collection-grid-content.justify-left.align-bottom {
                      left: {{ section.settings.button_position_left_right }};
                      bottom: {{ section.settings.button_position_top }};
                  }
                  body.layout_rtl .collection-grids-block-{{section.id}} .collectionGrid-{{ block.id }} .collection-grid-content.justify-left.align-bottom {
                    left: unset;
                    right: {{ section.settings.button_position_left_right }};
                  }
                  .collection-grids-block-{{section.id}} .collectionGrid-{{ block.id }} .collection-grid-content.justify-center.align-top {
                      left: 50%;
                      top: {{ section.settings.button_position_top }};
                      transform: translate(-50%);
                  }
                  .collection-grids-block-{{section.id}} .collectionGrid-{{ block.id }} .collection-grid-content.justify-center.align-center {
                      left: 50%;
                      top: 50%;
                      transform: translate(-50%, -50%);
                  }
                  .collection-grids-block-{{section.id}} .collectionGrid-{{ block.id }} .collection-grid-content.justify-center.align-bottom {
                      left: 50%;
                      bottom: {{ section.settings.button_position_top }};
                      transform: translate(-50%);
                  }
                  .collection-grids-block-{{section.id}} .collectionGrid-{{ block.id }} .collection-grid-content.justify-right.align-top {
                      right: {{ section.settings.button_position_left_right }};
                      top: {{ section.settings.button_position_top }};
                  }
                  body.layout_rtl .collection-grids-block-{{section.id}} .collectionGrid-{{ block.id }} .collection-grid-content.justify-right.align-top {
                    right: unset;
                    left: {{ section.settings.button_position_left_right }};
                  }
                  .collection-grids-block-{{section.id}} .collectionGrid-{{ block.id }} .collection-grid-content.justify-right.align-center {
                      right: {{ section.settings.button_position_left_right }};
                      top: 50%;
                      transform: translateY(-50%);
                  }
                  body.layout_rtl .collection-grids-block-{{section.id}} .collectionGrid-{{ block.id }} .collection-grid-content.justify-right.align-center {
                    right: unset;
                    left: {{ section.settings.button_position_left_right }};
                  }
                  .collection-grids-block-{{section.id}} .collectionGrid-{{ block.id }} .collection-grid-content.justify-right.align-bottom {
                      right: {{ section.settings.button_position_left_right }};
                      bottom: {{ section.settings.button_position_top }};
                  }
                  body.layout_rtl .collection-grids-block-{{section.id}} .collectionGrid-{{ block.id }} .collection-grid-content.justify-right.align-bottom {
                    right: unset;
                    left: {{ section.settings.button_position_left_right }};
                  }
                {% endif %}
                
                @media (max-width: 1024px) {
                  {% if section.settings.title_style == '2'%}
                      .collection-grids-block-{{section.id}}.collection-grids .collection-grid-img {
                          margin-bottom: 0px;
                      } 
                      .collection-grids-block-{{section.id}} .collectionGrid-box{
                          padding-bottom:30px;
                      }
                  {% endif %}
                }
              
                @media (max-width: 767px) {
                  {% if section.settings.title_style == '2'%}
                      .collection-grids-block-{{section.id}}.collection-grids .collection-grid-img {
                          margin-bottom: 0px;
                      }      
                    {% endif %} 
                }
            </style>
          {% endfor %}
        </div>
      </div>
    </div>
  </div>
{% endif %}

