{%- liquid
    assign as_grid_gap = section.settings.grid_gap
    assign grid_gap = section.settings.grid_gap | times: 0.5 | append: 'px'

    assign container = section.settings.container
    assign collection = section.settings.product_block_collection
    assign limit = section.settings.product_block_limit
    assign layout = section.settings.product_block_layout
    assign infinite_scroll = section.settings.product_block_infinite_scroll

    assign block_title = section.settings.product_block_title
    assign block_title_align = section.settings.title_align
    assign image_ratio = settings.product_block_image_ratio
    assign swipe_on_mobile = section.settings.product_block_swipe_on_mobile

    assign products_to_display = 0
    if collections[collection].handle != ''
        if collections[collection].all_products_count > limit
            assign products_to_display = limit
        else
            assign products_to_display = collections[collection].all_products_count
        endif 
    else
        assign products_to_display = limit
    endif

    assign swipe = false
    if swipe_on_mobile == true and layout == 'grid' and products_to_display > 2
        assign swipe = true
    endif

    assign block_banners = section.blocks | where: 'type', 'banner'
    assign block_banner = block_banners[0]
    assign show_banner = false
    if block_banner != blank
        assign show_banner = true
    endif

    assign countdown = section.settings.countdown
    
    assign product_bg = section.settings.product_bg
    assign product_bg_gradient = section.settings.product_bg_gradient
    assign product_bg_image = section.settings.product_bg_image

    assign color_title = section.settings.color_title
    assign border_color_title = section.settings.border_color_title
    assign pd_top_title = section.settings.pd_top_title
    assign fontsize_title = section.settings.fontsize_title
    assign fontsize_title_mb = section.settings.fontsize_title_mb
    assign product_button_width = section.settings.product_button_width
    assign pd_top_product_button = section.settings.pd_top_product_button
    assign pd_bottom_product_button = section.settings.pd_bottom_product_button

    assign mg_bottom_title = section.settings.mg_bottom_title
    assign mg_bottom_title_mb = section.settings.mg_bottom_title_mb
    assign mg_bottom_view = section.settings.mg_bottom_view
    assign mg_bottom_view_mb = section.settings.mg_bottom_view_mb

    assign color_view_all = section.settings.color_view_all
    assign color_view_all_icon = section.settings.color_view_all_icon
    assign font_weight_view_all = section.settings.font_weight_view_all

    assign color_button = section.settings.color_button
    assign bg_color_button = section.settings.bg_color_button
    assign border_color_button = section.settings.border_color_button
    assign bg_color_gradient_button = section.settings.bg_color_gradient_button

    assign color_button_hover = section.settings.color_button_hover
    assign bg_color_button_hover = section.settings.bg_color_button_hover
    assign border_color_button_hover = section.settings.border_color_button_hover
    assign bg_color_gradient_button_hover = section.settings.bg_color_gradient_button_hover

    assign mg_top_desktop = section.settings.mg_top_desktop
    assign mg_top_tablet = section.settings.mg_top_tablet
    assign mg_top_mobile = section.settings.mg_top_mobile

    assign mg_bottom_desktop = section.settings.mg_bottom_desktop
    assign mg_bottom_tablet = section.settings.mg_bottom_tablet
    assign mg_bottom_mobile = section.settings.mg_bottom_mobile

    assign spacing_top_dots = section.settings.spacing_top_dots

    assign padding_full_width = section.settings.padding_full_width

    assign mg_bottom_button = section.settings.mg_bottom_button
    assign enable_border_top = section.settings.enable_border_top
    assign enable_border_bottom = section.settings.enable_border_bottom
    assign color_border_bottom = section.settings.color_border_bottom
    assign border_height = section.settings.border_height

    assign button_text_font_size = section.settings.button_text_font_size
    assign button_text_font_weight = section.settings.button_text_font_weight
    assign button_text_text_transform = section.settings.button_text_text_transform

    assign collection_color = section.settings.collection_color
    assign collection_color_hover = section.settings.collection_color_hover
    assign collection_bg_color = section.settings.collection_bg_color
    assign collection_bg_color_gradient = section.settings.collection_bg_color_gradient

    assign position_vertical_arrow = section.settings.position_vertical_arrow

    if show_banner
        assign fontsize_text = block_banner.settings.fontsize_text
        assign fontsize_text_mb = block_banner.settings.fontsize_text_mb
        assign color_text = block_banner.settings.color_text
        assign mg_bottom_text = block_banner.settings.mg_bottom_text

        assign fontsize_sub_text = block_banner.settings.fontsize_sub_text
        assign fontsize_sub_text_mb = block_banner.settings.fontsize_sub_text_mb
        assign color_sub_text = block_banner.settings.color_sub_text
        assign mg_bottom_sub_text = block_banner.settings.mg_bottom_sub_text

        assign fontsize_description = block_banner.settings.fontsize_description
        assign color_description = block_banner.settings.color_description
        
        assign banner_border_radius = block_banner.settings.banner_border_radius

        assign button_bg_color = block_banner.settings.button_bg_color
        assign button_color = block_banner.settings.button_color
        assign button_border_color = block_banner.settings.button_border_color
        assign button_bg_color_gradient = block_banner.settings.button_bg_color_gradient

        assign button_bg_color_hover = block_banner.settings.button_bg_color_hover
        assign button_color_hover = block_banner.settings.button_color_hover
        assign button_border_color_hover = block_banner.settings.button_border_color_hover
        assign button_bg_color_gradient_hover = block_banner.settings.button_bg_color_gradient_hover
    endif

    if section.settings.enable_layout_special
        assign bg_card = section.settings.bg_card
        assign padding_card = section.settings.padding_card
        assign border_radius_card = section.settings.border_radius_card
        assign border_width_card = section.settings.border_width_card
        assign border_color_card = section.settings.border_color_card
    endif

    if section.settings.countdown
        assign color_countdown = section.settings.color_countdown
        assign font_size_countdown_text = section.settings.font_size_countdown_text
        assign countdown_margin_left = section.settings.countdown_margin_left
    endif

    assign enable_pd_bottom = section.settings.enable_product_card_pd_bottom
    assign pd_bottom = section.settings.product_card_pd_bottom

    assign enable_minus_mg_top = section.settings.enable_minus_mg_top
    assign minus_spacing_top = section.settings.minus_spacing_top | append: 'px'

    if settings.enable_layout_rtl != blank
        assign rtl = true
    else
        assign rtl = false
    endif
-%}
{% render 'product-block-style-3' %}
<style>
  {% if settings.type_font_1 == 'shopify_fonts' and settings.type_font_2 == 'shopify_fonts' %}
      {% assign base_font = settings.type_font_1_font | font_modify: 'weight', '400' %}
      {% assign base_font_medium = settings.type_font_1_font | font_modify: 'weight', '500' %}
      {% assign base_font_sem_bold = settings.type_font_1_font | font_modify: 'weight', '600' %}
      {% assign base_font_bold = settings.type_font_1_font | font_modify: 'weight', 'bold' %}

      {{ base_font | font_face | replace: '@font-face {', '@font-face { font-display: swap;' }}
      {{ base_font_medium | font_face | replace: '@font-face {', '@font-face { font-display: swap;' }}
      {{ base_font_sem_bold | font_face | replace: '@font-face {', '@font-face { font-display: swap;' }}
      {{ base_font_bold | font_face | replace: '@font-face {', '@font-face { font-display: swap;' }}

      {% assign fonts_name = base_font.family | append: ',' | append: base_font.fallback_families %}

      {% if settings.type_font_2_font.family != settings.type_font_1_font.family %}
          {% assign header_font = settings.type_font_2_font | font_modify: 'weight', '400' %}
          {% assign header_font_bold = settings.type_font_2_font | font_modify: 'weight', 'bold' %}
          {% assign header_font_sem_bold = settings.type_font_2_font | font_modify: 'weight', '600' %}
          {% assign header_font_medium = settings.type_font_2_font | font_modify: 'weight', '500' %}
          
          {{ header_font | font_face | replace: '@font-face {', '@font-face { font-display: swap;' }}
          {{ header_font_medium | font_face | replace: '@font-face {', '@font-face { font-display: swap;' }}
          {{ header_font_sem_bold | font_face | replace: '@font-face {', '@font-face { font-display: swap;' }}
          {{ header_font_bold | font_face | replace: '@font-face {', '@font-face { font-display: swap;' }}

                {% assign fonts_name_2 = header_font.family | append: ',' | append: header_font.fallback_families %}
      {% else %}
                {% assign fonts_name_2 = base_font.family | append: ',' | append: base_font.fallback_families %}
      {% endif %}
  {% elsif settings.type_font_1 != 'shopify_fonts' and settings.type_font_2 != 'shopify_fonts' %}
        {%- assign font_1 = settings.type_font_1_name -%}
        {%- capture font_name_1 -%}
            {{font_1 | strip | replace: ' ', '+'}}:300,300i,400,400i,500,500i,600,600i,700,700i,800,800i
        {%- endcapture -%}
        @import url('https://fonts.googleapis.com/css?family={{font_name_1}}&display=swap');
              {% assign fonts_name = font_1 %}

        {% if settings.type_font_1_name != settings.type_font_2_name %}
            {%- assign font_2 = settings.type_font_2_name -%}
            {%- capture fonts_name_2 -%}
                {{font_2 | strip | replace: ' ', '+'}}:300,300i,400,400i,500,500i,600,600i,700,700i,800,800i
            {%- endcapture -%}
            @import url('https://fonts.googleapis.com/css?family={{fonts_name_2}}&display=swap');
                  {% assign fonts_name_2 = font_2 %}
        {% else %}
                  {% assign fonts_name_2 = font_1 %}
        {% endif %}
  {% elsif settings.type_font_1 == 'shopify_fonts' %}
        {% assign base_font = settings.type_font_1_font | font_modify: 'weight', '400' %}
        {% assign base_font_medium = settings.type_font_1_font | font_modify: 'weight', '500' %}
        {% assign base_font_sem_bold = settings.type_font_1_font | font_modify: 'weight', '600' %}
        {% assign base_font_bold = settings.type_font_1_font | font_modify: 'weight', 'bold' %}

        {{ base_font | font_face | replace: '@font-face {', '@font-face { font-display: swap;' }}
        {{ base_font_medium | font_face | replace: '@font-face {', '@font-face { font-display: swap;' }}
        {{ base_font_sem_bold | font_face | replace: '@font-face {', '@font-face { font-display: swap;' }}
        {{ base_font_bold | font_face | replace: '@font-face {', '@font-face { font-display: swap;' }}

        {% assign fonts_name = base_font.family | append: ',' | append: base_font.fallback_families %}
      
        {% if base_font.family != settings.type_font_2_name %}
            {%- assign font_2 = settings.type_font_2_name -%}
            {%- capture fonts_name_2 -%}
                {{font_2 | strip | replace: ' ', '+'}}:300,300i,400,400i,500,500i,600,600i,700,700i,800,800i
            {%- endcapture -%}
            @import url('https://fonts.googleapis.com/css?family={{fonts_name_2}}&display=swap');
                    {% assign fonts_name_2 = font_2 %}
        {% else %}
                  {% assign fonts_name_2 = base_font.family | append: ',' | append: base_font.fallback_families %}
        {% endif %}
  {% else %}
        {% assign header_font = settings.type_font_2_font | font_modify: 'weight', '400' %}
        {% assign header_font_bold = settings.type_font_2_font | font_modify: 'weight', 'bold' %}
        {% assign header_font_sem_bold = settings.type_font_2_font | font_modify: 'weight', '600' %}
        {% assign header_font_medium = settings.type_font_2_font | font_modify: 'weight', '500' %}

        {% if header_font.family != settings.type_font_1_name %}
            {%- assign font_1 = settings.type_font_1_name -%}
            {%- capture font_name_1 -%}
                {{font_1 | strip | replace: ' ', '+'}}:300,300i,400,400i,500,500i,600,600i,700,700i,800,800i
            {%- endcapture -%}
            @import url('https://fonts.googleapis.com/css?family={{font_name_1}}&display=swap');
                  {% assign fonts_name = font_1 %}
        {% else %}
                  {% assign fonts_name = header_font.family | append: ',' | append: header_font.fallback_families %}
        {% endif %}

        {{ header_font | font_face | replace: '@font-face {', '@font-face { font-display: swap;' }}
        {{ header_font_medium | font_face | replace: '@font-face {', '@font-face { font-display: swap;' }}
        {{ header_font_sem_bold | font_face | replace: '@font-face {', '@font-face { font-display: swap;' }}
        {{ header_font_bold | font_face | replace: '@font-face {', '@font-face { font-display: swap;' }}
             
        {% assign fonts_name_2 = header_font.family | append: ',' | append: header_font.fallback_families %}
  {% endif %}
</style>
<div class="trending-product product-block-3 trending-block-{{section.id}} product-layout-{{ section.settings.product_block_layout }}">
  <div class="container">
    <div class="trending-product-inner">
      <div class="row {% if section.blocks.size > 0 %}row-grid-margin{% endif %} product-banner-{%- if section.settings.image_right != blank -%}right{% endif %}">
        {% if section.blocks.size > 0 %}
          <div class="col-xl-{{ section.settings.image_grid }} col-lg-{{ section.settings.image_grid_tablate }} col-md-{{ section.settings.image_grid_mobile }} product-banner-outer col-grid-padding col-order">
            {% for block in section.blocks %}
              <div class="product-banner wow {{ block.settings.animations_type_image_card }} delay-{{ block.settings.animation_delay_image_card | replace: ".", "_" }}">
                <style>
                  trending-product.product-block-3.trending-block-{{section.id}} .product-banner.section-desc.delay-{{ block.settings.animation_delay_image_card | replace: ".", "_" }} {
                      animation-delay: {{ block.settings.animation_delay_image_card }}
                  }
                </style>
                <div class="product-banner-inner">
                  <div class="product-banner-img {% if block.settings.image_behavior != 'none' %} animate--{{ block.settings.image_behavior }} animate-hidden{% endif %}">
                    {% if block.settings.image != blank %}
                      {%- assign preview_img = block.settings.image -%}
                      <div class="animate-ambient-image">
                        <img {% render 'lazyload',preview_img: preview_img %} />
                      </div>
                      {% if settings.enable_lazyload %}<span class="data-lazy-loading"></span>{% endif %}
                    {% else %}
                      <div class="not_img" style="--height_not_img: 524px">365 x 524</div>
                    {% endif %}
                  </div>
                  <div class="product-banner-content">
                    {%- if block.settings.sub_title != blank -%}
                      <h4 class="banner-subtitle">{{ block.settings.sub_title }}</h4>
                    {%- endif -%}
                    {%- if block.settings.title_text != blank -%}
                      <h3 class="banner-title">{{ block.settings.title_text }}</h3>
                    {%- endif -%}
                    {%- if block.settings.button_text != blank -%}
                      {% if block.settings.button_style == '1'%}
                        <div class="image-banner-button button-style-{{ block.settings.button_style }}">
                          <a {% if block.settings.button_link == blank %} role="link" aria-disabled="true"{% else %} href="{{ block.settings.button_link }}"{% endif %} class="button">
                            {{ block.settings.button_text }}
                          </a>
                        </div>
                      {% else block.settings.button_style == '2' %}
                        <div class="image-banner-button button-style-{{ block.settings.button_style }}">
                          <a {% if block.settings.button_link == blank %} role="link" aria-disabled="true"{% else %} href="{{ block.settings.button_link }}"{% endif %} class="button">
                            {{ block.settings.button_text }} {% render 'arrow-right-2' %}
                          </a>
                        </div>
                      {% else block.settings.button_style == '3' %}
                        <div class="image-banner-button button-style-{{ block.settings.button_style }}">
                          <a {% if block.settings.button_link == blank %} role="link" aria-disabled="true"{% else %} href="{{ block.settings.button_link }}"{% endif %} class="button">
                            {{ block.settings.button_text }} {% render 'arrow-right-2' %}
                          </a>
                        </div>
                      {% endif %}
                    {%- endif -%}
                  </div>
                </div>
              </div>
              <style>
                :root {
                    {% if block.settings.title_font_family == 'font_1' %}
                        --banner-title-font-family: {{fonts_name}};
                    {% else %}
                        --banner-title-font-family: {{fonts_name_2}};
                    {% endif %}
                }
                .trending-product.trending-block-{{section.id}} .product-banner {
                      position: relative;
                }
                .trending-product.trending-block-{{section.id}} .product-banner-img {
                    border-radius: {{ block.settings.border_radius | append: 'px' }};
                    overflow: hidden;
                }
                .trending-product.trending-block-{{section.id}} .product-banner-img .animate-ambient-image {
                  height: 100%;
                }
                {% if block.settings.image_behavior == "zoom-in" %}
                  .trending-product.trending-block-{{section.id}} .product-banner-img .animate-ambient-image {
                        height: 100%;
                  }
                {% endif %}
                .product-block-3.trending-block-{{section.id}} .product-banner-content .banner-title {
                    {% if block.settings.font_size_title == 0 %}
                      font-size: var(--font-h3-size);
                    {% else %}
                      font-size: {{ block.settings.font_size_title | append: 'px' }};
                    {% endif %}
                    {% if block.settings.title_line_height == 0 %}
                      line-height: 1.2;
                    {% else %}
                      line-height: {{ block.settings.title_line_height | append: 'px' }};
                    {% endif %}
                    font-family: var(--banner-title-font-family);
                    font-weight: {{ block.settings.title_font_weight }};
                    text-transform: {{ block.settings.title_text_transform }};
                    {% if block.settings.title_color != blank %}
                      color: {{ block.settings.title_color }};
                    {% else %}
                      color: var(--heading-text-color);
                    {% endif %}
                    max-width: {{ block.settings.title_max_width | append: 'px' }};
                    padding-bottom: {{ block.settings.title__space_top | append: 'px' }};
                    {% if block.settings.title_letter_spacing == 0 %}
                      letter-spacing: var(--heading-letter-spacing);
                    {% else %}
                      letter-spacing: {{ block.settings.title_letter_spacing | append: 'px' }};
                    {% endif %}
                }
                .product-block-3.trending-block-{{section.id}} .product-banner-content .banner-subtitle {
                    font-size: {{ block.settings.font_size_sub_title | append: 'px' }};
                    line-height: calc({{ block.settings.font_size_sub_title | append: 'px' }} + 4px);
                    font-family: var(--sub-title-font-family);
                    font-weight: {{ block.settings.sub_title_font_weight }};
                    {% if block.settings.sub_title_color != blank %}
                      color: {{ block.settings.sub_title_color }};
                    {% else %}
                      color: var(--heading-text-color);
                    {% endif %}
                    padding-bottom: {{ block.settings.sub_title__space_top | append: 'px' }};
                    letter-spacing: {{ block.settings.font_size_sub_letter_spacing | append: 'px' }};
                    text-transform: {{ block.settings.sub_title_text_transform }};
                }
                .trending-product.trending-block-{{section.id}} .product-banner-content .button-style-3 .button {
                    padding: 0;
                    border-radius: 0;
                }
                .trending-product.trending-block-{{section.id}} .product-banner-content .button {
                    {% if block.settings.font_size_button == 0 %}
                      font-size: var(--font-button-size);
                    {% else %}
                      font-size: {{ block.settings.font_size_button | append: 'px' }};
                    {% endif %}
                    {% if block.settings.button_color != blank %}
                      color: {{ block.settings.button_color }};
                    {% else %}
                      color: var(--button-color);
                    {% endif %}
                    border: {{ block.settings.button_border | append: 'px' }} solid;
                    {% if block.settings.button_bg_color != blank %}
                      background-color: {{ block.settings.button_bg_color }};
                    {% else %}
                      background-color: var(--button-bg-color);
                    {% endif %}
                    {% if block.settings.button_border_color != blank %}
                      border-color: {{ block.settings.button_border_color }};
                    {% else %}
                      border-color: var(--button-border-color);
                    {% endif %}
                    {% if block.settings.button_letter_spacing == 0 %}
                       letter-spacing: var(--button-letter-spacing);
                    {% else %}
                      letter-spacing: {{ block.settings.button_letter_spacing }};
                    {% endif %}
                    {% if block.settings.product_button_top_bottom_padding == 0 %}
                      padding-top: var(--button-top-bottom-padding);
                      padding-bottom: var(--button-top-bottom-padding);
                    {% else %}
                      padding-top: {{ block.settings.product_button_top_bottom_padding | append: 'px' }};
                      padding-bottom: {{ block.settings.product_button_top_bottom_padding | append: 'px' }};
                    {% endif %}
                    {% if block.settings.product_button_left_right_padding == 0 %}
                      padding-left: var(--button-left-right-padding);
                      padding-right: var(--button-left-right-padding);
                    {% else %}
                      padding-left: {{ block.settings.product_button_left_right_padding | append: 'px'  }};
                      padding-right: {{ block.settings.product_button_left_right_padding | append: 'px'  }};
                    {% endif %}
                    text-transform: {{ block.settings.product_text_transform }};
                }
                .trending-product.trending-block-{{section.id}} .product-banner-content .button svg {
                    width: {{ block.settings.font_size_button | append: 'px' }};
                    transition: all 400ms ease;
                    {% if block.settings.button_color != blank %}
                      fill: {{ block.settings.button_color }};
                    {% else %}
                      fill: var(--button-color);
                    {% endif %}
                }
                
                {% if settings.button_hover_style == '2' %}
                  .trending-product.trending-block-{{section.id}} .product-banner-content .button {
                    position: relative;
                    overflow: hidden;
                    z-index: 0;
                  }
                  .trending-product.trending-block-{{section.id}} .product-banner-content .button:before {
                    content: "";
                    position: absolute;
                    top: 0;
                    right: 0;
                    bottom: 0;
                    left: 0;
                    transform: unset;
                    display: unset;
                    border: unset;
                    border-radius: unset;
                    width: unset;
                    height: unset;
                    animation: unset;
                    opacity: unset;
                    visibility: unset;
                    {% if block.settings.button_bghover_color != blank %}
                      background: {{ block.settings.button_bghover_color }};
                    {% else %}
                      background: var(--button-bg-hover-color);
                    {% endif %}
                    right: 100%;
                    z-index: -1;
                    transition: all 400ms ease;
                  }
                  .trending-product.trending-block-{{section.id}} .product-banner-content .button:hover {
                    {% if block.settings.button_hover_color != blank %}
                       color: {{ block.settings.button_hover_color }};
                    {% else %}
                      color: var(--button-hover-color);
                    {% endif %}
                    {% if block.settings.button_borderhover_color != blank %}
                      border-color: {{ block.settings.button_borderhover_color }};
                    {% else %}
                      border-color: var(--button-border-hover-color);
                    {% endif %}
                  }
                  .trending-product.trending-block-{{section.id}} .product-banner-content .button:hover svg {
                    {% if block.settings.button_hover_color != blank %}
                         fill: {{ block.settings.button_hover_color }};
                      {% else %}
                        fill: var(--button-hover-color);
                      {% endif %}
                  }
                  .trending-product.trending-block-{{section.id}} .product-banner-content .button:hover:before {
                    right: 0%;
                  }
                {% elsif settings.button_hover_style == '3' %}
                  .trending-product.trending-block-{{section.id}} .product-banner-content .button {
                    position: relative;
                    overflow: hidden;
                    z-index: 0;
                  }
                  .trending-product.trending-block-{{section.id}} .product-banner-content .button:before {
                    content: "";
                    position: absolute;
                    top: 0;
                    left: -70%;
                    display: unset;
                    border: unset;
                    border-radius: unset;
                    animation: unset;
                    opacity: unset;
                    visibility: unset;
                    {% if block.settings.button_bghover_color != blank %}
                      background: {{ block.settings.button_bghover_color }};
                    {% else %}
                      background: var(--button-bg-hover-color);
                    {% endif %}
                    z-index: -1;
                    transition: all 400ms ease;
                    clip-path: polygon(25px 0%, 100% 0%, 100% 100%, 0% 100%);
                    width: 70%;
                    height: 100%;
                    transform: rotateY(180deg);
                  }
