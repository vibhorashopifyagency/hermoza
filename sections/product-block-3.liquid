{%- if layout == 'slider' -%}
  <script>
    $(document).ready(function(){
      $(".trending-block-{{section.id}} .product-prev-icon").appendTo(".trending-block-{{section.id}} .products-carousel .slick-arrow.slick-prev");
      $(".trending-block-{{section.id}} .product-next-icon").appendTo(".trending-block-{{section.id}} .products-carousel .slick-arrow.slick-next");
    });
    if ($(".trending-block-{{section.id}} .products-carousel").length > 0) {
        $(".trending-block-{{section.id}} .products-carousel").slick({
          slidesToShow: {{ column }},
          slidesToScroll: {{ column }},
          speed: 400,
          dots: {{ dots }},
          arrows: {{ arrows }},
          rtl: {{ rtl }},
          margin: 30,
          autoplay: {{ autoplay }},
          autoplaySpeed: {{ slideshow_speed }},
          responsive: [
            {
              breakpoint: 1200,
              settings: {
                dots: {{dots_mb}},
                arrows: {{ arrows_mb }},
                slidesToScroll: {{ product_table }},
                slidesToShow: {{ product_table }}
              }
            },
            {
              breakpoint: 768,
              settings: {
                dots: {{dots_mb}},
                arrows: {{ arrows_mb }},
                slidesToScroll: {{ product_mobile }},
                slidesToShow: {{ product_mobile }}
              }
            },
          ]
        });
    }
  </script>
{%- endif -%}

