        <div class="{% if section.blocks.size > 0 %} col-xl-{{ section.settings.image__grid }} col-lg-{{ section.settings.image__grid_tablate }} col-md-{{ section.settings.image__grid_mobile }} product-banner-gird col-grid-padding{% else %}col-xl-12 col-lg-12 col-md-12{% endif %} col-order-1">
