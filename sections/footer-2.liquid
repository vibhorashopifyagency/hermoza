{% render 'footer-style-2' %}

<footer class="footer">
  {%- if section.blocks.size > 0 -%}
    <div class="footer-section">
      <div class="container">
        <div class="footer-row">
          {% for block in section.blocks %}
            <div class="footer-block__item footer-block__column footer-block__{{ block.type }} footer-block-id-{{block.id}} order-tb-{{ block.settings.order_tb }} order-mb-{{ block.settings.order_tb }}">
              <div class="footer-block-inner">
                {%- if block.settings.heading != blank -%}
                    <h2 class="footer-block__heading text-{{ block.settings.heading_position }}" data-toggle-column-footer>
                        {{- block.settings.heading | escape -}}
                    </h2>
                {%- endif -%}
                {%- if block.type == 'text' -%}
                  <div class="footer-block__details">
                    {%- if block.settings.logo != blank -%}
                      <div class="footer-image_logo text-{{ block.settings.log_position }} text-mb-{{ block.settings.log_position_mb }}">
                          <img data-srcset="{{ block.settings.logo | img_url: '200x' }}" class="lazyload" alt="{{ block.settings.logo.alt | default: shop.name | escape }}" />
                      </div>
                    {%- endif -%}
                    {%- if block.settings.text != blank -%}
                      <div class="footer-desc-box text-{{ block.settings.description_position }}">
                          {{ block.settings.text }}
                      </div>
                    {%- endif -%}
                    {%- if block.settings.show_social == true -%}
                      <div class="footer-social-media text-{{ block.settings.social_position }}">
                          {% render 'scoder-social-media-icons' %}
                      </div>
                    {%- endif -%}
                    {%- if block.settings.show_payment == true -%}
                      <div class="footer__payment payment-align-{{ section.settings.payment_icon_position }}">
                        <ul class="list list-payment clearfix" role="list">
                            {%- if section.settings.payment_type == 'default' -%}
                                {% comment %}
                                {% assign enabled_payment_types = 'visa,master,american_express,paypal' | remove: ' ' | split: ',' %}
                                {% endcomment %}
                                {%- for type in shop.enabled_payment_types -%}
                                    <li class="list-payment__item">
                                        {{ type | payment_type_svg_tag: class: 'icon icon--full-color' }}
                                    </li>
                                {%- endfor -%}
                            {%- else -%}
                                {% assign payment_string = 'visa,master,maestro,american_express,apple_pay,bitcoin,dankort,diners_club,discover,paypal,dogecoin,dwolla,forbrugsforeningen,jcb,litecoin,afterpay,google_pay,ideal,klarna,klarna-pay-later,shopify_pay,sofort' %}
                                {% assign methods = payment_string | split: ',' %}
                                {%- for method in methods -%}
                                    {%- assign show_current_method = method | prepend: 'show_payment_' -%}
                                    {% if section.settings[show_current_method] %}
                                        <li class="list-payment__item {{method}}">
                                            {{ method | payment_type_svg_tag: class: 'icon icon--full-color' }}
                                        </li>
                                        {% endif %}
                                {%- endfor -%}
                            {%- endif -%}
                        </ul>
                      </div>
                    {%- endif -%}
                  </div>
                {%- elsif block.type == 'link_list' -%}
                  {%- if block.settings.menu != blank -%}
                      <div class="footer-block__list">
                          <ul class="list-unstyled">
                              {%- for link in block.settings.menu.links -%}
                                  <li>
                                      <a {% if link.url == blank %} role="link" aria-disabled="true"{% else %} href="{{ link.url }}"{% endif %} class="link link-underline list-menu__item--link">
                                          <span class="text">{{ link.title }}</span>
                                      </a>
                                  </li>
                              {%- endfor -%}
                          </ul>
                      </div>
                  {%- endif -%}
              {%- elsif block.type == 'newsletter' -%}
                   <div class="footer-block__newsletter footer-block__list">
                      {%- liquid
                          assign check_text = block.settings.check_text
                          assign button_text = block.settings.button_text
                          assign button_width = block.settings.button_width | append: 'px'
                          assign input_text = block.settings.input_text
                      -%}

                      <style>
                        .footer-block__newsletter .field__input {
                            color: {{ block.settings.input_color }};
                            background: {{ block.settings.input_color_bg }};
                            border-color: {{ block.settings.input_color_border }};
                            margin-bottom: 15px;
                            font-size: {{ block.settings.button_font_size | append: 'px' }};
                        }
                        {%- if block.settings.card_bottom_border != blank -%}
                          .footer-block__newsletter .field__input {
                            border-top: unset;
                            border-left: unset;
                            border-right: unset;
                            border-width: {{ block.settings.card_border_bottom_width | append: 'px' }};
                            padding-left: 0;
                            padding-right: 0;
                          }
                        {%- endif -%}
                        .footer-block__newsletter .button {
                            color: {{ block.settings.button_color }};
                            background: {{ block.settings.button_color_bg }};
                            border-color: {{ block.settings.button_color_border }};
                        }
                        .footer-block__newsletter .button:hover {
                            color: {{ block.settings.button_color_hover }};
                            background: {{ block.settings.button_color_bg_hover }};
                            border-color: {{ block.settings.button_color_border_hover }};
                        }
                        .footer-block__newsletter .newsletter-form__field-wrapper {
                            position: relative;
                        }
                        .footer-block__newsletter .button.icon-btn {
                            padding: 0;
                            border-radius: unset;
                            position: absolute;
                            bottom: 25px;
                            right: 0;
                        }
                        .footer-block__newsletter .button.icon-btn svg {
                            margin: 0;
                            width: {{ block.settings.button_font_size | append: 'px' }};
                            transition: all 400ms ease;
                        }
                        .footer-block__newsletter .button.icon-btn svg {
                            stroke: {{ block.settings.button_color }};
                        }
                        .footer-block__newsletter .button.icon-btn:hover svg {
                            stroke: {{ block.settings.button_color_hover }};
                        }
                        body.layout_rtl .footer-block__newsletter .button.icon-btn svg {
                            transform: unset;
                        }
                        body.layout_rtl .footer-block__newsletter .button.icon-btn {
                            left:0;
                            right: unset;
                        }

                        @media (max-width: 1024px) {
                          .footer-block__newsletter .field__input {
                              font-size: {{ block.settings.button_font_size_tb | append: 'px' }};
                          }
                        }
                        @media (max-width: 767px) {
                          .footer-block__newsletter .field__input {
                              font-size: {{ block.settings.button_font_size_mb | append: 'px' }};
                          }
                        }
                      </style>
                      {%- if block.settings.text != blank -%}
                        <div class="footer-desc-box text-{{ block.settings.description_position }}">
                            {{ block.settings.text }}
                        </div>
                      {%- endif -%}
                      <{% if check_text %}form-has-check{% else %}div{% endif %} class="footer-block__newsletter-item footer-block__newsletter-form">
                          {%- form 'customer', id: 'ContactFooter', class: 'footer__newsletter newsletter-form' -%}
                              <input type="hidden" name="contact[tags]" value="newsletter">
                              <div class="newsletter-form__field-wrapper">
                                  <div class="field">
                                      <label class="field__label hiddenLabels" for="NewsletterForm--{{ section.id }}">
                                          {{ 'newsletter.label' | t }}
                                      </label>
                                      <input
                                          id="NewsletterForm--{{ section.id }}"
                                          type="email"
                                          name="contact[email]"
                                          class="field__input form-input-placeholder"
                                          value="{{ form.email }}"
                                          aria-required="true"
                                          autocorrect="off"
                                          autocapitalize="off"
                                          autocomplete="email"
                                          {% if form.errors %}
                                              autofocus
                                              aria-invalid="true"
                                              aria-describedby="ContactFooter-error"
                                          {% elsif form.posted_successfully? %}
                                              aria-describedby="ContactFooter-success"
                                          {% endif %}
                                          placeholder="{{ input_text }}"
                                      >
                                  </div>
                                  {% if section.settings.button_style == '1'%}
                                    <button type="submit" {% if check_text != blank %}disabled{% endif %} class="button newsletter-form__button" name="commit" id="Subscribe--{{ section.id }}" aria-label="{{ 'newsletter.button_label' | t }}" style="--button-width: {{ button_width }}">
                                        {{ button_text }}
                                    </button>
                                  {% else %}
                                    <button type="submit" {% if check_text != blank %}disabled{% endif %} class="button icon-btn newsletter-form__button" name="commit" id="Subscribe--{{ section.id }}" aria-label="{{ 'newsletter.button_label' | t }}" style="--button-width: {{ button_width }}">
                                        {% render 'icon-mail-2' %}
                                    </button>
                                  {% endif %}
                              </div>
                              {%- if check_text != blank -%}
                              <div class="form--check">
                                  <input type="checkbox" id="check_newsletter">
                                  <label for="check_newsletter">
                                      {{ block.settings.check_text }}
                                  </label>
                              </div>
                              {%- endif -%}
                              {%- if form.errors -%}
                                  <div class="newsletter-form__message newsletter-form__message--error form__message" id="ContactFooter-error">
                                      {% render 'icon-error' %}{{ form.errors.translated_fields['email'] | capitalize }} {{ form.errors.messages['email'] }}
                                  </div>
                              {%- endif -%}
                              {%- if form.posted_successfully? -%}
                              <script>
                                  $(document).ready(function() {
                                      var newsletterSuccessPopup = (function() {
                                          var popup = $('.newsletter-success-modal');
                                          var close = popup.find('.close');
                                          var success = popup.find('.success');
                                          var error = popup.find('.error');
                                          var overlay = $('.background-overlay');
                                          popup.show();
                                          overlay.show();
                                          error.hide();
                                          success.show();
                                          $.cookie('emailSubcribeModal', 'closed', {
                                              expires: 1,
                                              path: '/'
                                          });
                                          close.off('click').on('click', function() {
                                              popup.fadeOut();
                                              overlay.hide();
                                          });
                                          overlay.off('click').on('click', function() {
                                              popup.fadeOut();
                                              overlay.hide();
                                          });
                                      })();
                                      var newURL = location.href.split("?")[0];
                                      window.history.pushState('object', document.title, newURL);
                                  })
                              </script>
                              {% else %}
                                  <script>
                                      $(document).ready(function() {
                                          if (window.location.href.indexOf('newsletter&form_type=customer') > -1) {
                                              var newsletterSuccessPopup = (function() {
                                                  var popup = $('.newsletter-success-modal');
                                                  var close = popup.find('.close');
                                                  var success = popup.find('.success');
                                                  var error = popup.find('.error');
                                                  var overlay = $('.background-overlay');
                                                  popup.show();
                                                  overlay.show();
                                                  error.show();
                                                  success.hide();
                                                  $.cookie('emailSubcribeModal', 'closed', {
                                                      expires: 1,
                                                      path: '/'
                                                  });
                                                  close.off('click').on('click', function() {
                                                      popup.fadeOut();
                                                      overlay.hide();
                                                  });
                                                  overlay.off('click').on('click', function() {
                                                      popup.fadeOut();
                                                      overlay.hide();
                                                  });
                                              })();
                                              var newURL = location.href.split("?")[0];
                                              window.history.pushState('object', document.title, newURL);
                                          }
                                      })
                                  </script>
                              {%- endif -%}
                          {%- endform -%}
                      </{% if check_text %}form-has-check{% else %}div{% endif %}>
                      {%- if check_text != blank -%}
                      <script>
                          class NewsletterForm extends HTMLElement {
                              constructor() {
                                  super();
                                  this.querySelector('.form--check label').addEventListener('click', this.onClickChecked.bind(this))
                              }
                              onClickChecked(e) {
                                  (e.target.closest('.form--check').querySelector('input').checked) ?
                                  this.querySelector('.newsletter-form__button').setAttribute('disabled', true) :
                                  this.querySelector('.newsletter-form__button').removeAttribute('disabled');
                              }
                          }
                          customElements.define('form-has-check', NewsletterForm);
                      </script>
                      {%- endif -%}
                   </div>
              {%- endif -%}
              </div>
            </div>
            <style>
              .footer-block-id-{{block.id}} .footer-image_logo {
                  padding-bottom: {{ block.settings.logo__space_bottom | append: 'px' }};
              }
              .footer-block-id-{{block.id}} .footer-desc-box {
                  padding-bottom: {{ block.settings.description__space_bottom | append: 'px' }};
              }
              .footer-block-id-{{block.id}} .footer-social-media ul {
                  display: flex;
                  flex-wrap: wrap;
              }
              .footer-block-id-{{block.id}} .footer-social-media.text-center ul {
                  justify-content: center;
              }
              .footer-block-id-{{block.id}} .footer-social-media.text-right ul {
                  justify-content: flex-end;
              }
              .footer-block-id-{{block.id}} .footer-social-media ul li {
                  padding: 0px {{ block.settings.social_icon_margin_right | append: 'px' }};
              }
              body.layout_rtl .footer-block-id-{{block.id}} .footer-social-media ul li:first-child {
                  padding-right: 0;
                  padding-left: {{ block.settings.social_icon_margin_right | append: 'px' }} !important;
              }
              body.layout_rtl .footer-block-id-{{block.id}} .footer-social-media ul li:last-child {
                  padding-left: 0;
                  padding-right: {{ block.settings.social_icon_margin_right | append: 'px' }} !important;
              }
              .footer-block-id-{{block.id}} .footer-social-media ul li a {
                  border: {{ block.settings.social_icon_border_width | append: 'px' }} solid {{ block.settings.social_border_color }};
                  border-radius: {{ block.settings.social_icon_border_radius | append: 'px' }};
                  display: block;
                  padding: {{ block.settings.social_icon_padding | append: 'px' }};
                  background-color: {{ block.settings.social_background_color }};
              }
              .footer-block-id-{{block.id}} .footer-social-media ul li a:hover {
                  border: {{ block.settings.social_icon_border_width | append: 'px' }} solid {{ block.settings.social_border_color_hover }};
                  background-color: {{ block.settings.social_background_color_hover }};
              }
               .footer-block-id-{{block.id}} .footer-social-media ul li a svg {
                  height: {{ block.settings.social_icon_width | append: 'px' }};
                  width: {{ block.settings.social_icon_width | append: 'px' }};
                  display: block;
              }
              .footer-block-id-{{block.id}} .footer-social-media ul li a svg path {
                  fill: {{ block.settings.social_color }};
                  transition: all 400ms ease;
              }
              .footer-block-id-{{block.id}} .footer-social-media ul li a:hover svg path {
                  fill: {{ block.settings.social_color_hover }};
              }
              .footer-block__item.footer-block-id-{{block.id}} {
                  width: {{ block.settings.custom_width }};
                  padding: 0px 15px;
                  padding-top: {{ block.settings.mg_top_desktop | append: 'px' }};
                  padding-bottom: {{ block.settings.mg_bottom_desktop | append: 'px' }};
                  {%- if block.settings.card_border != blank -%}
                    border-right: {{ block.settings.card_border_width | append: 'px' }} solid {{ block.settings.card_border_color }};
                  {% endif %}
              }
              .footer-block-id-{{block.id}} .footer-block-inner {
                  max-width: {{ block.settings.content_width | append: 'px' }};
                  margin: 0 auto;
              }
              body.layout_rtl .footer-block__item.footer-block-id-{{block.id}} {
                  {%- if block.settings.card_border != blank -%}
                    border-left: {{ block.settings.card_border_width | append: 'px' }} solid {{ block.settings.card_border_color }};
                    border-right: unset;
                  {% endif %}
              }
              .footer-section > .container,
              .footer-bottom > .container{
                  max-width: {{ section.settings.content_width_1 | append: 'px' }};
              }
  
              @media (max-width: 1024px) {
                .footer-block-id-{{block.id}} .footer-image_logo {
                    padding-bottom: {{ block.settings.logo__space_bottom_tb | append: 'px' }};
                }
                .footer-block-id-{{block.id}} .footer-desc-box {
                    padding-bottom: {{ block.settings.description__space_bottom_tb | append: 'px' }};
                }
                .footer-block__item.footer-block-id-{{block.id}} {
                    width: {{ block.settings.custom_width_table }};
                    padding-top: {{ block.settings.mg_top_tablet | append: 'px' }};
                    padding-bottom: {{ block.settings.mg_bottom_tablet | append: 'px' }};
                }
                .footer-block__item.footer-block-id-footer-2-0 {
                    border-right: 0;
                }
              }
  
              @media (max-width: 767px) {
                .footer-block-id-{{block.id}} .footer-image_logo {
                    padding-bottom: {{ block.settings.logo__space_bottom_mb | append: 'px' }};
                }
                .footer-block-id-{{block.id}} .footer-desc-box {
                    padding-bottom: {{ block.settings.description__space_bottom_mb | append: 'px' }};
                }
                .footer-block__item.footer-block-id-{{block.id}} {
                    width: {{ block.settings.custom_width_mobile }};
                    padding-top: {{ block.settings.mg_top_mobile | append: 'px' }};
                    padding-bottom: {{ block.settings.mg_bottom_mobile | append: 'px' }};
                }
              }
            </style>
          {% endfor %}
        </div>
      </div>
    </div>
  {%- endif -%}
<script>
   if($(window).width() < 767) {
       $(".footer-block__heading").click(function(){
        $(this).toggleClass("active").next().slideToggle();
        $(".footer-block__list").not($(this).next()).slideUp();
        $(this).parent().parent().siblings().find(".footer-block__heading").removeClass("active");
      });
   }
</script>
  <div class="footer-bottom">
    <div class="container">
      <div class="footer-bottom-inner">
        <div class="row align-item-center">
          {%- if section.settings.show_copyright == true -%}
            <div class="col-lg-{{ section.settings.content_grid }} col-md-{{ section.settings.content_grid_tablate }} col-{{ section.settings.content_grid_mobile }} copyright">
              {%- if section.settings.copyright_content != blank -%}
                <div class="footer__copyright text-{{ section.settings.position }}">
                    {{ section.settings.copyright_content }}
                </div>
              {%- endif -%}
            </div>
          {%- endif -%}
          {%- if section.settings.show_social == true -%}
            <div class="col-lg-{{ section.settings.content_grid }} col-md-{{ section.settings.content_grid_tablate }} col-{{ section.settings.content_grid_mobile }} payment">
              <div class="footer-social-media">
                  {% render 'scoder-social-media-icons' %}
              </div>
            </div>
          {%- endif -%}
          {%- if section.settings.show_payment == true -%}
            <div class="col-lg-{{ section.settings.content_grid_3 }} col-md-{{ section.settings.content_grid_tablate_3 }} col-{{ section.settings.content_grid_mobile_3 }} payment">
                <div class="footer__payment payment-align-{{ section.settings.payment_icon_position }}">
                  <ul class="list list-payment clearfix" role="list">
                      {%- if section.settings.payment_type == 'default' -%}
                          {% comment %}
                          {% assign enabled_payment_types = 'visa,master,american_express,paypal' | remove: ' ' | split: ',' %}
                          {% endcomment %}
                          {%- for type in shop.enabled_payment_types -%}
                              <li class="list-payment__item">
                                  {{ type | payment_type_svg_tag: class: 'icon icon--full-color' }}
                              </li>
                          {%- endfor -%}
                      {%- else -%}
                          {% assign payment_string = 'visa,master,maestro,american_express,apple_pay,bitcoin,dankort,diners_club,discover,paypal,dogecoin,dwolla,forbrugsforeningen,jcb,litecoin,afterpay,google_pay,ideal,klarna,klarna-pay-later,shopify_pay,sofort' %}
                          {% assign methods = payment_string | split: ',' %}
                          {%- for method in methods -%}
                              {%- assign show_current_method = method | prepend: 'show_payment_' -%}
                              {% if section.settings[show_current_method] %}
                                  <li class="list-payment__item {{method}}">
                                      {{ method | payment_type_svg_tag: class: 'icon icon--full-color' }}
                                  </li>
                                  {% endif %}
                          {%- endfor -%}
                      {%- endif -%}
                  </ul>
              </div>
            </div>
          {%- endif -%}
          {%- if section.settings.copyrightmenu != blank -%}
            <div class="col-lg-{{ section.settings.content_grid_2 }} col-md-{{ section.settings.content_grid_tablate_2 }} col-{{ section.settings.content_grid_mobile_2 }} copyright-menu">
              <div class="footer-copyright__list">
                  <ul class="list-unstyled">
                      {%- for link in section.settings.copyrightmenu.links -%}
                          <li>
                              <a {% if link.url == blank %} role="link" aria-disabled="true"{% else %} href="{{ link.url }}"{% endif %} class="link link-underline list-menu__item--link">
                                  <span class="text">{{ link.title }}</span>
                              </a>
                          </li>
                      {%- endfor -%}
                  </ul>
              </div>
            </div>
          {%- endif -%}
        </div>
      </div>
    </div>
  </div>
</footer>


{% schema %}
{
    "name": "t:sections.footer.name_2",
    "max_blocks": 6,
    "settings": [
      {
          "type": "header",
          "content": "t:sections.layout.content"
      },
      {
          "type" : "color",
          "id" : "background_color",
          "label" : "t:settings_schema.colors.settings.colors_name.background_color",
          "default" : "#222222"
      },
      {
          "type" : "color_background",
          "id" : "background_gradient",
          "label" : "t:settings_schema.colors.settings.colors_name.background_color_gradient"
      },
      {
          "type": "range",
          "id": "content_width_1",
          "label": "t:sections.image-banner.blocks.image.settings.content.label__34",
          "min": 0,
          "max": 1800,
          "step": 20,
          "unit": "t:sections.layout.mg_desktop.unit",
          "default": 1400
      },
      {
          "type" : "image_picker",
          "id" : "background_image",
          "label" : "t:settings_schema.colors.settings.colors_name.background_image"
      },
      {
          "id": "footer_border_top",
          "type": "checkbox",
          "label": "t:settings_schema.typography.settings.typography.border_enable_top",
          "default": false
      },
      {
          "id": "footer_border_top_color",
          "type": "color",
          "label": "t:settings_schema.typography.settings.typography.border_top_color",
          "default": "#808080"
      },
      {
            "type": "header",
            "content": "t:sections.layout.margin_top"
        },
        {
            "type": "range",
            "id": "mg_top_desktop",
            "label": "t:sections.layout.mg_desktop.label",
            "min": 0,
            "max": 100,
            "step": 1,
            "unit": "t:sections.layout.mg_desktop.unit",
            "default": 50
        },
        {
            "type": "range",
            "id": "mg_top_tablet",
            "label": "t:sections.layout.mg_tablet.label",
            "min": 0,
            "max": 100,
            "step": 1,
            "unit": "t:sections.layout.mg_desktop.unit",
            "default": 40
        },
        {
            "type": "range",
            "id": "mg_top_mobile",
            "label": "t:sections.layout.mg_mobile.label",
            "min": 0,
            "max": 100,
            "step": 1,
            "unit": "t:sections.layout.mg_desktop.unit",
            "default": 25
        },
        {
            "type": "header",
            "content": "t:sections.layout.margin_bottom"
        },
        {
            "type": "range",
            "id": "mg_bottom_desktop",
            "label": "t:sections.layout.mg_desktop.label",
            "min": 0,
            "max": 100,
            "step": 1,
            "unit": "t:sections.layout.mg_desktop.unit",
            "default": 50
        },
        {
            "type": "range",
            "id": "mg_bottom_tablet",
            "label": "t:sections.layout.mg_tablet.label",
            "min": 0,
            "max": 100,
            "step": 1,
            "unit": "t:sections.layout.mg_desktop.unit",
            "default": 40
        },
        {
            "type": "range",
            "id": "mg_bottom_mobile",
            "label": "t:sections.layout.mg_mobile.label",
            "min": 0,
