<div class="image-luxury-main luxury-block-{{section.id}}">
  <div class="{% if section.settings.container == 'fullwidth'%}container-full{% else %}container{% endif %}">
    <div class="row">
      <div class="luxury-left wow {{ section.settings.animations_type_image1 }}">
        <div class="luxury-img-box">
          {%- if section.settings.text_1 != blank -%}
            <div class="luxury-left-text">
              {{ section.settings.text_1 }}
            </div>
          {%- endif -%}
          {%- if section.settings.left_image != blank -%}
              {%- assign preview_img_1 = section.settings.left_image -%}
              <div class="image image-adapt {% if section.settings.image_behavior1 != 'none' %} animate--{{ section.settings.image_behavior1 }} animate-hidden{% endif %}">
                  <div class="animate-ambient-image">
                    <img {% render 'lazyload', preview_img: preview_img_1 %} loading="lazy" />
                  </div>
                  {% if settings.enable_lazyload %}<span class="data-lazy-loading"></span>{% endif %}
              </div>
          {%- else -%}
              <div class="not_img" style="--height_not_img: 484px">
                825 x 484px
              </div>  
          {%- endif -%}
        </div>
      </div>
      <div class="luxury-center wow {{ section.settings.animations_type_image2 }}">
        <div class="luxury-img-box">
          {%- if section.settings.center_image != blank -%}
              {%- assign preview_img_2 = section.settings.center_image -%}
              <div class="image image-adapt">
                <div class="{%- if section.settings.moveUpDown != blank -%} animate--moveUpDown{%- endif -%}">
                    <img {% render 'lazyload', preview_img: preview_img_2 %} loading="lazy" />
                </div>
                  {% if settings.enable_lazyload %}<span class="data-lazy-loading"></span>{% endif %}
              </div>
          {%- else -%}
              <div class="not_img" style="--height_not_img: 422px">
                248 x 422px
              </div>  
          {%- endif -%}
          <div class="luxury-center-text">
            {{ section.settings.text_2 }}
          </div>
        </div>
      </div>
      <div class="luxury-right wow {{ section.settings.animations_type_image3 }}">
        <div class="luxury-img-box">
          {%- if section.settings.right_image != blank -%}
              {%- assign preview_img_3 = section.settings.right_image -%}
              <div class="image image-adapt {% if section.settings.image_behavior2 != 'none' %} animate--{{ section.settings.image_behavior2 }} animate-hidden{% endif %}">
                  <div class="animate-ambient-image">
                    <img {% render 'lazyload', preview_img: preview_img_3 %} loading="lazy" />
                  </div>
                  {% if settings.enable_lazyload %}<span class="data-lazy-loading"></span>{% endif %}
              </div>
          {%- else -%}
              <div class="not_img" style="--height_not_img: 699px">
                580 x 699px
              </div>  
          {%- endif -%}
        </div>
      </div>
    </div>
  </div>
</div>

<style>
