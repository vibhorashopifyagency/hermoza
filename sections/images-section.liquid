        {%- capture font_name_1 -%}
            {{font_1 | strip | replace: ' ', '+'}}:300,300i,400,400i,500,500i,600,600i,700,700i,800,800i
        {%- endcapture -%}
        @import url('https://fonts.googleapis.com/css?family={{font_name_1}}&display=swap');
              {% assign fonts_name = font_1 %}
  
        {% if settings.type_font_1_name != settings.type_font_2_name %}
            {%- assign font_2 = settings.type_font_2_name -%}
            {%- capture fonts_name_2 -%}
                {{font_2 | strip | replace: ' ', '+'}}:300,300i,400,400i,500,500i,600,600i,700,700i,800,800i
            {%- endcapture -%}
            @import url('https://fonts.googleapis.com/css?family={{fonts_name_2}}&display=swap');
                  {% assign fonts_name_2 = font_2 %}
        {% else %}
                  {% assign fonts_name_2 = font_1 %}
        {% endif %}
  {% elsif settings.type_font_1 == 'shopify_fonts' %}
        {% assign base_font = settings.type_font_1_font | font_modify: 'weight', '400' %}
        {% assign base_font_medium = settings.type_font_1_font | font_modify: 'weight', '500' %}
        {% assign base_font_sem_bold = settings.type_font_1_font | font_modify: 'weight', '600' %}
        {% assign base_font_bold = settings.type_font_1_font | font_modify: 'weight', 'bold' %}
  
        {{ base_font | font_face | replace: '@font-face {', '@font-face { font-display: swap;' }}
        {{ base_font_medium | font_face | replace: '@font-face {', '@font-face { font-display: swap;' }}
        {{ base_font_sem_bold | font_face | replace: '@font-face {', '@font-face { font-display: swap;' }}
        {{ base_font_bold | font_face | replace: '@font-face {', '@font-face { font-display: swap;' }}
  
        {% assign fonts_name = base_font.family | append: ',' | append: base_font.fallback_families %}
      
        {% if base_font.family != settings.type_font_2_name %}
            {%- assign font_2 = settings.type_font_2_name -%}
            {%- capture fonts_name_2 -%}
                {{font_2 | strip | replace: ' ', '+'}}:300,300i,400,400i,500,500i,600,600i,700,700i,800,800i
            {%- endcapture -%}
            @import url('https://fonts.googleapis.com/css?family={{fonts_name_2}}&display=swap');
                    {% assign fonts_name_2 = font_2 %}
        {% else %}
                  {% assign fonts_name_2 = base_font.family | append: ',' | append: base_font.fallback_families %}
        {% endif %}
  {% else %}
        {% assign header_font = settings.type_font_2_font | font_modify: 'weight', '400' %}
        {% assign header_font_bold = settings.type_font_2_font | font_modify: 'weight', 'bold' %}
        {% assign header_font_sem_bold = settings.type_font_2_font | font_modify: 'weight', '600' %}
        {% assign header_font_medium = settings.type_font_2_font | font_modify: 'weight', '500' %}
  
        {% if header_font.family != settings.type_font_1_name %}
            {%- assign font_1 = settings.type_font_1_name -%}
            {%- capture font_name_1 -%}
                {{font_1 | strip | replace: ' ', '+'}}:300,300i,400,400i,500,500i,600,600i,700,700i,800,800i
            {%- endcapture -%}
            @import url('https://fonts.googleapis.com/css?family={{font_name_1}}&display=swap');
                  {% assign fonts_name = font_1 %}
        {% else %}
                  {% assign fonts_name = header_font.family | append: ',' | append: header_font.fallback_families %}
        {% endif %}
  
        {{ header_font | font_face | replace: '@font-face {', '@font-face { font-display: swap;' }}
        {{ header_font_medium | font_face | replace: '@font-face {', '@font-face { font-display: swap;' }}
        {{ header_font_sem_bold | font_face | replace: '@font-face {', '@font-face { font-display: swap;' }}
        {{ header_font_bold | font_face | replace: '@font-face {', '@font-face { font-display: swap;' }}
             
        {% assign fonts_name_2 = header_font.family | append: ',' | append: header_font.fallback_families %}
  {% endif %}

  :root {
      {% if section.settings.text_1_font == 'font_1' %}
          --text-1-font-family: {{fonts_name}};
      {% else %}
          --text-1-font-family: {{fonts_name_2}};
      {% endif %}

      {% if section.settings.text_2_font == 'font_1' %}
          --text-2-font-family: {{fonts_name}};
      {% else %}
          --text-2-font-family: {{fonts_name_2}};
      {% endif %}
      
  }
  
  .luxury-block-{{section.id}} {
    padding-top: {{ section.settings.mg_top_desktop | append: 'px' }};
    padding-bottom: {{ section.settings.mg_bottom_desktop | append: 'px' }};
    {% if section.settings.background_gradient != blank or section.settings.background_color != blank %}
      background: {% if section.settings.background_gradient != blank %}{{ section.settings.background_gradient }}{% else %}{{ section.settings.background_color }}{% endif %};
    {% endif %}
  }
  .image-luxury-main .row {
      margin-left: 0;
      margin-right: 0;
  }
  .luxury-left {
      padding-left: 0;
      width: 44%;
      animation-delay: {{ section.settings.animation_delay_image1 }}
  }
  .luxury-left-text {
      font-size: {{ section.settings.font_size_text_1 | append: 'px' }};
      line-height: {{ section.settings.text_1_line_height | append: 'px' }};
      {% if section.settings.text_1_color != blank %}
          color: {{ section.settings.text_1_color }};
      {% else %}
          color: var(--body-text-color);
      {% endif %}
      font-weight: {{ section.settings.text_1_font_weight }};
      text-transform: {{ section.settings.text_1_transform }};
      letter-spacing: {{ section.settings.text_1_spacing | append: 'px' }};
      padding-top: {{ section.settings.text_1_space | append: 'px' }};
      padding-bottom: {{ section.settings.text_1_space | append: 'px' }};
      font-family: var(--text-1-font-family);
      width: 100%;
  }
  .luxury-img-box img {
      display: block;
  }
  .luxury-right {
      padding-right: 0;
      width: 30%;
      animation-delay: {{ section.settings.animation_delay_image3 }}
  }
  .luxury-center {
      width: 26%;
      padding-top: 100px;
      padding-bottom: 100px;
      display: flex;
      animation-delay: {{ section.settings.animation_delay_image2 }}
  }
  .luxury-img-box {
      position: relative;
  }
  .luxury-right .luxury-img-box {
      height: 100%;
      padding-bottom: 100px;
  }
  .luxury-right .image {
      height: 100%;
  }
  .luxury-right .image img {
      height: 100%;
      object-fit: cover;
  }
    .luxury-center-text {
      font-size: {{ section.settings.font_size_text_2 | append: 'px' }};
      line-height: {{ section.settings.text_2_line_height | append: 'px' }};
      {% if section.settings.text_2_color != blank %}
          color: {{ section.settings.text_2_color }};
      {% else %}
          color: var(--heading-text-color);
      {% endif %}
      font-weight: {{ section.settings.text_2_font_weight }};
      text-transform: {{ section.settings.text_2_transform }};
      letter-spacing: {{ section.settings.text_2_spacing | append: 'px' }};
      font-family: var(--text-2-font-family);
      padding-top: 100px;
      width: 100%;
      text-align: center;
  }
  .luxury-center .image {
      display: flex;
      justify-content: center;
      width: 100%;
  }
  .luxury-center .luxury-img-box {
      width: 100%;
      display: flex;
      flex-wrap: wrap;
      align-self: flex-end;
  }
  .luxury-left .luxury-img-box {
      display: flex;
      flex-wrap: wrap;
      height: 100%;
  }
  .luxury-left .luxury-img-box .image {
      align-self: flex-end;
      width: 100%;
  }

  @media (max-width: 1199px) {
    .luxury-block-{{section.id}} {
      padding-top: {{ section.settings.mg_top_tablet | append: 'px' }};
      padding-bottom: {{ section.settings.mg_bottom_tablet | append: 'px' }};
    }
  }
  @media (max-width: 1024px) {
    .luxury-left-text {
        font-size: {{ section.settings.font_size_text_1_tb | append: 'px' }};
        line-height: {{ section.settings.text_1_line_height_tb | append: 'px' }};
        padding-top: {{ section.settings.text_1_space_tb | append: 'px' }};
        padding-bottom: 30px;
    }
    .luxury-center-text {
        font-size: {{ section.settings.font_size_text_2_tb | append: 'px' }};
        line-height: {{ section.settings.text_2_line_height_tb | append: 'px' }};
        padding-top: 80px;
    }
    .luxury-left {
        padding-left: 15px;
        width: 100%;
        padding-bottom: 30px;
    }
    .luxury-left .luxury-img-box img {
        width: 100%;
    }
    .luxury-center {
      width: 50%;
      padding-top: 80px;
      padding-bottom: 80px;
    }
    .luxury-right {
        padding-right: 15px;
        width: 50%;
    }
    .luxury-right .luxury-img-box {
        padding-bottom: 80px;
    }
    .luxury-center .image img {
        max-height: 250px;
    }
  }
  @media (max-width: 767px) {
    .luxury-block-{{section.id}} {
      padding-top: {{ section.settings.mg_top_mobile | append: 'px' }};
      padding-bottom: {{ section.settings.mg_bottom_mobile | append: 'px' }};
    }
    .luxury-left-text {
        font-size: {{ section.settings.font_size_text_1_mb | append: 'px' }};
        line-height: {{ section.settings.text_1_line_height_mb | append: 'px' }};
        padding-top: {{ section.settings.text_1_space_mb | append: 'px' }};
        padding-bottom: {{ section.settings.text_1_space_mb | append: 'px' }};
        padding-left: 0 !important;
    }
    .luxury-center-text {
        font-size: {{ section.settings.font_size_text_2_mb | append: 'px' }};
        line-height: {{ section.settings.text_2_line_height_mb | append: 'px' }};
        padding-top: 30px;
    }
    .luxury-right .luxury-img-box {
        padding-bottom: 50px;
    }
    .luxury-center {
        padding-top: 50px;
        padding-bottom: 50px;
    }
  }
</style>

{% if settings.enable_layout_rtl != blank %}
  <script>
    function container_spacer(){
      var win = $(window).width();
      var con = $('main .container').width();
      var total = (win - con) / 2;
  
      $('.luxury-left-text').css('padding-right', total);
    }
    $(document).ready(function() {
        container_spacer();
  
        $(window).resize(function(){
            container_spacer();
        });
    });
  </script>
{% else %}
  <script>
    function container_spacer(){
      var win = $(window).width();
      var con = $('main .container').width();
      var total = (win - con) / 2;
  
      $('.luxury-left-text').css('padding-left', total);
    }
    $(document).ready(function() {
        container_spacer();
  
        $(window).resize(function(){
            container_spacer();
        });
    });
  </script>
{% endif %}

