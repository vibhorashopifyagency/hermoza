    <div class="customer-review-next-icon">{% render 'arrow-right' %}</div>
  </div>
{% else %}
  <div class="slick-button-arrow">
    <div class="customer-review-prev-icon">{% render 'arrow-left-2' %}</div>
    <div class="customer-review-next-icon">{% render 'arrow-right-2' %}</div>
  </div>
