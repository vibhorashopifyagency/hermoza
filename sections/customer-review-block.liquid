<script>
  $(document).ready(function(){
    $(".customer-review-prev-icon").appendTo(".customer-review-block .slick-arrow.slick-prev");
    $(".customer-review-next-icon").appendTo(".customer-review-block .slick-arrow.slick-next");
  });
  if ($(".customer-review-slide").length > 0) {
      $(".customer-review-slide").slick({
        slidesToShow: {{ slider_show }},
        slidesToScroll: 1,
        speed: 400,
        dots: {{ slider_dots }},
        arrows: {{ slider_arrows }},
        rtl: {{ rtl }},
        autoplay: {{ autoplay }},
        autoplaySpeed: {{ slide_speed }},
        responsive: [
          {
            breakpoint: 1200,
            settings: {
              dots: {{ slider_dots_mb }},
              arrows: {{ slider_arrows_mb }},
              slidesToShow: {{ slider_show_tb }}
            }
          },
          {
            breakpoint: 768,
            settings: {
              slidesToShow: {{ slider_show_mb }},
              dots: {{ slider_dots_mb }},
              arrows: {{ slider_arrows_mb }},
            }
          },
        ]
      });
  }
</script>

