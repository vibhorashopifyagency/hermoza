{%- liquid
  assign container = section.settings.container
  assign background_color = section.settings.background_color
  assign background_gradient = section.settings.background_gradient
-%}

<style>
  {% if settings.type_font_1 == 'shopify_fonts' and settings.type_font_2 == 'shopify_fonts' %}
      {% assign base_font = settings.type_font_1_font | font_modify: 'weight', '400' %}
      {% assign base_font_medium = settings.type_font_1_font | font_modify: 'weight', '500' %}
      {% assign base_font_sem_bold = settings.type_font_1_font | font_modify: 'weight', '600' %}
      {% assign base_font_bold = settings.type_font_1_font | font_modify: 'weight', 'bold' %}

      {{ base_font | font_face | replace: '@font-face {', '@font-face { font-display: swap;' }}
      {{ base_font_medium | font_face | replace: '@font-face {', '@font-face { font-display: swap;' }}
      {{ base_font_sem_bold | font_face | replace: '@font-face {', '@font-face { font-display: swap;' }}
      {{ base_font_bold | font_face | replace: '@font-face {', '@font-face { font-display: swap;' }}

      {% assign fonts_name = base_font.family | append: ',' | append: base_font.fallback_families %}

      {% if settings.type_font_2_font.family != settings.type_font_1_font.family %}
          {% assign header_font = settings.type_font_2_font | font_modify: 'weight', '400' %}
          {% assign header_font_bold = settings.type_font_2_font | font_modify: 'weight', 'bold' %}
          {% assign header_font_sem_bold = settings.type_font_2_font | font_modify: 'weight', '600' %}
          {% assign header_font_medium = settings.type_font_2_font | font_modify: 'weight', '500' %}
          
          {{ header_font | font_face | replace: '@font-face {', '@font-face { font-display: swap;' }}
          {{ header_font_medium | font_face | replace: '@font-face {', '@font-face { font-display: swap;' }}
          {{ header_font_sem_bold | font_face | replace: '@font-face {', '@font-face { font-display: swap;' }}
          {{ header_font_bold | font_face | replace: '@font-face {', '@font-face { font-display: swap;' }}

                {% assign fonts_name_2 = header_font.family | append: ',' | append: header_font.fallback_families %}
      {% else %}
                {% assign fonts_name_2 = base_font.family | append: ',' | append: base_font.fallback_families %}
      {% endif %}
  {% elsif settings.type_font_1 != 'shopify_fonts' and settings.type_font_2 != 'shopify_fonts' %}
        {%- assign font_1 = settings.type_font_1_name -%}
        {%- capture font_name_1 -%}
            {{font_1 | strip | replace: ' ', '+'}}:300,300i,400,400i,500,500i,600,600i,700,700i,800,800i
        {%- endcapture -%}
        @import url('https://fonts.googleapis.com/css?family={{font_name_1}}&display=swap');
              {% assign fonts_name = font_1 %}

        {% if settings.type_font_1_name != settings.type_font_2_name %}
            {%- assign font_2 = settings.type_font_2_name -%}
            {%- capture fonts_name_2 -%}
                {{font_2 | strip | replace: ' ', '+'}}:300,300i,400,400i,500,500i,600,600i,700,700i,800,800i
            {%- endcapture -%}
            @import url('https://fonts.googleapis.com/css?family={{fonts_name_2}}&display=swap');
                  {% assign fonts_name_2 = font_2 %}
        {% else %}
                  {% assign fonts_name_2 = font_1 %}
        {% endif %}
  {% elsif settings.type_font_1 == 'shopify_fonts' %}
        {% assign base_font = settings.type_font_1_font | font_modify: 'weight', '400' %}
        {% assign base_font_medium = settings.type_font_1_font | font_modify: 'weight', '500' %}
        {% assign base_font_sem_bold = settings.type_font_1_font | font_modify: 'weight', '600' %}
        {% assign base_font_bold = settings.type_font_1_font | font_modify: 'weight', 'bold' %}

        {{ base_font | font_face | replace: '@font-face {', '@font-face { font-display: swap;' }}
        {{ base_font_medium | font_face | replace: '@font-face {', '@font-face { font-display: swap;' }}
        {{ base_font_sem_bold | font_face | replace: '@font-face {', '@font-face { font-display: swap;' }}
        {{ base_font_bold | font_face | replace: '@font-face {', '@font-face { font-display: swap;' }}

        {% assign fonts_name = base_font.family | append: ',' | append: base_font.fallback_families %}
      
        {% if base_font.family != settings.type_font_2_name %}
            {%- assign font_2 = settings.type_font_2_name -%}
            {%- capture fonts_name_2 -%}
                {{font_2 | strip | replace: ' ', '+'}}:300,300i,400,400i,500,500i,600,600i,700,700i,800,800i
            {%- endcapture -%}
            @import url('https://fonts.googleapis.com/css?family={{fonts_name_2}}&display=swap');
                    {% assign fonts_name_2 = font_2 %}
        {% else %}
                  {% assign fonts_name_2 = base_font.family | append: ',' | append: base_font.fallback_families %}
        {% endif %}
  {% else %}
        {% assign header_font = settings.type_font_2_font | font_modify: 'weight', '400' %}
        {% assign header_font_bold = settings.type_font_2_font | font_modify: 'weight', 'bold' %}
        {% assign header_font_sem_bold = settings.type_font_2_font | font_modify: 'weight', '600' %}
        {% assign header_font_medium = settings.type_font_2_font | font_modify: 'weight', '500' %}

        {% if header_font.family != settings.type_font_1_name %}
            {%- assign font_1 = settings.type_font_1_name -%}
            {%- capture font_name_1 -%}
                {{font_1 | strip | replace: ' ', '+'}}:300,300i,400,400i,500,500i,600,600i,700,700i,800,800i
            {%- endcapture -%}
            @import url('https://fonts.googleapis.com/css?family={{font_name_1}}&display=swap');
                  {% assign fonts_name = font_1 %}
        {% else %}
                  {% assign fonts_name = header_font.family | append: ',' | append: header_font.fallback_families %}
        {% endif %}

        {{ header_font | font_face | replace: '@font-face {', '@font-face { font-display: swap;' }}
        {{ header_font_medium | font_face | replace: '@font-face {', '@font-face { font-display: swap;' }}
        {{ header_font_sem_bold | font_face | replace: '@font-face {', '@font-face { font-display: swap;' }}
        {{ header_font_bold | font_face | replace: '@font-face {', '@font-face { font-display: swap;' }}
             
        {% assign fonts_name_2 = header_font.family | append: ',' | append: header_font.fallback_families %}
  {% endif %}
  
  .modern-grid-block-{{section.id}} {
      background: {% if background_gradient != blank %}{{ background_gradient }}{% else %}{{ background_color }}{% endif %};
      padding-top: {{ section.settings.mg_top_desktop | append: 'px' }};
      padding-bottom: {{ section.settings.mg_bottom_desktop | append: 'px' }};
      position: relative;
  }
  .modern-grid-block-{{section.id}} .row.align-iteam-center {
      align-items: center;
  }
  .modern-grid-block-{{section.id}} .row.align-iteam-bottom {
      align-items: flex-end;
  }
  .modern-grid-block-{{section.id}} .section-header .title {
      text-align: {{ section.settings.section_title_align }};
      padding-bottom: {{ section.settings.mg_bottom_title | append: 'px' }};
      animation-delay: {{ section.settings.animation_delay_title }};
      font-weight: {{ section.settings.block_title_font_weight }};
      font-size: {{ section.settings.title_font_size | append: 'px' }};
  }
  .modern-grid-block-{{section.id}} .section-header .description-text {
      text-align: {{ section.settings.section_title_align }};
      padding-bottom: {{ section.settings.desc_space_bottom | append: 'px' }};
      animation-delay: {{ section.settings.animation_delay_desc }};
      font-weight: {{ section.settings.description__font__weight }};
      font-size: {{ section.settings.description__font__size | append: 'px' }};
      color: {{ section.settings.description__color }};
      max-width: {{ section.settings.content__width | append: 'px' }};
  }
  .modern-grid-block-{{section.id}} .section-header .description-text.text-center {
    margin-left: auto;
    margin-right: auto;
  }
  .modern-grid-block-{{section.id}} .headding-btn {
    text-align: {{ section.settings.section_title_align }};
    margin-bottom: {{ section.settings.button_space_bottom | append: 'px' }};
  }
  .modern-grid-block-{{section.id}} .headding-btn .button {
    color: {{ section.settings.button_color }};
    background-color: {{ section.settings.button_bg_color }};
    border: {{ section.settings.button_border | append: 'px' }} solid {{ section.settings.button_border_color }};
    letter-spacing: {{ section.settings.button_letter_spacing | append: 'px' }};
    padding:{{ section.settings.btn_padding_top_bottom | append: 'px' }} {{ section.settings.btn_padding_left_right | append: 'px' }};
    font-size:{{ section.settings.font_size_button | append: 'px' }};
    border-bottom: {{ section.settings.button_border_bottom | append: 'px' }} solid {{ section.settings.button_border_color_bottom }};
  }
  .modern-grid-block-{{section.id}} .headding-btn .button svg {
    fill: {{ section.settings.button_color }};
    transition: all 400ms ease;
  }
  .modern-grid-block-{{section.id}} .headding-btn .button:hover svg {
    fill: {{ section.settings.button_hover_color }};
  }
  .modern-grid-block-{{section.id}} .headding-btn .button:hover {
    color: {{ section.settings.button_hover_color }};
    background-color: {{ section.settings.button_bghover_color }};
    border: {{ section.settings.button_border | append: 'px' }} solid {{ section.settings.button_borderhover_color }};
    border-bottom: {{ section.settings.button_border_bottom | append: 'px' }} solid {{ section.settings.button_borderhover_color_bottom }};
  }
  .modern-grid-block-{{section.id}} .vector_1 {
      position: absolute;
      {% if section.settings.vector_top != blank %}
        top: {{ section.settings.vector_top }};
      {% endif %}
      {% if section.settings.vector_bottom != blank %}
        bottom: {{ section.settings.vector_bottom }};
      {% endif %}
      {% if section.settings.vector_left != blank %}
        left: {{ section.settings.vector_left }};
      {% endif %}
      {% if section.settings.vector_right != blank %}
        right: {{ section.settings.vector_right }};
      {% endif %}
  }
  .modern-grid-block-{{section.id}} .vector_2 {
      position: absolute;
      {% if section.settings.vector_top_2 != blank %}
        top: {{ section.settings.vector_top_2 }};
      {% endif %}
      {% if section.settings.vector_bottom_2 != blank %}
        bottom: {{ section.settings.vector_bottom_2 }};
      {% endif %}
      {% if section.settings.vector_left_2 != blank %}
        left: {{ section.settings.vector_left_2 }};
      {% endif %}
      {% if section.settings.vector_right_2 != blank %}
        right: {{ section.settings.vector_right_2 }};
      {% endif %}
  }

  @media (max-width: 1199px) {
    .modern-grid-block-{{section.id}} {
      padding-top: {{ section.settings.mg_top_tablet | append: 'px' }};
      padding-bottom: {{ section.settings.mg_bottom_tablet | append: 'px' }};
    }  
  }

  @media (max-width: 1024px) {
    .modern-grid-block-{{section.id}} .section-header .title {
        padding-bottom: {{ section.settings.mg_bottom_title_tb | append: 'px' }};
        font-size:{{ section.settings.title_font_size_tb | append: 'px' }};
    } 
    .modern-grid-block-{{section.id}} .section-header .description-text {
        padding-bottom: {{ section.settings.desc_space_bottom_tb | append: 'px' }};
        font-size: {{ section.settings.description__font__tb | append: 'px' }};
    }
    .modern-grid-block-{{section.id}} .headding-btn {
      margin-bottom: {{ section.settings.button_space_bottom_tb | append: 'px' }};
    }
    .modern-grid-block-{{section.id}} .vector_1 {
        {% if section.settings.vector_tablet_top != blank %}
          top: {{ section.settings.vector_tablet_top }};
        {% endif %}
        {% if section.settings.vector_tablet_bottom != blank %}
          bottom: {{ section.settings.vector_tablet_bottom }};
        {% endif %}
        {% if section.settings.vector_tablet_left != blank %}
          left: {{ section.settings.vector_tablet_left }};
        {% endif %}
        {% if section.settings.vector_tablet_right != blank %}
          right: {{ section.settings.vector_tablet_right }};
        {% endif %}
    }
    .modern-grid-block-{{section.id}} .vector_2 {
        {% if section.settings.vector_tablet_top_2 != blank %}
          top: {{ section.settings.vector_tablet_top_2 }};
        {% endif %}
        {% if section.settings.vector_tablet_bottom_2 != blank %}
          bottom: {{ section.settings.vector_tablet_bottom_2 }};
        {% endif %}
        {% if section.settings.vector_tablet_left_2 != blank %}
          left: {{ section.settings.vector_tablet_left_2 }};
        {% endif %}
        {% if section.settings.vector_tablet_right_2 != blank %}
          right: {{ section.settings.vector_tablet_right_2 }};
        {% endif %}
    }
  }
  
  @media (max-width: 767px) {
    .modern-grid-block-{{section.id}} {
      padding-top: {{ section.settings.mg_top_mobile | append: 'px' }};
      padding-bottom: {{ section.settings.mg_bottom_mobile | append: 'px' }};
    }
    .modern-grid-block-{{section.id}} .section-header .title {
        text-align: {{ section.settings.section_title_align_mobile }};
        padding-bottom: {{ section.settings.mg_bottom_title_mb | append: 'px' }};
        font-size:{{ section.settings.title_font_size_mb | append: 'px' }};
    }
    .modern-grid-block-{{section.id}} .section-header .description-text {
        padding-bottom: {{ section.settings.desc_space_bottom_mb | append: 'px' }};
        font-size: {{ section.settings.description__font__mb | append: 'px' }};
    }
    .modern-grid-block-{{section.id}} .headding-btn {
      margin-bottom: {{ section.settings.button_space_bottom_mb | append: 'px' }};
    }
    .modern-grid-block-{{section.id}} .vector_1 {
        {% if section.settings.vector_mobile_top != blank %}
          top: {{ section.settings.vector_mobile_top }};
        {% endif %}
        {% if section.settings.vector_mobile_bottom != blank %}
          bottom: {{ section.settings.vector_mobile_bottom }};
        {% endif %}
        {% if section.settings.vector_mobile_left != blank %}
          left: {{ section.settings.vector_mobile_left }};
        {% endif %}
        {% if section.settings.vector_mobile_right != blank %}
          right: {{ section.settings.vector_mobile_right }};
        {% endif %}
    }
    .modern-grid-block-{{section.id}} .vector_2 {
        {% if section.settings.vector_mobile_top_2 != blank %}
          top: {{ section.settings.vector_mobile_top_2 }};
        {% endif %}
        {% if section.settings.vector_mobile_bottom_2 != blank %}
          bottom: {{ section.settings.vector_mobile_bottom_2 }};
        {% endif %}
        {% if section.settings.vector_mobile_left_2 != blank %}
          left: {{ section.settings.vector_mobile_left_2 }};
        {% endif %}
        {% if section.settings.vector_mobile_right_2 != blank %}
          right: {{ section.settings.vector_mobile_right_2 }};
        {% endif %}
    }
  }
</style>

<div class="modern-grid modern-grid-block-{{section.id}}">
  {%- if section.settings.vector_image_1 != blank -%}
    <div class="vector_1 vector_1_{{section.id}}">
      {%- liquid
        assign vector_img_1 = section.settings.vector_image_1
      -%}
      <div class="vector_img_1-an {% if section.settings.vectore_1_animations_type != 'none'%}wow{% endif %} {{ section.settings.vectore_1_animations_type }} delay-{{ section.settings.vectore_1_animation_delay | replace: ".", "_" }}">
        <img {% render 'lazyload', preview_img: vector_img_1 %} loading="lazy"/>
        {% if settings.enable_lazyload %}<span class="data-lazy-loading"></span>{% endif %}
      </div>
      <style>
        .modern-grid-block-{{section.id}} .vector_img_1-an.delay-{{ section.settings.vectore_1_animation_delay | replace: ".", "_" }} {
            animation-delay: {{ section.settings.vectore_1_animation_delay }}
        }
      </style>
    </div>
  {%- endif -%}

  {%- if section.settings.vector_image_2 != blank -%}
    <div class="vector_2 vector_2_{{section.id}}">
      {%- liquid
        assign vector_img_2 = section.settings.vector_image_2
      -%}
      <div class="vector_img_2-an {% if section.settings.vectore_2_animations_type != 'none'%}wow{% endif %} {{ section.settings.vectore_2_animations_type }} delay-{{ section.settings.vectore_2_animation_delay | replace: ".", "_" }}">
        <img {% render 'lazyload', preview_img: vector_img_2 %} loading="lazy"/>
        {% if settings.enable_lazyload %}<span class="data-lazy-loading"></span>{% endif %}  
      </div>
      <style>
        .modern-grid-block-{{section.id}} .vector_img_2-an.delay-{{ section.settings.vectore_2_animation_delay | replace: ".", "_" }} {
            animation-delay: {{ section.settings.vectore_2_animation_delay }}
        }
      </style>
    </div>
  {%- endif -%}
    
  <div class="{% if container == 'fullwidth'%}container-full{% else %}container{% endif %}">
    {%- if section.settings.section_title != blank -%}
        <div class="section-header">
          <div class="row align-item-center">
            <div class="col-xl-12 col-lg-12 col-md-12">
              <div class="section-title text-{{ section.settings.block_title_align }} text-mb-{{ section.settings.block_title_align_mobile }}">
                {% if section.settings.animations_type_title != blank %}
                  <h3 class="title {% if section.settings.animations_type_title != 'none'%}wow{% endif %} {{ section.settings.animations_type_title }}">
                      <span class="text">{{ section.settings.section_title | escape }}</span>
                  </h3>
                {% endif %}
                {% if section.settings.description__text != blank %}
                  <div class="description-text text-{{ section.settings.section_title_align }} {% if section.settings.animations_type_desc != 'none'%}wow{% endif %} {{ section.settings.animations_type_desc }}"
                              style="animation-delay: {{ section.settings.animations_type_desc }};"
                    >
                    {{ section.settings.description__text }}
                  </div>
                {% endif %}
                {%- if section.settings.image_banner_button != blank -%}
                    {% if section.settings.button_style == '1'%}
                      <div class="headding-btn {% if section.settings.animations_type_button != 'none'%}wow{% endif %} {{ section.settings.animations_type_button }} delay-{{ section.settings.animation_delay_button | replace: ".", "_" }}">
                        <style>
                            .image-banner-block-{{section.id}}.image-banner .image-banner-button.delay-{{ section.settings.animation_delay_button | replace: ".", "_" }} {
                                animation-delay: {{ section.settings.animation_delay_button }}
                            }
                        </style>
                        <a {% if section.settings.image_banner_link == blank %} role="link" aria-disabled="true"{% else %} href="{{ section.settings.image_banner_link }}"{% endif %} class="button">
                          {{ section.settings.image_banner_button }}
                        </a>
                      </div>
                    {% else %}
                      <div class="headding-btn {% if section.settings.animations_type_button != 'none'%}wow{% endif %} {{ section.settings.animations_type_button }} delay-{{ section.settings.animation_delay_button | replace: ".", "_" }}">
                        <style>
                            .image-banner-block-{{section.id}}.image-banner .image-banner-button.delay-{{ section.settings.animation_delay_button | replace: ".", "_" }} {
                                animation-delay: {{ section.settings.animation_delay_button }}
                            }
                        </style>
                        <a {% if section.settings.image_banner_link == blank %} role="link" aria-disabled="true"{% else %} href="{{ section.settings.image_banner_link }}"{% endif %} class="button">
                          {{ section.settings.image_banner_button }} {% render 'arrow-right-2' %}
                        </a>
                      </div>
                    {% endif %}
                  {%- endif -%}
              </div>
            </div>
          </div>
        </div>
      {%- endif -%}
    <div class="row align-iteam-{{ section.settings.align }}">
      {%- for block in section.blocks -%}
        {%- case block.type -%}
          {%- when 'text' -%}
            <div class="modern-grid-content modern-grid-content-{{ block.id }} col-lg-{{ block.settings.content_grid }} col-md-{{ block.settings.content_grid_tablate }} col-{{ block.settings.content_grid_mobile }} {% if block.settings.animations_type_content_block != 'none'%}wow{% endif %} {{ block.settings.animations_type_content_block }} delay-{{ block.settings.animation_delay_content_block | replace: ".", "_" }}">
              <style>
                .modern-grid-content-{{ block.id }} {
                    padding-top: {{ block.settings.mg_top_desktop | append: 'px' }};
                    padding-bottom: {{ block.settings.mg_bottom_desktop | append: 'px' }};
                }
                .modern-grid-content-{{ block.id }}.delay-{{ block.settings.animation_delay_content_block | replace: ".", "_" }} {
                      animation-delay: {{ block.settings.animation_delay_content_block }}
                  }
                :root {
                  {% if block.settings.sub_heading_fontfamily == 'font_1' %}
                      --modern-sub-heading-font-family: {{fonts_name}};
                  {% else %}
                      --modern-sub-heading-font-family: {{fonts_name_2}};
                  {% endif %}

                  {% if block.settings.heading_fontfamily == 'font_1' %}
                      --modern-heading-font-family: {{fonts_name}};
                  {% else %}
                      --modern-heading-font-family: {{fonts_name_2}};
                  {% endif %}
                }
                .modern-grid-content-{{ block.id }} .modern-sub-heading{
                    font-size: {{ block.settings.sub_title_font_size | append: 'px' }}; 
                    padding-bottom: {{ block.settings.sub_title__space_bottom | append: 'px' }};
                    font-weight: {{ block.settings.sub_title_font_weight }};
                    color: {{ block.settings.sub_title_color }};
                    text-transform: {{ block.settings.sub_title_text_transform }};
                    font-family: var(--modern-sub-heading-font-family);
                    letter-spacing: {{ block.settings.sub_title_letter_spacing | append: 'px' }};
                }
                .modern-grid-content-{{ block.id }} .modern-content-heading{
                    font-size: {{ block.settings.title_font_size | append: 'px' }}; 
                    padding-bottom: {{ block.settings.title__space_bottom | append: 'px' }};
                    font-weight: {{ block.settings.title_font_weight }};
                    color: {{ block.settings.title_color }};
                    text-transform: {{ block.settings.title_text_transform }};
                    font-family: var(--modern-heading-font-family);
                    letter-spacing: {{ block.settings.title_letter_spacing | append: 'px' }};
                }
                .modern-grid-content-{{ block.id }} .modern-content-text {
                    font-size: {{ block.settings.desc_title | append: 'px' }};
                    max-width: {{ block.settings.content__width | append: 'px' }};
                    color: {{ block.settings.desc_color }};
                    font-weight: {{ block.settings.desc_font_weight }};
                    padding-bottom: {{ block.settings.description__space_bottom | append: 'px' }};
                }
                .modern-grid-content-{{ block.id }} .number-title {
                  display: flex;
                  flex-wrap: wrap;
                  align-items: center;
                }
                .modern-grid-content-{{ block.id }} .number-title .number-title-1 {
                    font-size: {{ block.settings.number_font_size | append: 'px' }};
                    color: {{ block.settings.number_color }};
                    font-weight: {{ block.settings.number_font_weight }};
                }
                .modern-grid-content-{{ block.id }} .number-title .number-text {
                    font-size: {{ block.settings.number_text_font_size | append: 'px' }};
                    color: {{ block.settings.number_text_color }};
                    font-weight: {{ block.settings.number_text_font_weight }};
                    padding-left: 15px;
                }
                
                @media (max-width: 1024px) {
                  .modern-grid-content-{{ block.id }} .modern-sub-heading{
                    font-size: {{ block.settings.sub_title_tb_font_size | append: 'px' }}; 
                    padding-bottom: {{ block.settings.sub_title__space_bottom_tb | append: 'px' }};
                  }
                  .modern-grid-content-{{ block.id }} .modern-content-heading{
                    font-size: {{ block.settings.title_tb_font_size | append: 'px' }}; 
                    padding-bottom: {{ block.settings.title__space_bottom_tb | append: 'px' }};
                  }
                  .modern-grid-content-{{ block.id }} .modern-content-text {
                      font-size: {{ block.settings.desc_tb | append: 'px' }};
                      padding-bottom: {{ block.settings.description__space_bottom_tb | append: 'px' }};
                  }
