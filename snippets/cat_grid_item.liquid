<div class="packery-item {{ listClass }} block-{{ block.id }} wow {{ block.settings.animations_type }} delay-{{ block.settings.animation_delay | replace: ".", "_" }}">
  <style>
    .packery-grid .packery-item.delay-{{ block.settings.animation_delay | replace: ".", "_" }} {
        animation-delay: {{ block.settings.animation_delay }}
    }
  </style>
  <div class="packery-item-inner">
    <div class="packery-img {% if block.settings.image_behavior != 'none' %} animate--{{ block.settings.image_behavior }} animate-hidden{% endif %}">
      {% if block.settings.image != blank %}
          {%- assign preview_img = block.settings.image -%}
          <a {% if block.settings.link == blank %} role="link" aria-disabled="true"{% else %} href="{{ block.settings.link }}"{% endif %} class="image-zoom animate-ambient-image">
              <img {% render 'lazyload',preview_img: preview_img %} />
              {% if settings.enable_lazyload %}<span class="data-lazy-loading"></span>{% endif %}
          </a>
      {% else %}
          <a {% if block.settings.link == blank %} role="link" aria-disabled="true"{% else %} href="{{ block.settings.link }}"{% endif %} class="image-zoom">
              <div class="not_img" style="--height_not_img: {{ imgHeight }}">{{ imgWidth }} x {{ imgHeight }}</div>
          </a>
      {% endif %}
    </div>
    <div class="packery-content align-{{ block.settings.align_horizontal }} justify-{{ block.settings.varticl_align }} {%- if block.settings.button_style == '3' -%}button-style-bottom{%- endif -%}">
      {%- if block.settings.title_text != blank -%}
        <h6 class="packery-title">{{ block.settings.title_text }}</h6>
      {%- endif -%}
      {%- if block.settings.button != blank -%}
          <a {% if block.settings.link == blank %} role="link" aria-disabled="true"{% else %} href="{{ block.settings.link }}"{% endif %} class="button packery-button button-style-{{ block.settings.button_style }}">
              {%- if block.settings.btn_icon != blank -%}
                  {{ block.settings.btn_icon }}
              {%- endif -%}
              <span>{{ block.settings.button }}</span>
          </a>
        {%- endif -%}
      </div>
  </div>
  <style>
   .button.packery-button.button-style-2{
        text-decoration: underline;
        background-color: unset;
        border: unset;
        text-transform: none;
        padding:0;
        {% if block.settings.button_color != blank %}
            color: {{ block.settings.button_color }};
        {% else %}
             color: var(--link-color);
        {% endif %}
    }
     .button.packery-button.button-style-2:hover{
       background-color: unset;  
       border:unset;
       {% if block.settings.button_hover_color != blank %}
            color: {{ block.settings.button_hover_color }};
       {% else %}
            color: var(--button-hover-color);
       {% endif %}
    }
    .block-{{ block.id }} .button.packery-button {
        {% if block.settings.button_color != blank %}
              color: {{ block.settings.button_color }};
        {% else %}
             color: var(--button-color);
        {% endif %}
        {% if block.settings.button_border_color != blank %}
            border: {{ section.settings.button_border | append: 'px' }} solid {{ block.settings.button_border_color }};
        {% else %}
             border: {{ section.settings.button_border | append: 'px' }} solid var(--button-border-color);
        {% endif %}
        {% if block.settings.button_bg_color != blank %}
             background: {{ block.settings.button_bg_color }};
        {% else %}
             background: var(--button-bg-color);
        {% endif %}
        white-space: nowrap;
    }
    
    {% if settings.button_hover_style == '2' %}
      .block-{{ block.id }} .button.packery-button {
        position: relative;
        overflow: hidden;
        z-index: 0;
      }
      .block-{{ block.id }} .button.packery-button:before {
        content: "";
        position: absolute;
        top: 0;
        right: 0;
        bottom: 0;
        left: 0;
        transform: unset;
        display: unset;
        border: unset;
        border-radius: unset;
        width: unset;
        height: unset;
        animation: unset;
        opacity: unset;
        visibility: unset;
        background: {{ block.settings.button_bghover_color }};
        right: 100%;
        z-index: -1;
        transition: all 400ms ease;
      }
      .block-{{ block.id }} .button.packery-button:hover {
        {% if block.settings.button_hover_color != blank %}
            color: {{ block.settings.button_hover_color }};
        {% else %}
             color: var(--button-hover-color);
        {% endif %}
        {% if block.settings.button_borderhover_color != blank %}
            border: {{ section.settings.button_border | append: 'px' }} solid {{ block.settings.button_borderhover_color }};
        {% else %}
            border: {{ section.settings.button_border | append: 'px' }} solid var(--button-border-hover-color);
        {% endif %}
        
      }
      .block-{{ block.id }} .button.packery-button:hover svg {
        {% if block.settings.button_hover_color != blank %}
             fill: {{ block.settings.button_hover_color }};
        {% else %}
             fill: var(--button-hover-color);
        {% endif %}
        
      }
      .block-{{ block.id }} .button.packery-button:hover:before {
        right: 0%;
      }
    {% elsif settings.button_hover_style == '3' %}
      .block-{{ block.id }} .button.packery-button {
        position: relative;
        overflow: hidden;
        z-index: 0;
      }
      .block-{{ block.id }} .button.packery-button:before {
        content: "";
        position: absolute;
        top: 0;
        left: -70%;
        display: unset;
        border: unset;
        border-radius: unset;
        animation: unset;
        opacity: unset;
        visibility: unset;
        background: {{ block.settings.button_bghover_color }};
        z-index: -1;
        transition: all 400ms ease;
        clip-path: polygon(25px 0%, 100% 0%, 100% 100%, 0% 100%);
        width: 70%;
        height: 100%;
        transform: rotateY(180deg);
      }
      .block-{{ block.id }} .button.packery-button:after {
        content: "";
        position: absolute;
        top: 0;
        right: -70%;
        transform: unset;
        display: unset;
        border: unset;
        border-radius: unset;
        animation: unset;
        opacity: unset;
        visibility: unset;
        background: {{ block.settings.button_bghover_color }};
        z-index: -1;
        transition: all 400ms ease;
        clip-path: polygon(25px 0%, 100% 0%, 100% 100%, 0% 100%);
        width: 70%;
        height: 100%;
      }
      .block-{{ block.id }} .button.packery-button:hover {
        {% if block.settings.button_hover_color != blank %}
            color: {{ block.settings.button_hover_color }};
        {% else %}
             color: var(--button-hover-color);
        {% endif %}
        {% if block.settings.button_borderhover_color != blank %}
            border: {{ section.settings.button_border | append: 'px' }} solid {{ block.settings.button_borderhover_color }};
        {% else %}
            border: {{ section.settings.button_border | append: 'px' }} solid var(--button-border-hover-color);
        {% endif %}
      }
      .block-{{ block.id }} .button.packery-button:hover svg {
        {% if block.settings.button_hover_color != blank %}
             fill: {{ block.settings.button_hover_color }};
        {% else %}
             fill: var(--button-hover-color);
        {% endif %}
      }
      .block-{{ block.id }} .button.packery-button:hover:before {
        left: calc(0% - 2px);
      }
      .block-{{ block.id }} .button.packery-button:hover:after {
        right: calc(0% - 2px);
      }
    {% elsif settings.button_hover_style == '4' %}
      .block-{{ block.id }} .button.packery-button {
        position: relative;
        overflow: hidden;
        z-index: 0;
      }
      .block-{{ block.id }} .button.packery-button:before {
        content: "";
        position: absolute;
        top: 0;
        left: -70%;
        display: unset;
        border: unset;
        border-radius: unset;
        animation: unset;
        opacity: unset;
        visibility: unset;
        background: {{ block.settings.button_bghover_color }};
        z-index: -1;
        transition: all 400ms ease;
        clip-path: polygon(25px 0%, 100% 0%, 100% 100%, 0% 100%);
        width: 70%;
        height: 100%;
        transform: rotateY(180deg);
      }
      .block-{{ block.id }} .button.packery-button:after {
        content: "";
        position: absolute;
        top: 0;
        right: -70%;
        transform: unset;
        display: unset;
        border: unset;
        border-radius: unset;
        animation: unset;
        opacity: unset;
        visibility: unset;
        background: {{ block.settings.button_bghover_color }};
        z-index: -1;
        transition: all 400ms ease;
        clip-path: polygon(0px 0%, 100% 0%, 100% 100%, 25px 100%);
        width: 70%;
        height: 100%;
      }
      .block-{{ block.id }} .button.packery-button:hover {
        {% if block.settings.button_hover_color != blank %}
            color: {{ block.settings.button_hover_color }};
        {% else %}
             color: var(--button-hover-color);
        {% endif %}
        {% if block.settings.button_borderhover_color != blank %}
            border: {{ section.settings.button_border | append: 'px' }} solid {{ block.settings.button_borderhover_color }};
        {% else %}
            border: {{ section.settings.button_border | append: 'px' }} solid var(--button-border-hover-color);
        {% endif %}
      }
      .block-{{ block.id }} .button.packery-button:hover svg {
        {% if block.settings.button_hover_color != blank %}
             fill: {{ block.settings.button_hover_color }};
        {% else %}
             fill: var(--button-hover-color);
        {% endif %}
      }
      .block-{{ block.id }} .button.packery-button:hover:before {
        left: calc(0% - 2px);
      }
      .block-{{ block.id }} .button.packery-button:hover:after {
        right: calc(0% - 2px);
      }
    {% elsif settings.button_hover_style == '5' %}
      .block-{{ block.id }} .button.packery-button {
        position: relative;
        overflow: hidden;
        z-index: 0;
      }
      .block-{{ block.id }} .button.packery-button:before {
        content: "";
        position: absolute;
        top: 50%;
        left: -30px;
        display: unset;
        border: unset;
        animation: unset;
        opacity: unset;
        visibility: unset;
        background: {{ block.settings.button_bghover_color }};
        z-index: -1;
        transition: all 400ms ease;
        width: 20px;
        height: 20px;
        transform: translateY(-50%);
        border-radius: 100%;
      }
      .block-{{ block.id }} .button.packery-button:after {
        content: "";
        position: absolute;
        top: 50%;
        right: -30px;
        display: unset;
        border: unset;
        animation: unset;
        opacity: unset;
        visibility: unset;
        background: {{ block.settings.button_bghover_color }};
        z-index: -1;
        transition: all 400ms ease;
        width: 20px;
        height: 20px;
        transform: translateY(-50%);
        border-radius: 100%;
      }
      .block-{{ block.id }} .button.packery-button:hover {
        {% if block.settings.button_hover_color != blank %}
            color: {{ block.settings.button_hover_color }};
        {% else %}
             color: var(--button-hover-color);
        {% endif %}
        {% if block.settings.button_borderhover_color != blank %}
            border: {{ section.settings.button_border | append: 'px' }} solid {{ block.settings.button_borderhover_color }};
        {% else %}
            border: {{ section.settings.button_border | append: 'px' }} solid var(--button-border-hover-color);
        {% endif %}
      }
      .block-{{ block.id }} .button.packery-button:hover svg {
         {% if block.settings.button_hover_color != blank %}
             fill: {{ block.settings.button_hover_color }};
        {% else %}
             fill: var(--button-hover-color);
        {% endif %}
      }
      .block-{{ block.id }} .button.packery-button:hover:before {
        -webkit-animation: criss-cross-left 0.8s both;
        animation: criss-cross-left 0.8s both;
        -webkit-animation-direction: alternate;
        animation-direction: alternate;
      }
      .block-{{ block.id }} .button.packery-button:hover:after {
        -webkit-animation: criss-cross-right 0.8s both;
        animation: criss-cross-right 0.8s both;
        -webkit-animation-direction: alternate;
        animation-direction: alternate;
      }
      @-webkit-keyframes criss-cross-left {
        0% {
          left: -30px;
        }
        50% {
          left: calc(50% - 10px);
          width: 20px;
          height: 20px;
        }
        100% {
          left: 40%;
          width: 375px;
          height: 375px;
        }
      }
      
      @keyframes criss-cross-left {
        0% {
          left: -30px;
        }
        50% {
          left: calc(50% - 10px);
          width: 20px;
          height: 20px;
        }
        100% {
          left: 40%;
          width: 375px;
          height: 375px;
        }
      }
      @-webkit-keyframes criss-cross-right {
        0% {
          right: -30px;
        }
        50% {
          right: calc(50% - 10px);
          width: 20px;
          height: 20px;
        }
        100% {
          right: calc(50% - 10px);
          width: 375px;
          height: 375px;
        }
      }
      @keyframes criss-cross-right {
        0% {
          right: -30px;
        }
        50% {
          right: calc(50% - 10px);
          width: 20px;
          height: 20px;
        }
        100% {
          right: calc(50% - 10px);
          width: 375px;
          height: 375px;
        }
      }
    {% else %}
      .block-{{ block.id }} .button.packery-button:hover {
        {% if block.settings.button_borderhover_color != blank %}
            border: {{ section.settings.button_border | append: 'px' }} solid {{ block.settings.button_borderhover_color }};
        {% else %}
            border: {{ section.settings.button_border | append: 'px' }} solid var(--button-border-hover-color);
        {% endif %}
        {% if block.settings.button_hover_color != blank %}
            color: {{ block.settings.button_hover_color }};
        {% else %}
            color: var(--button-hover-color);
        {% endif %}
        {% if block.settings.button_bghover_color != blank %}
            background: {{ block.settings.button_bghover_color }};
        {% else %}
            background: var(--button-bg-hover-color);
        {% endif %}
      }
    {% endif %}

    .block-{{ block.id }} .packery-title {
        font-size: {{ block.settings.title_font_size | append: 'px' }};
        line-height: {{ block.settings.heading_title_line_height | append: 'px' }};
        font-weight: {{ block.settings.title_font_weight }};
        letter-spacing: {{ block.settings.title_letter_spacing | append: 'px' }};
        text-transform: {{ block.settings.title_text_transform }};
        {% if block.settings.title_color != blank %}
            color: {{ block.settings.title_color }};
        {% else %}
            color: var(--heading-text-color);
        {% endif %}
        padding-bottom: {{ block.settings.title__space_top | append: 'px' }};
        font-family: var(--packery-title-font-type);
    }
    .block-{{ block.id }} .packery-content.align-top.justify-left {
        top: {{ block.settings.content_top_bottom_padding | append: 'px' }};
        left: {{ block.settings.content_left_right_padding | append: 'px' }};
        transform: unset;
        bottom: unset;
        max-width: calc(100% - {{ block.settings.content_left_right_padding | append: 'px' }}*2);
    }
    body.layout_rtl .block-{{ block.id }} .packery-content.align-top.justify-left {
      left: unset;
      right: {{ block.settings.content_left_right_padding | append: 'px' }};
    }
    .block-{{ block.id }} .packery-content.align-top.justify-right { 
        top: {{ block.settings.content_top_bottom_padding | append: 'px' }};
        right: {{ block.settings.content_left_right_padding | append: 'px' }};
        transform: unset;
        left: unset;
        bottom: unset;
        max-width: calc(100% - {{ block.settings.content_left_right_padding | append: 'px' }}*2);
    }
    body.layout_rtl .block-{{ block.id }} .packery-content.align-top.justify-right {
      right: unset;
      left: {{ block.settings.content_left_right_padding | append: 'px' }};
    }
    .block-{{ block.id }} .packery-content.align-bottom.justify-right {
        bottom: {{ block.settings.content_top_bottom_padding | append: 'px' }};
        right: {{ block.settings.content_left_right_padding | append: 'px' }};
        transform: unset;
        left: unset;
        max-width: calc(100% - {{ block.settings.content_left_right_padding | append: 'px' }}*2);
    }
    body.layout_rtl .block-{{ block.id }} .packery-content.align-bottom.justify-right {
      right: unset;
      left: {{ block.settings.content_left_right_padding | append: 'px' }};
    }
    .block-{{ block.id }} .packery-content.align-bottom.justify-left {
        bottom: {{ block.settings.content_top_bottom_padding | append: 'px' }};
        left: {{ block.settings.content_left_right_padding | append: 'px' }};
        transform: unset;
        max-width: calc(100% - {{ block.settings.content_left_right_padding | append: 'px' }}*2);
    }
    body.layout_rtl .block-{{ block.id }} .packery-content.align-bottom.justify-left {
      left: unset;
      right: {{ block.settings.content_left_right_padding | append: 'px' }};
    }
    .block-{{ block.id }} .packery-content.align-bottom.justify-center {
        bottom: {{ block.settings.content_top_bottom_padding | append: 'px' }};
        left: 50%;
        transform: translate(-50%);
        text-align: center;
        max-width: calc(100% - 30px);
    }
    .block-{{ block.id }} .packery-item-inner:hover .packery-content.align-bottom.justify-center {
        bottom: 50px;
    }
    .block-{{ block.id }} .packery-content.align-top.justify-center {
        top: {{ block.settings.content_top_bottom_padding | append: 'px' }};
        left: 50%;
        transform: translate(-50%);
        text-align: center;
        bottom: unset;
        max-width: calc(100% - 30px);
    }
    .block-{{ block.id }} .packery-content.align-center.justify-center {
        top: 50%;
        left: 50%;
        transform: translate(-50%, -50%);
        text-align: center;
        bottom: unset;
        max-width: calc(100% - 30px);
    }
    .block-{{ block.id }} .packery-content.align-center.justify-left {
        top: 50%;
        bottom: unset;
        left: {{ block.settings.content_left_right_padding | append: 'px' }};
        transform: translateY(-50%);
    }
    body.layout_rtl .block-{{ block.id }} .packery-content.align-center.justify-left {
      left: unset;
      right: {{ block.settings.content_left_right_padding | append: 'px' }};
    }
    .block-{{ block.id }} .packery-content.align-center.justify-right {
        top: 50%;
        right: {{ block.settings.content_left_right_padding | append: 'px' }};
        left: unset;
        bottom: unset;
        transform: translateY(-50%);
    }
    body.layout_rtl .block-{{ block.id }} .packery-content.align-center.justify-right {
      right: unset;
      left: {{ block.settings.content_left_right_padding | append: 'px' }};
    }
    .block-{{ block.id }} .button-style-bottom {
      bottom: -60px !important;
    }
    .block-{{ block.id }} .packery-item-inner:hover .button-style-bottom {
      bottom: {{ block.settings.content_top_bottom_padding | append: 'px' }} !important;
    }
    
    @media (max-width: 1024px) {
        .block-{{ block.id }} .packery-title {
            font-size: {{ block.settings.heading_title_tb | append: 'px' }};
            line-height: {{ block.settings.heading_title_line_height_tb | append: 'px' }};
            padding-bottom: {{ block.settings.title__space_top_tb | append: 'px' }};
        }
        .block-{{ block.id }} .packery-content.align-top.justify-left {
          top: 15px;
          left: 15px;
          max-width: calc(100% - 30px);
        }
        .block-{{ block.id }} .packery-content.align-top.justify-right { 
          top: 15px;
          right: 15px;
          max-width: calc(100% - 30px);
        }
        .block-{{ block.id }} .packery-content.align-bottom.justify-right {
            bottom: 15px;
            right: 15px;
            max-width: calc(100% - 30px);
        }
        .block-{{ block.id }} .packery-content.align-bottom.justify-left {
            bottom: 15px;
            left: 15px;
            max-width: calc(100% - 30px);
        }
    }

    @media (max-width: 767px) {
         .block-{{ block.id }} .packery-title {
            font-size: {{ block.settings.heading_title_mb | append: 'px' }};
            line-height: {{ block.settings.heading_title_line_height_mb | append: 'px' }};
            padding-bottom: {{ block.settings.title__space_top_mb | append: 'px' }};
        }
    }
    
    
            
  </style>
</div>